{
    "chapter": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "102|1|Praacuryara pratiyogitaya tomalokaka amanoyogi karai raakhiche."
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "102|2|Yetiyalaike tomaloke kabarata upanita nohoraa."
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "102|3|Ketiya'o nahaya, ati sonakale tomaloke janibalai paba."
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "102|4|Akau ketiya'o nahaya, ati sonakale tomaloke janibalai paba."
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "102|5|Ketiya'o nahaya, yadi tomaloke niscitabharae janilahemtena."
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "102|6|Niscaya tomaloke jahima (jahannama) dekhiba."
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "102|7|Akau, niscaya tomaloke iyaka patiyana yoraakai dekhiba."
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "102|8|Tara pichata se'idina arasya'i tomalokaka niyamata samparke prasna karaa haâ€™ba."
        }
    ]
}