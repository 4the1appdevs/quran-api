{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "107|1|Tumi taka dekhichane yiye hicapa pratidanaka asbikara karae"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "107|2|si se'ijane'i, yiye anathaka kathorabharae khedi pathiyaya"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "107|3|arau si dukhiyaka khana khuraabalai utsaha nidiye."
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "107|4|Eteke dhbansa se'ibidha chalata adayakaraisakalara"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "107|5|yisakale nijara chalatata amanoyogi"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "107|6|yisakale anaka dekhuraabalaihe karae"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "107|7|arau sarau-suraa grhasamagrai pradanata badha diye."
        }
    ]
}