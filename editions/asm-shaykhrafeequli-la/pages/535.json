{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "56|17|Sihamtara ase-pase ghuraiphuraiba cira kisorasakale"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "56|18|prabahita nijaraara suraapurna panapatra arau piyalabora lai"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "56|19|se'i suraa pana karaile te'omlokara mura nughuraaya arau te'omloka jnanaharaa'o naha’ba"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "56|20|arau (ghuraiphuraiba) te'omlokara pachandaniya phala-mula lai"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "56|21|arau te'omlokara rauci anuyayi caraa'ira mansa lai."
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "56|22|Arau te'omlokara babe thakiba mrganayani (danara cakubisista) hura"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "56|23|yeniba sihamta suraksita mukutahe"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "56|24|sihamte yi amala karaichila tara puraskarasbaraupa."
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "56|25|Tata sihamte kono alagatiyala katha arau kono papabakya sunibalai napaba"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "56|26|suniba kerala chalama arau chalama."
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "56|27|Arau somphalara dala; kimana ye bhagyabana somphalara dalato"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "56|28|Te'omloke thakiba ene udyanata, ya’ta ache kamitabihina bagarai gacha"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "56|29|arau thokerae bharaa kala gacha"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "56|30|arau samprasaraita cham"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "56|31|arau sadaya prabahamana pani"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "56|32|arau pracura phala-mula"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "56|33|yibora sesa naha'ba arau nisid'dha'o naha’ba."
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "56|34|Arau (sihamta thakiba) su'ucca sayyasamuhata"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "56|35|niscaya ami sihamtaka (hurasakalaka) srsti karaicho bisesaraupata"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "56|36|tara pichata sihamtaka kumarai bana'icho"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "56|37|nija sbamira prati asakta arau samabayasiya"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "56|38|somphalara lokasakalara babe."
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "56|39|Sihamtara bechibhage'i ha'ba purbarartilokasalara majara paraa"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "56|40|arau bahusankhyaka ha'ba parabartisakalara majara paraa."
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "56|41|Arau ba'omphalara dala, kimana ye durbhagiya ba'omphalara dalato"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "56|42|sihamta thakiba atyanta usna bayu arau uttapta utala panita"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "56|43|arau ati ka'la dhomraara chamta"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "56|44|yito sitalo nahaya arau araamadayako nahaya."
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "56|45|Niscaya sihamta itipurbe bhoga-bilasata magna achila"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "56|46|arau sihamta abiraama lipta achila jaghan'ya papa kamata."
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "56|47|Arau sihamte kaichila, ‘ami yetiya marai mati arau harata parainata hama tetiya'o amaka punaraut'thita karaa ha’bane’"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "56|48|arau amara pitrpurausasakalako (uthoraa ha’bane)"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "56|49|koraa, ‘niscaya purbarartisakalako arau parabartisakalako"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "56|50|sakaloke ekatraita karaa ha’ba eta nirdharaita dinara nirdista samayata’."
        }
    ]
}