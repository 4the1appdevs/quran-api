    "pages": [
        {
            "chapter": 76,
            "verse": 26,
            "text": "76|26|Arau raatira ekansata te'omra prati chajadaranata horaa arau raatira dirgha samaya te'omra paraitrata ghosana arau tachabiha patha karaa."
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "76|27|Niscaya sihamta prthiraira jiranaka bhala paya arau sihamte sihamtara sanmukhara kathina dinatoka upeksa karae."
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "76|28|Amiye'i sihamtaka srsti karaicho arau sihamtara gathana sudrrha karaicho. Arau ami yetiya iccha karaima tetiya'i sihamtara sthanata sihamtara darae (karaobaka) ani paraibartana karai dima."
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "76|29|Niscaya e'ito eta upadesa; eteke yiye iccha karae si yena tara pratipalakara phale yoraara patha bachi laya."
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "76|30|Arau allahe iccha nakaraile tomaloke iccha karaibalai saksama naha’ba; niscaya allaha mahajnani, prajnamaya."
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "76|31|Te'om yaka iccha take'i nija rahamatara antarbhukta karae, arau yalimasakalara babe te'om prastuta karai raakhiche yantranadayaka sasti."
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "77|1|Sapata kalyanasbaraupa praeraita batahara"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "77|2|tara pichata pracanda begata prabahita dhumuhara"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "77|3|sapata pracanda sancalanakaraira"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "77|4|tara pichata suspastaraupe parthakyakaraira"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "77|5|Tara pichata sihamtara, yisakale manuhara antarata upadesa paumca'i diye"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "77|6|ojara-apatti duraikarana arau satarka karaara babe"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "77|7|niscaya tomalokaka yi pratisrauti diya haiche seya arasye sanghatita ha’ba."
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "77|8|Yetiya naksatraraaji alohina ha’ba"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "77|9|arau yetiya akasakhana bidirna karaa ha’ba"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "77|10|arau yetiya parbatasamuhaka curnabicurna karaa ha’ba"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "77|11|arau yetiya raachulasakalaka nirdharaita samayata upasthita karaa ha’ba"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "77|12|konato dinara babe e'ibora sthagita rakha haiche"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "77|13|bicara dinara babe."
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "77|14|Arau kihe tomaka janaba bicara dirasa ki"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "77|15|se'i dina durbhoga mithyaraopakaraisakalara babe."
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "77|16|Ami purbarartisakalaka dhbansa karaa na'ine"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "77|17|tara pichata parabartisakalako sihamtara anugami banama."
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "77|18|Aparaadhibilakara lagata ami enekuraa acarane'i karao."
        },
        {
            "chapter": 77,
            "verse": 19,
