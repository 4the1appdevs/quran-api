{
    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "78|31|Niscaya muttakbisakalara babe ache saphalata."
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "78|32|Udyanasamuha arau angurasamuha."
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "78|33|Arau samabayasiya uda`bhinna nara-yurati tarauni."
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "78|34|Arau paraipurna panapatra."
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "78|35|Tata te'omloke kono anarthaka katha arau micha bakya'o suna napaba."
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "78|36|Aponara pratipalakara phalara paraa puraskara, yathocita danasbaraupa."
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "78|37|Yijana akasasamuha arau prthirai arau e'i dutara majata yi ache sakalorae pratipalaka, parama karaunamaya, te'omra ocarata abedana-nibedanara sakti sihamtara karao nathakiba."
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "78|38|Se'idina rauha arau phiraistasakala saraibad'dhabharae thiya ha’ba; rahamane yaka anumati diba tara bahirae kone'o katha naka’ba, arau te'om sathika katha ka’ba."
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "78|39|E'i dinato satya, eteke yara iccha siye tara pratipalakara ocarata asraya grahana karaka."
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "78|40|Niscaya ami tomalokaka ati nikatararti sasti samparke satarka karailo. Yidina manuhe nija krtakarma dekhibalai paba arau kaphirae ka’ba, haya! Ma'i yadi mati ha'lohemtena."
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "79|1|Sapata nirmamabharae utpatanakaraisakalara."
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "79|2|Arau mrdubharae bandhanamuktakaraisakalara."
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "79|3|Arau tibragatita samturai phuraasakalara."
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "79|4|Arau drautagatita agabarhi yoraasakalara."
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "79|5|Tara pichata yaratiya kama nirbahakaraisakalara."
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "79|6|Se'idina kampanakaraiye prakampita karaiba."
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "79|7|Taka anusarana karaiba parabarti kampanakarai."
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "79|8|Se'idina kichumana hrdaya, bhayata santrasta ha’ba."
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "79|9|Sihamtara drstisamuha atankita hai nata ha’ba."
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "79|10|Sihamte kaya, ‘ami akau agara arasthata ubhati yama ne"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "79|11|yetiya ami curnabicurna asthita parainata hama’"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "79|12|sihamte kaya, ‘yadi e'itorae haya tente e'ito eta ksatikara pratyarartana’."
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "79|13|Prakrtate e'ito eta biraata sabda."
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "79|14|Tatksanata bhu-prsthata sihamtara abirbhara ha’ba."
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "79|15|Muchara ghatanato tumi pa'ichane"
        }
    ]
}