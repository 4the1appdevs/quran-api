{"pages":[{"chapter":87,"verse":16,"text":"87|16|Kintu tomaloke prthiraira jiranaka praadhan'ya diya"},{"chapter":87,"verse":17,"text":"87|17|athaca akhiraata sarbottama arau sthayi."},{"chapter":87,"verse":18,"text":"87|18|Niscaya iyara barnana ache purbararti chahiphasamuhata"},{"chapter":87,"verse":19,"text":"87|19|ibraahima arau muchara chahiphasamuhata."},{"chapter":88,"verse":1,"text":"88|1|Tomara ocarata acchannakarai (kiyamatara) sambada ahichene"},{"chapter":88,"verse":2,"text":"88|2|se'idina kichumana mukhamandala ha’ba aranata"},{"chapter":88,"verse":3,"text":"88|3|karmaklanta, paraisraanta"},{"chapter":88,"verse":4,"text":"88|4|sihamta praraesa karaiba jbalanta ju'ita"},{"chapter":88,"verse":5,"text":"88|5|uttapta utala nijaraara pani pana karaibalai diya ha’ba"},{"chapter":88,"verse":6,"text":"88|6|sihamtara babe kamitabisista ghamhara bahirae ana kono khadya nathakiba"},{"chapter":88,"verse":7,"text":"88|7|i sihamtara pusti'o sadhana nakaraiba arau ksudha'o nibrtta nakaraiba."},{"chapter":88,"verse":8,"text":"88|8|Arau se'idina kichumana mukhamandala ha’ba anandojjbala"},{"chapter":88,"verse":9,"text":"88|9|nija karmara saphalyata paraitrpta"},{"chapter":88,"verse":10,"text":"88|10|su'ucca jannatata"},{"chapter":88,"verse":11,"text":"88|11|tata sihamte kono anarthaka katha sunibalai napaba"},{"chapter":88,"verse":12,"text":"88|12|tata thakiba prabahamana nijaraa"},{"chapter":88,"verse":13,"text":"88|13|tata thakiba unnata sayyasamuha"},{"chapter":88,"verse":14,"text":"88|14|Arau prastuta thakiba panapatrasamuha"},{"chapter":88,"verse":15,"text":"88|15|arau sarai saraikai rakha garausamuha"},{"chapter":88,"verse":16,"text":"88|16|arau bistrta parai thoraa dalicasamuha"},{"chapter":88,"verse":17,"text":"88|17|ihamte utara pine drstipata nakarae neki, kenekai taka srsti karaa haiche"},{"chapter":88,"verse":18,"text":"88|18|arau akasara phale, kenekai taka urd'dhata sthapana karaa haiche"},{"chapter":88,"verse":19,"text":"88|19|arau parbatasamuhara phale, se'ibora kenekai pratisthita karaa haiche"},{"chapter":88,"verse":20,"text":"88|20|arau prthiraikhanara phale, kidarae taka bistrta karaa haiche"},{"chapter":88,"verse":21,"text":"88|21|eteke tumi upadesa diya, tumito kerala ejana upadesadata"},{"chapter":88,"verse":22,"text":"88|22|tumi sihamtara oparata sakti prayogakarai nahaya."},{"chapter":88,"verse":23,"text":"88|23|Kintu konoba'i mukha ghuraa'i la’le athaba kupharai karaile"},{"chapter":88,"verse":24,"text":"88|24|tente allahe taka mahasasti pradana karaiba."},{"chapter":88,"verse":25,"text":"88|25|Niscaya amara ocarate'i sihamtara pratyarartana."},{"chapter":88,"verse":26,"text":"88|26|Arau sihamtara hicapa-nikaca amarae'i dayitba."}]}