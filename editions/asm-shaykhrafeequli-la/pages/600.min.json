{"pages":[{"chapter":100,"verse":10,"text":"100|10|Arau antarata yi ache seya prakasita ha’ba"},{"chapter":100,"verse":11,"text":"100|11|niscaya sihamtara pratipalaka se'idina sihamtara arastha samparke sabisesa arahita."},{"chapter":101,"verse":1,"text":"101|1|Bhitiprada maha bipada."},{"chapter":101,"verse":2,"text":"101|2|Bhitiprada maha bipada ki"},{"chapter":101,"verse":3,"text":"101|3|bhitiprada maha bipada samparke tumi ki jana"},{"chapter":101,"verse":4,"text":"101|4|se'idina manuha ha’ba biksipta kita-patangara darae."},{"chapter":101,"verse":5,"text":"101|5|Arau parbatasamuha ha’ba dhuna rangina unara darae."},{"chapter":101,"verse":6,"text":"101|6|Eteke yara tarjusamuha gadhura ha’ba"},{"chapter":101,"verse":7,"text":"101|7|te'om thakiba santosajanaka jiranata"},{"chapter":101,"verse":8,"text":"101|8|arau yara tarjusamuha patala ha’ba"},{"chapter":101,"verse":9,"text":"101|9|tara thikana ha’ba ‘ha-raiyaha’."},{"chapter":101,"verse":10,"text":"101|10|Arau tumi ki jana se'itono ki"},{"chapter":101,"verse":11,"text":"101|11|i eta atyanta uttapta agnikunda."},{"chapter":102,"verse":1,"text":"102|1|Praacuryara pratiyogitaya tomalokaka amanoyogi karai raakhiche."},{"chapter":102,"verse":2,"text":"102|2|Yetiyalaike tomaloke kabarata upanita nohoraa."},{"chapter":102,"verse":3,"text":"102|3|Ketiya'o nahaya, ati sonakale tomaloke janibalai paba."},{"chapter":102,"verse":4,"text":"102|4|Akau ketiya'o nahaya, ati sonakale tomaloke janibalai paba."},{"chapter":102,"verse":5,"text":"102|5|Ketiya'o nahaya, yadi tomaloke niscitabharae janilahemtena."},{"chapter":102,"verse":6,"text":"102|6|Niscaya tomaloke jahima (jahannama) dekhiba."},{"chapter":102,"verse":7,"text":"102|7|Akau, niscaya tomaloke iyaka patiyana yoraakai dekhiba."},{"chapter":102,"verse":8,"text":"102|8|Tara pichata se'idina arasya'i tomalokaka niyamata samparke prasna karaa ha’ba."}]}