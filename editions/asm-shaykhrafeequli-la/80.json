{
    "chapter": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "80|1|Te'om birakta prakasa karaile arau mukha ghuraa'i la’le"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "80|2|te'omra ocarata andha byaktijana ahara karane."
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "80|3|Kihe tomaka janaba ye, hayato si paraisud'dha ha’ba"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "80|4|athaba upadesa grahana karaiba arau se'i upadesa tara babe labhajanaka ha’ba."
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "80|5|Arau yiye nijake amukhapeksi buli bharae"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "80|6|tumi tara prati manoyoga dicha."
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "80|7|Athaca si paraisud'dha naha’le tomara kono dayitba na'i"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "80|8|anahate yiye tomara ocaralai daurai ahile"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "80|9|arau si (allahaka) bhayo karae"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "80|10|tara prati tumi udasina ha’la."
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "80|11|Ketiya'o nahaya, niscaya e'ito upadesa bani"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "80|12|gatike yiye iccha karaiba siye'i iyaka smarana raakhiba"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "80|13|e'ito maryada sampanna chahiphata lipibad'dha."
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "80|14|Samunnata, paraitra"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "80|15|lekhaka ba dutasakalara hatata."
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "80|16|(Yisakala) mahasanmanita, punyabana."
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "80|17|Manuha dhbansa ha'omka, kimana ye akrtajna"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "80|18|te'om taka ki bastura paraa srsti karaiche"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "80|19|sukrabindura paraa, te'om taka srsti karaiche, pichata tara paraimita bikasa sadhana karaiche"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "80|20|tara pichata tara babe patha sahaja karai diche"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "80|21|tara pichata taka mrtyu pradana karaile arau kabarastha karaile."
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "80|22|Akau yetiya te'om iccha karaiba taka punarjiraita karaiba."
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "80|23|Ketiya'o nahaya, te'om taka yi adesa karaichila seya si etiya'o purna karaa na'i."
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "80|24|Eteke manuhe nija khadyara prati laksya karaa ucita."
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "80|25|Niscaya ami pracura paraimane pani barsana karaom"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "80|26|Tara pichata ami prthiraika yathayathabharae bidirna karaom"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "80|27|tara pichata ami tata utpanna karaom sasya"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "80|28|anura arau saka-pacali"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "80|29|yayatuna arau khejura gacha"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "80|30|ghana gacha-gachani bisista udyana"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "80|31|phala-mula arau trna-ghamha"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "80|32|e'ibora tomalokara arau tomalokara gharaciya jantura jiranopakaranara babe."
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "80|33|Eteke yetiya tiksna sabda ahiba"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "80|34|se'idina manuhe palayana karaiba tara bhatrra paraa"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "80|35|arau tara maka-bapekara paraa"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "80|36|tara strai arau santanara paraa"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "80|37|se'idina sihamta pratyakarae ha’ba ene eka gurautara arastha yito taka sampurnaraupe byasta raakhiba."
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "80|38|Se'idina kichumana ceheraa ha’ba ujjbala"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "80|39|hamhirae bharaa, anandata utphulla"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "80|40|arau se'idina kichumana ceheraa dhulirae malina ha’ba"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "80|41|ka’la ararane ararai dharaiba."
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "80|42|E'omloke'i kaphira, atyanta papi."
        }
    ]
}