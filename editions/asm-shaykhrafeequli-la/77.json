{
    "chapter": [
        {
            "chapter": 77,
            "verse": 1,
            "text": "77|1|Sapata kalyanasbaraupa praeraita batahara"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "77|2|tara pichata pracanda begata prabahita dhumuhara"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "77|3|sapata pracanda sancalanakaraira"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "77|4|tara pichata suspastaraupe parthakyakaraira"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "77|5|Tara pichata sihamtara, yisakale manuhara antarata upadesa paumca'i diye"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "77|6|ojara-apatti duraikarana arau satarka karaara babe"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "77|7|niscaya tomalokaka yi pratisrauti diya haiche seya arasye sanghatita ha’ba."
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "77|8|Yetiya naksatraraaji alohina ha’ba"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "77|9|arau yetiya akasakhana bidirna karaa ha’ba"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "77|10|arau yetiya parbatasamuhaka curnabicurna karaa ha’ba"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "77|11|arau yetiya raachulasakalaka nirdharaita samayata upasthita karaa ha’ba"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "77|12|konato dinara babe e'ibora sthagita rakha haiche"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "77|13|bicara dinara babe."
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "77|14|Arau kihe tomaka janaba bicara dirasa ki"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "77|15|se'i dina durbhoga mithyaraopakaraisakalara babe."
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "77|16|Ami purbarartisakalaka dhbansa karaa na'ine"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "77|17|tara pichata parabartisakalako sihamtara anugami banama."
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "77|18|Aparaadhibilakara lagata ami enekuraa acarane'i karao."
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "77|19|Se'i dina durbhoga mithyaraopakaraisakalara babe."
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "77|20|Ami tomalokaka ghrnaniya panira paraa srsti karaa na'ine"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "77|21|tara pichata ami iyaka niraapada sthanata suraksita karai raakhicho"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "77|22|eta nirdista samaya laike"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "77|23|tara pichata ami paraimapa karaicho, eteke ami kimana nipuna paraimapakarai"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "77|24|se'i dina durbhoga mithyaraopakaraisakalara babe."
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "77|25|Ami prthiraikhanaka dharanakarairaupe srsti karaa na'ine"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "77|26|jiraitasakala arau mrtasakalara babe"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "77|27|arau ami tata sthapana karaicho sudrrha arau su'ucca parbatasamuha arau tomalokaka pana karaa'icho trsna nibaranakarai mitha upayogi pani."
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "77|28|Se'i dina durbhoga mithyaraopakaraisakalara babe."
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "77|29|(Sihamtaka koraa ha’ba) tomaloke yaka asbikara karaichila, etiya gati karaa taraphale."
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "77|30|Yoraa tinita sakha bisista ju'ira chamra pine"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "77|31|yito cham sitala nahaya arau i agnisikhara paraa raksa'o nakaraiba"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "77|32|niscaya jahanname utksepana karaiba brhat atraalika sadrsa phirainati"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "77|33|enekuraa yena haladhiya utara sraeni"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "77|34|se'i dina durbhoga mithyaraopakaraisakalara babe."
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "77|35|E'ito enekuraa dina yidina sihamte katha ka’ba noraaraiba"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "77|36|arau sihamtaka ajuhata peca karaara anumati'o diya naha’ba."
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "77|37|Se'i dina durbhoga mithyaraopakaraisakalara babe."
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "77|38|‘E'ito phayacalara dina ami ekatraita karaicho tomalokaka arau purbarartisakalaka’."
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "77|39|Eteke yadi tomalokara kono kausala ache tente prayoga karaa mora biraud'dhe."
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "77|40|Se'i dina durbhoga mithyaraopakaraisakalara babe."
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "77|41|Niscaya muttakbisakala thakiba chamra talata arau nijaraa bahula sthanata"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "77|42|arau sihamtara hempaha anuyayi phalamulara majata."
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "77|43|(Sihamtaka koraa ha'ba) tomaloke yi amala karaichila tara pratidanasbaraupe trptira saite panahara karaa"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "77|44|satkarmasilasakalaka ami enekuraa'i pratidana di thako."
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "77|45|Se'i dina durbhoga mithyaraopakaraisakalara babe."
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "77|46|(He kaphirasakala!) Tomaloke ahara karaa arau bhoga karaa khantekara babe; niscaya tomaloka aparaadhi."
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "77|47|Se'i dina durbhoga mithyaraopakaraisakalara babe."
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "77|48|Yetiya sihamtaka koraa haya, ‘rauku’ karaa sihamte rauku’ nakarae’."
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "77|49|Se'i dina durbhoga mithyaraopakaraisakalara babe."
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "77|50|Eteke kora'anara paraibarte sihamte arau kona banira prati imana aniba"
        }
    ]
}