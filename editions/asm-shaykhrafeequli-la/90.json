{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "90|1|Ma'i sapata karaicho e'i nagarakhanara"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "90|2|arau tumi e'i nagarara adhibasi"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "90|3|sapata janmadatara arau yito si janma diche."
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "90|4|Nisandehe ami manuhaka srsti karaicho kathora dukha-kastara majata."
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "90|5|Si bhabe neki ye, ketiya'o tara oparata konorae'i ksamatabana nahaâ€™ba"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "90|6|si kaya, 'ma'i pracura dhana-sampada nasa karaicho'."
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "90|7|Si bhabe neki ye, kone'o taka dekha na'i"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "90|8|tara babe ami duta caku srsti karaa na'ine"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "90|9|ekhana jibha arau duta omtha"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "90|10|arau ami taka duta patha dekhuraa'icho."
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "90|11|Kintu si durgama giraipathata praraesa karaa na'i."
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "90|12|Arau tomaka kihe janaba ye, durgama giraipatha ki"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "90|13|se'ito haiche, dasamukti."
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "90|14|Athaba durbhiksara dinata khadyadana karaa"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "90|15|nikatatmiya etimaka"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "90|16|athaba daraidra-nispesita abharagrastaka"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "90|17|tara pichata si enelokara antarbhukta hai yaya yisakale imana aniche arau parasparaka dhairya dharana karaibalai upadesa diche arau upadesa diche parasparaka daya-anugraha karaibalai"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "90|18|e'omloke'i saubhagyasali."
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "90|19|Arau yisakale amara ayatasamuhaka asbikara karaiche, sihamte'i hatabhagya."
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "90|20|Sihamta abaraud'dha ju'ita paraibestita thakiba."
        }
    ]
}