{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "96|1|parha tomara pratipalakara namata, yiye srsti karaiche"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "96|2|te'om manuhaka alakbara paraa srsti karaiche."
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "96|3|Parha, karana tomara pratipalaka mahamahima."
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "96|4|Yijane kalamara sahayata siksa diche."
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "96|5|Te'om manuhaka ene siksa diche, yito manuhe jana nachila."
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "96|6|Bastarate, manuhe simalanghana karai thake."
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "96|7|Karana si nijake sbanirbharasila buli bharae."
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "96|8|Niscaya tomara pratipalakara ocarate'i pratyarartana."
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "96|9|Tumi taka dekha na'ine? Yiye nisedha karae."
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "96|10|Ejana bandaka, yetiya te'om chalata adaya karae."
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "96|11|Koraacona, yadi'o te'om hidayatara oparata pratisthita"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "96|12|athaba takbaraara nirdesa diye; (tathapi'o te'omka kenekai badha diye)"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "96|13|koraacona yadi si (nisedhakarai) asbikara karae arau mukha ghuraa'i laya"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "96|14|si najane neki, niscaya allahe dekhi ache"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "96|15|ketiya'o nahaya, yadi si birata nathake, tente niscaya ami taka agaculita dharai tani anima."
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "96|16|Michaliya, papisthara agaculi (kapala)."
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "96|17|Eteke si nija paraisadaka mati anaka."
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "96|18|Anatipalame ami mati anima jahannamara kathora praharaisakalaka."
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "96|19|Ketiya'o nahaya, tumi tara anusarana nakaraiba. Arau tumi (tomara pratipalakaka) chajada karaa arau naikatya labha karaa."
        }
    ]
}