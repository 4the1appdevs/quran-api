{"rukus":[{"chapter":101,"verse":1,"text":"101|1|Bhitiprada maha bipada."},{"chapter":101,"verse":2,"text":"101|2|Bhitiprada maha bipada ki"},{"chapter":101,"verse":3,"text":"101|3|bhitiprada maha bipada samparke tumi ki jana"},{"chapter":101,"verse":4,"text":"101|4|se'idina manuha ha’ba biksipta kita-patangara darae."},{"chapter":101,"verse":5,"text":"101|5|Arau parbatasamuha ha’ba dhuna rangina unara darae."},{"chapter":101,"verse":6,"text":"101|6|Eteke yara tarjusamuha gadhura ha’ba"},{"chapter":101,"verse":7,"text":"101|7|te'om thakiba santosajanaka jiranata"},{"chapter":101,"verse":8,"text":"101|8|arau yara tarjusamuha patala ha’ba"},{"chapter":101,"verse":9,"text":"101|9|tara thikana ha’ba ‘ha-raiyaha’."},{"chapter":101,"verse":10,"text":"101|10|Arau tumi ki jana se'itono ki"},{"chapter":101,"verse":11,"text":"101|11|i eta atyanta uttapta agnikunda."}]}