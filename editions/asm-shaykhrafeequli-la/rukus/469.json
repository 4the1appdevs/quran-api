{
    "rukus": [
        {
            "chapter": 56,
            "verse": 1,
            "text": "56|1|Yetiya kiyamata sanghatita ha’ba"
        },
        {
            "chapter": 56,
            "verse": 2,
            "text": "56|2|(tetiya) iyara sanghatanara asbikarakarai kono nathakiba."
        },
        {
            "chapter": 56,
            "verse": 3,
            "text": "56|3|I karaobaka adhahpatana karaiba, karaobaka samunnata karaiba"
        },
        {
            "chapter": 56,
            "verse": 4,
            "text": "56|4|yetiya prthirai prabala prakampanerae prakampita ha’ba"
        },
        {
            "chapter": 56,
            "verse": 5,
            "text": "56|5|arau parbatasamuha curna-bicurna hai paraiba"
        },
        {
            "chapter": 56,
            "verse": 6,
            "text": "56|6|phalata se'ibora biksipta dhulikanata parainata ha’ba"
        },
        {
            "chapter": 56,
            "verse": 7,
            "text": "56|7|arau tomaloke bibhakta hai paraiba tinita dalata"
        },
        {
            "chapter": 56,
            "verse": 8,
            "text": "56|8|eteke somphalara dala; arau somphalara dalato kimana ye saubhagyabana"
        },
        {
            "chapter": 56,
            "verse": 9,
            "text": "56|9|arau ba'omphalara dala; arau ba'omphalara dalato kimana ye durbhagiya"
        },
        {
            "chapter": 56,
            "verse": 10,
            "text": "56|10|arau agragamisakale'i agragami"
        },
        {
            "chapter": 56,
            "verse": 11,
            "text": "56|11|te'omloke'i sannidhyapraapta"
        },
        {
            "chapter": 56,
            "verse": 12,
            "text": "56|12|te'omloka thakiba niyamatapurna jannatasamuhata"
        },
        {
            "chapter": 56,
            "verse": 13,
            "text": "56|13|bahusankhyaka ha’ba purbarartisakalara majara paraa"
        },
        {
            "chapter": 56,
            "verse": 14,
            "text": "56|14|arau kama sankhyaka ha’ba parabartisakalara majara paraa."
        },
        {
            "chapter": 56,
            "verse": 15,
            "text": "56|15|Sonerae nirmita asanamuhata"
        },
        {
            "chapter": 56,
            "verse": 16,
            "text": "56|16|sihamte parasparae mukhamukhikai a'uji bahiba."
        },
        {
            "chapter": 56,
            "verse": 17,
            "text": "56|17|Sihamtara ase-pase ghuraiphuraiba cira kisorasakale"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "56|18|prabahita nijaraara suraapurna panapatra arau piyalabora lai"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "56|19|se'i suraa pana karaile te'omlokara mura nughuraaya arau te'omloka jnanaharaa'o naha’ba"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "56|20|arau (ghuraiphuraiba) te'omlokara pachandaniya phala-mula lai"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "56|21|arau te'omlokara rauci anuyayi caraa'ira mansa lai."
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "56|22|Arau te'omlokara babe thakiba mrganayani (danara cakubisista) hura"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "56|23|yeniba sihamta suraksita mukutahe"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "56|24|sihamte yi amala karaichila tara puraskarasbaraupa."
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "56|25|Tata sihamte kono alagatiyala katha arau kono papabakya sunibalai napaba"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "56|26|suniba kerala chalama arau chalama."
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "56|27|Arau somphalara dala; kimana ye bhagyabana somphalara dalato"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "56|28|Te'omloke thakiba ene udyanata, ya’ta ache kamitabihina bagarai gacha"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "56|29|arau thokerae bharaa kala gacha"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "56|30|arau samprasaraita cham"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "56|31|arau sadaya prabahamana pani"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "56|32|arau pracura phala-mula"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "56|33|yibora sesa naha'ba arau nisid'dha'o naha’ba."
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "56|34|Arau (sihamta thakiba) su'ucca sayyasamuhata"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "56|35|niscaya ami sihamtaka (hurasakalaka) srsti karaicho bisesaraupata"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "56|36|tara pichata sihamtaka kumarai bana'icho"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "56|37|nija sbamira prati asakta arau samabayasiya"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "56|38|somphalara lokasakalara babe."
        }
    ]
}