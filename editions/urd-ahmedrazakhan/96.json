{
    "chapter": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "پڑھو اپنے رب کے نام سے جس نے پیدا کیا"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "آدمی کو خون کی پھٹک سے بنایا، پڑھو"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "اور تمہارا رب ہی سب سے بڑا کریم،"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "جس نے قلم سے لکھنا سکھایا"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "آدمی کو سکھایا جو نہ جانتا تھا"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "ہاں ہاں بیشک آدمی سرکشی کرتا ہے،"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "اس پر کہ اپنے آپ کو غنی سمجھ لیا"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "بیشک تمہارے رب ہی کی طرف پھرنا ہے"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "بھلا دیکھو تو جو منع کرتا ہے،"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "بندے کو جب وہ نماز پڑھے"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "بھلا دیکھو تو اگر وہ ہدایت پر ہوتا،"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "یا پرہیزگاری بتاتا تو کیا خوب تھا،"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "بھلا دیکھو تو اگر جھٹلایا اور منہ پھیرا"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "تو کیا حال ہوگا کیا نہ جانا کہ اللہ دیکھ رہا ہے"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "ہاں ہاں اگر باز نہ آیا تو ضرور ہم پیشانی کے بال پکڑ کر کھینچیں گے"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "کیسی پیشانی جھوٹی خطاکار،"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "اب پکارے اپنی مجلس کو"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "ابھی ہم سپاہیوں کو بلاتے ہیں"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "ہاں ہاں، اس کی نہ سنو اور سجدہ کرو اور ہم سے قریب ہوجاؤ، (السجدة ۔۱۴)"
        }
    ]
}