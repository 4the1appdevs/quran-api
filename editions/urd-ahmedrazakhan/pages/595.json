{
    "pages": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "سورج اور اس کی روشنی کی قسم،"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "اور چاند کی جب اس کے پیچھے آئے"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "اور دن کی جب اسے چمکائے"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "اور رات کی جب اسے چھپائے"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "اور آسمان اور اس کے بنانے والے کی قسم،"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "اور زمین اور اس کے پھیلانے والے کی قسم،"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "اور جان کی اور اس کی جس نے اسے ٹھیک بنایا"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "پھر اس کی بدکاری اور اس کی پرہیزگاری دل میں ڈالی"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "بیشک مراد کو پہنچایا جس نے اسے ستھرا کیا"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "اور نامراد ہوا جس نے اسے معصیت میں چھپایا،"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "ثمود نے اپنی سرکشی سے جھٹلایا"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "جبکہ اس کا سب سے بدبخت اٹھ کھڑا ہوا،"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "تو ان سے اللہ کے رسول نے فرمایا اللہ کے ناقہ اور اس کی پینے کی باری سے بچو"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "تو انہوں نے اسے جھٹلایا پھر ناقہ کی کوچیں کاٹ دیں تو ان پر ان کے رب نے ان کے گناہ کے سبب تباہی ڈال کر وہ بستی برابر کردی"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "اور اس کے پیچھا کرنے والے کا اسے خوف نہیں"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "اور رات کی قسم جب چھائے"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "اور دن کی جب چمکے"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "اور اس کی جس نے نر و مادہ بنائے"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "بیشک تمہاری کوشش مختلف ہے"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "تو وہ جس نے دیا اور پرہیزگاری کی"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "اور سب سے اچھی کو سچ مانا"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "تو بہت جلد ہم اسے آسانی مہیا کردیں گے"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "اور وہ جس نے بخل کیا اور بے پرواہ بنا"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "اور سب سے اچھی کو جھٹلایا"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "تو بہت جلد ہم اسے دشواری مہیا کردیں گے"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "اور اس کا مال اسے کام نہ آئے گا جب ہلاکت میں پڑے گا"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "بیشک ہدایت فرمانا ہمارے ذمہ ہے،"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "اور بیشک آخرت اور دنیا دونوں کے ہمیں مالک ہیں،"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "تو میں تمہیں ڈراتا ہوں اس آگ سے جو بھڑک رہی ہے،"
        }
    ]
}