{
    "pages": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "آسمان کی قسم اور رات کے آنے والے کی، ف۲"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "اور کچھ تم نے جا نا وه رات کو آنے والا کیا ہے،"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "خوب چمکتا تارا،"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "کوئی جان نہیں جس پر نگہبان نہ ہو"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "تو چاہئے کہ آدمی غور کرے کہ کس چیز سے بنا یا گیا"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "جَست کرتے (اوچھلتے ہوئے) پانی سے، ف۵"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "جو نکلتا ہے پیٹھ اور سینوں کے بیچ سے"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "بے شک اللہ اس کے واپس کرینے پر قادر ہے"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "جس دن چھپی باتوں کی جانچ ہوگی"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "تو آدمی کے پاس نہ کچھ زور ہوگا نہ کوئی مددگار"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "آسمان کی قسم! جس سے مینھ اترتا ہے"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "اور زمین کی جو اس سے کھلتی ہے"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "بیشک قرآن ضرور فیصلہ کی بات ہے"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "اور کوئی ہنسی کی بات نہیں"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "بیشک کافر اپنا سا داؤ چلتے ہیں"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "اور میں اپنی خفیہ تدبیر فرماتا ہوں"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "تو تم کافروں کو ڈھیل دو انہیں کچھ تھوڑی مہلت دو"
        },
        {
            "chapter": 87,
            "verse": 1,
            "text": "اپنے رب کے نام کی پاکی بولو جب سے بلند ہے"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "جس نے بناکر ٹھیک کیا"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "اور جس نے اندازہ پر رکھ کر راہ دی"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "اور جس نے چارہ نکالا،"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "پھر اسے خشک سیاہ کردیا،"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "اب ہم تمہیں پڑھائیں گے کہ تم نہ بھولو گے"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "مگر جو اللہ چاہے بیشک وہ جانتا ہے ہر کھلے اور چھپے کو،"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "اور ہم تمہارے لیے آسانی کا سامان کردیں گے"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "تو تم نصیحت فرماؤ اگر نصیحت کام دے"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "عنقریب نصیحت مانے گا جو ڈرتا ہے"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "اور اس سے وہ بڑا بدبخت دور رہے گا،"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "جو سب سے بڑی آگ میں جائے گا"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "پھر نہ اس میں مرے اور نہ جیے"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "بیشک مراد کو پہنچا جو ستھرا ہوا"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "اور اپنے رب کا نام لے کر نماز پڑھی"
        }
    ]
}