        {
            "chapter": 96,
            "verse": 10,
            "text": "عَبْداً إِذَا صَلَّيٰ ۖ‏"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "أَرَٰ۬يْتَ إِن كَانَ عَلَي اَ۬لْهُدَيٰ"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "أَوْ أَمَرَ بِالتَّقْوَيٰ"
        },
