{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "وَالْعَٰدِيَٰتِ ضَبْحا ࣰ‏"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "فَالْمُورِيَٰتِ قَدْحا ࣰ‏"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "فَالْمُغِيرَٰتِ صُبْحا ࣰ‏"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "فَأَثَرْنَ بِهِۦ نَقْعا ࣰ‏"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "فَوَسَطْنَ بِهِۦ جَمْعاً"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "إِنَّ اَ۬لْإِنسَٰنَ لِرَبِّهِۦ لَكَنُود ࣱ‏"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "وَإِنَّهُۥ عَلَيٰ ذَٰلِكَ لَشَهِيد ࣱ‏"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "وَإِنَّهُۥ لِحُبِّ اِ۬لْخَيْرِ لَشَدِيدٌ ۖ‏"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "۞أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِے اِ۬لْقُبُورِ"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "وَحُصِّلَ مَا فِے اِ۬لصُّدُورِ"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "إِنَّ رَبَّهُم بِهِمْ يَوْمَئِذ ࣲ لَّخَبِيرُۢ ۖ‏"
        }
    ]
}