{
    "maqras": [
        {
            "chapter": 76,
            "verse": 19,
            "text": "۞وَيَطُوفُ عَلَيْهِمْ وِلْدَٰن ࣱ مُّخَلَّدُونَ إِذَا رَأَيْتَهُمْ حَسِبْتَهُمْ لُؤْلُؤا ࣰ مَّنثُورا ࣰ ۖ‏"
        },
        {
            "chapter": 76,
            "verse": 20,
            "text": "وَإِذَا رَأَيْتَ ثَمَّ رَأَيْتَ نَعِيما ࣰ وَمُلْكا ࣰ كَبِيراً"
        },
        {
            "chapter": 76,
            "verse": 21,
            "text": "عَٰلِيهِمْ ثِيَابُ سُندُسٍ خُضْر ࣱ وَإِسْتَبْرَق ࣱ ۖ وَحُلُّواْ أَسَاوِرَ مِن فِضَّة ࣲ ۖ وَسَقَيٰهُمْ رَبُّهُمْ شَرَابا ࣰ طَهُوراً ۖ‏"
        },
        {
            "chapter": 76,
            "verse": 22,
            "text": "إِنَّ هَٰذَا كَانَ لَكُمْ جَزَآء ࣰ وَكَانَ سَعْيُكُم مَّشْكُوراً ۖ‏"
        },
        {
            "chapter": 76,
            "verse": 23,
            "text": "إِنَّا نَحْنُ نَزَّلْنَا عَلَيْكَ اَ۬لْقُرْءَانَ تَنزِيلا ࣰ ۖ‏"
        },
        {
            "chapter": 76,
            "verse": 24,
            "text": "فَاصْبِرْ لِحُكْمِ رَبِّكَ وَلَا تُطِعْ مِنْهُمْ ءَاثِماً أَوْ كَفُورا ࣰ ۖ‏"
        },
        {
            "chapter": 76,
            "verse": 25,
            "text": "وَاذْكُرِ اِ۪سْمَ رَبِّكَ بُكْرَة ࣰ وَأَصِيلا ࣰ ۖ‏"
        },
        {
            "chapter": 76,
            "verse": 26,
            "text": "وَمِنَ اَ۬ليْلِ فَاسْجُدْ لَهُۥ وَسَبِّحْهُ لَيْلا ࣰ طَوِيلاً ۖ‏"
        },
        {
            "chapter": 76,
            "verse": 27,
            "text": "إِنَّ هَٰؤُلَآءِ يُحِبُّونَ اَ۬لْعَاجِلَةَ وَيَذَرُونَ وَرَآءَهُمْ يَوْما ࣰ ثَقِيلا ࣰ ۖ‏"
        },
        {
            "chapter": 76,
            "verse": 28,
            "text": "نَّحْنُ خَلَقْنَٰهُمْ وَشَدَدْنَا أَسْرَهُمْ وَإِذَا شِئْنَا بَدَّلْنَا أَمْثَٰلَهُمْ تَبْدِيلاً ۖ‏"
        },
        {
            "chapter": 76,
            "verse": 29,
            "text": "إِنَّ هَٰذِهِۦ تَذْكِرَة ࣱ ۖ فَمَن شَآءَ اَ۪تَّخَذَ إِلَيٰ رَبِّهِۦ سَبِيلا ࣰ ۖ‏"
        },
        {
            "chapter": 76,
            "verse": 30,
            "text": "وَمَا تَشَآءُونَ إِلَّا أَنْ يَّشَآءَ اَ۬للَّهُ ۖ إِنَّ اَ۬للَّهَ كَانَ عَلِيماً حَكِيما ࣰ ۖ‏"
        },
        {
            "chapter": 76,
            "verse": 31,
            "text": "يُدْخِلُ مَنْ يَّشَآءُ فِے رَحْمَتِهِۦ ۖ وَالظَّٰلِمِينَ أَعَدَّ لَهُمْ عَذَاباً أَلِيماَۢ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 1,
            "text": "وَالْمُرْسَلَٰتِ عُرْفا ࣰ‏"
        },
        {
            "chapter": 77,
            "verse": 2,
            "text": "فَالْعَٰصِفَٰتِ عَصْفا ࣰ‏"
        },
        {
            "chapter": 77,
            "verse": 3,
            "text": "وَالنَّٰشِرَٰتِ نَشْرا ࣰ‏"
        },
        {
            "chapter": 77,
            "verse": 4,
            "text": "فَالْفَٰرِقَٰتِ فَرْقا ࣰ‏"
        },
        {
            "chapter": 77,
            "verse": 5,
            "text": "فَالْمُلْقِيَٰتِ ذِكْراً"
        },
        {
            "chapter": 77,
            "verse": 6,
            "text": "عُذْراً أَوْ نُذُراً"
        },
        {
            "chapter": 77,
            "verse": 7,
            "text": "إِنَّمَا تُوعَدُونَ لَوَٰقِع ࣱ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 8,
            "text": "فَإِذَا اَ۬لنُّجُومُ طُمِسَتْ"
        },
        {
            "chapter": 77,
            "verse": 9,
            "text": "وَإِذَا اَ۬لسَّمَآءُ فُرِجَتْ"
        },
        {
            "chapter": 77,
            "verse": 10,
            "text": "وَإِذَا اَ۬لْجِبَالُ نُسِفَتْ"
        },
        {
            "chapter": 77,
            "verse": 11,
            "text": "وَإِذَا اَ۬لرُّسُلُ أُقِّتَتْ"
        },
        {
            "chapter": 77,
            "verse": 12,
            "text": "لِأَيِّ يَوْمٍ أُجِّلَتْ"
        },
        {
            "chapter": 77,
            "verse": 13,
            "text": "لِيَوْمِ اِ۬لْفَصْلِ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 14,
            "text": "وَمَا أَدْرَيٰكَ مَا يَوْمُ اُ۬لْفَصْلِ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 15,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 16,
            "text": "۞أَلَمْ نُهْلِكِ اِ۬لْأَوَّلِينَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 17,
            "text": "ثُمَّ نُتْبِعُهُمُ اُ۬لْأٓخِرِينَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 18,
            "text": "كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 19,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 20,
            "text": "أَلَمْ نَخْلُقكُّم مِّن مَّآء ࣲ مَّهِين ࣲ‏"
        },
        {
            "chapter": 77,
            "verse": 21,
            "text": "فَجَعَلْنَٰهُ فِے قَرَار ࣲ مَّكِينٍ"
        },
        {
            "chapter": 77,
            "verse": 22,
            "text": "إِلَيٰ قَدَر ࣲ مَّعْلُوم ࣲ‏"
        },
        {
            "chapter": 77,
            "verse": 23,
            "text": "فَقَدَّرْنَا ۖ فَنِعْمَ اَ۬لْقَٰدِرُونَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 24,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 25,
            "text": "أَلَمْ نَجْعَلِ اِ۬لْأَرْضَ كِفَاتاً"
        },
        {
            "chapter": 77,
            "verse": 26,
            "text": "أَحْيَآء ࣰ وَأَمْوَٰتا ࣰ‏"
        },
        {
            "chapter": 77,
            "verse": 27,
            "text": "وَجَعَلْنَا فِيهَا رَوَٰسِيَ شَٰمِخَٰت ࣲ وَأَسْقَيْنَٰكُم مَّآء ࣰ فُرَاتا ࣰ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 28,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 29,
            "text": "اَ۪نطَلِقُواْ إِلَيٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ"
        },
        {
            "chapter": 77,
            "verse": 30,
            "text": "اَ۪نطَلِقُواْ إِلَيٰ ظِلّ ࣲ ذِے ثَلَٰثِ شُعَب ࣲ‏"
        },
        {
            "chapter": 77,
            "verse": 31,
            "text": "لَّا ظَلِيل ࣲ وَلَا يُغْنِے مِنَ اَ۬للَّهَبِ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 32,
            "text": "إِنَّهَا تَرْمِے بِشَرَر ࣲ كَالْقَصْرِ"
        },
        {
            "chapter": 77,
            "verse": 33,
            "text": "كَأَنَّهُۥ جِمَٰلَٰت ࣱ صُفْر ࣱ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 34,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 35,
            "text": "هَٰذَا يَوْمُ لَا يَنطِقُونَ"
        },
        {
            "chapter": 77,
            "verse": 36,
            "text": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 37,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 38,
            "text": "هَٰذَا يَوْمُ اُ۬لْفَصْلِ جَمَعْنَٰكُمْ وَالْأَوَّلِينَ"
        },
        {
            "chapter": 77,
            "verse": 39,
            "text": "فَإِن كَانَ لَكُمْ كَيْد ࣱ فَكِيدُونِ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 40,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 41,
            "text": "إِنَّ اَ۬لْمُتَّقِينَ فِے ظِلَٰل ࣲ وَعُيُون ࣲ‏"
        },
        {
            "chapter": 77,
            "verse": 42,
            "text": "وَفَوَٰكِهَ مِمَّا يَشْتَهُونَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 43,
            "text": "كُلُواْ وَاشْرَبُواْ ۖ هَنِيٓـٔاَۢ بِمَا كُنتُمْ تَعْمَلُونَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 44,
            "text": "إِنَّا كَذَٰلِكَ نَجْزِے اِ۬لْمُحْسِنِينَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 45,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 46,
            "text": "كُلُواْ وَتَمَتَّعُواْ قَلِيلاً إِنَّكُم مُّجْرِمُونَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 47,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 48,
            "text": "وَإِذَا قِيلَ لَهُمُ اُ۪رْكَعُواْ لَا يَرْكَعُونَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 49,
            "text": "وَيْل ࣱ يَوْمَئِذ ࣲ لِّلْمُكَذِّبِينَ ۖ‏"
        },
        {
            "chapter": 77,
            "verse": 50,
            "text": "فَبِأَيِّ حَدِيثِۢ بَعْدَهُۥ يُؤْمِنُونَ ۖ‏"
        }
    ]
}