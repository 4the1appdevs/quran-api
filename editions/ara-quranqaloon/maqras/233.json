{
    "maqras": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "عَمَّ يَتَسَآءَلُونَ"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "عَنِ اِ۬لنَّبَإِ اِ۬لْعَظِيمِ"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "اِ۬لذِے هُمْ فِيهِ مُخْتَلِفُونَ ۖ‏"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "كَلَّا سَيَعْلَمُونَ"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "ثُمَّ كَلَّا سَيَعْلَمُونَ ۖ‏"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "أَلَمْ نَجْعَلِ اِ۬لْأَرْضَ مِهَٰدا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "وَالْجِبَالَ أَوْتَادا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "وَخَلَقْنَٰكُمْ أَزْوَٰجا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "وَجَعَلْنَا نَوْمَكُمْ سُبَاتا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "وَجَعَلْنَا اَ۬ليْلَ لِبَاسا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "وَجَعَلْنَا اَ۬لنَّهَارَ مَعَاشا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "وَبَنَيْنَا فَوْقَكُمْ سَبْعا ࣰ شِدَادا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "وَجَعَلْنَا سِرَاجا ࣰ وَهَّاجا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "وَأَنزَلْنَا مِنَ اَ۬لْمُعْصِرَٰتِ مَآء ࣰ ثَجَّاجا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "لِّنُخْرِجَ بِهِۦ حَبّا ࣰ وَنَبَاتا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "وَجَنَّٰتٍ أَلْفَافاً ۖ‏"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "إِنَّ يَوْمَ اَ۬لْفَصْلِ كَانَ مِيقَٰتا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "يَوْمَ يُنفَخُ فِے اِ۬لصُّورِ فَتَأْتُونَ أَفْوَاجا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "وَفُتِّحَتِ اِ۬لسَّمَآءُ فَكَانَتْ أَبْوَابا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "وَسُيِّرَتِ اِ۬لْجِبَالُ فَكَانَتْ سَرَاباً ۖ‏"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "إِنَّ جَهَنَّمَ كَانَتْ مِرْصَادا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "لِّلطَّٰغِينَ مَـَٔابا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "لَّٰبِثِينَ فِيهَا أَحْقَابا ࣰ ۖ‏"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "لَّا يَذُوقُونَ فِيهَا بَرْدا ࣰ وَلَا شَرَاباً"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "إِلَّا حَمِيما ࣰ وَغَسَاقا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "جَزَآء ࣰ وِفَاقاً ۖ‏"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "إِنَّهُمْ كَانُواْ لَا يَرْجُونَ حِسَابا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "وَكَذَّبُواْ بِـَٔايَٰتِنَا كِذَّابا ࣰ ۖ‏"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "وَكُلَّ شَےْءٍ أَحْصَيْنَٰهُ كِتَٰبا ࣰ ۖ‏"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "فَذُوقُواْ فَلَن نَّزِيدَكُمْ إِلَّا عَذَاباً ۖ‏"
        },
        {
            "chapter": 78,
            "verse": 31,
            "text": "إِنَّ لِلْمُتَّقِينَ مَفَازاً"
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "حَدَآئِقَ وَأَعْنَٰبا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "وَكَوَاعِبَ أَتْرَابا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "وَكَأْسا ࣰ دِهَاقا ࣰ‏"
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "لَّا يَسْمَعُونَ فِيهَا لَغْوا ࣰ وَلَا كِذَّابا ࣰ ۖ‏"
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "جَزَآء ࣰ مِّن رَّبِّكَ عَطَآءً حِسَابا ࣰ ۖ‏"
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "رَّبُّ اُ۬لسَّمَٰوَٰتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا ۖ اَ۬لرَّحْمَٰنُ لَا يَمْلِكُونَ مِنْهُ خِطَابا ࣰ ۖ‏"
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "يَوْمَ يَقُومُ اُ۬لرُّوحُ وَالْمَلَٰٓئِكَةُ صَفّا ࣰ لَّا يَتَكَلَّمُونَ إِلَّا مَنْ أَذِنَ لَهُ اُ۬لرَّحْمَٰنُ وَقَالَ صَوَابا ࣰ ۖ‏"
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "ذَٰلِكَ اَ۬لْيَوْمُ اُ۬لْحَقُّ ۖ فَمَن شَآءَ اَ۪تَّخَذَ إِلَيٰ رَبِّهِۦ مَـَٔاباً ۖ‏"
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "إِنَّا أَنذَرْنَٰكُمْ عَذَابا ࣰ قَرِيبا ࣰ يَوْمَ يَنظُرُ اُ۬لْمَرْءُ مَا قَدَّمَتْ يَدَٰهُ وَيَقُولُ اُ۬لْكَافِرُ يَٰلَيْتَنِے كُنتُ تُرَٰباَۢ ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "وَالنَّٰزِعَٰتِ غَرْقا ࣰ‏"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "وَالنَّٰشِطَٰتِ نَشْطا ࣰ‏"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "وَالسَّٰبِحَٰتِ سَبْحا ࣰ‏"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "فَالسَّٰبِقَٰتِ سَبْقا ࣰ‏"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "فَالْمُدَبِّرَٰتِ أَمْرا ࣰ‏"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "يَوْمَ تَرْجُفُ اُ۬لرَّاجِفَةُ"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "تَتْبَعُهَا اَ۬لرَّادِفَةُ ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "قُلُوب ࣱ يَوْمَئِذ ࣲ وَاجِفَةٌ"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "أَبْصَٰرُهَا خَٰشِعَة ࣱ ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "يَقُولُونَ أَٰ۟نَّا لَمَرْدُودُونَ فِے اِ۬لْحَافِرَةِ"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "إِذَا كُنَّا عِظَٰما ࣰ نَّخِرَة ࣰ ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "قَالُواْ تِلْكَ إِذا ࣰ كَرَّةٌ خَاسِرَة ࣱ‏"
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "فَإِنَّمَا هِيَ زَجْرَة ࣱ وَٰحِدَة ࣱ‏"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "فَإِذَا هُم بِالسَّاهِرَةِ ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "هَلْ أَتَيٰكَ حَدِيثُ مُوسَيٰ"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "إِذْ نَادَيٰهُ رَبُّهُۥ بِالْوَادِ اِ۬لْمُقَدَّسِ طُوَي ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "اَ۪ذْهَبْ إِلَيٰ فِرْعَوْنَ إِنَّهُۥ طَغَيٰ"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "فَقُلْ هَل لَّكَ إِلَيٰ أَن تَزَّكَّيٰ"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "وَأَهْدِيَكَ إِلَيٰ رَبِّكَ فَتَخْشَيٰ ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "فَأَرَيٰهُ اُ۬لْأٓيَةَ اَ۬لْكُبْرَيٰ"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "فَكَذَّبَ وَعَصَيٰ"
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "ثُمَّ أَدْبَرَ يَسْعَيٰ"
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "فَحَشَرَ فَنَادَيٰ"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "فَقَالَ أَنَا رَبُّكُمُ اُ۬لْأَعْلَيٰ"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "فَأَخَذَهُ اُ۬للَّهُ نَكَالَ اَ۬لْأٓخِرَةِ وَالْأُولَيٰ ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "إِنَّ فِے ذَٰلِكَ لَعِبْرَة ࣰ لِّمَنْ يَّخْشَيٰ ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "ءَٰا۬نتُمْ أَشَدُّ خَلْقاً أَمِ اِ۬لسَّمَآءُ ۖ بَنَيٰهَا"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "رَفَعَ سَمْكَهَا فَسَوَّيٰهَا"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَيٰهَا ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 30,
            "text": "وَالْأَرْضَ بَعْدَ ذَٰلِكَ دَحَيٰهَا"
        },
        {
            "chapter": 79,
            "verse": 31,
            "text": "أَخْرَجَ مِنْهَا مَآءَهَا وَمَرْعَيٰهَا"
        },
        {
            "chapter": 79,
            "verse": 32,
            "text": "وَالْجِبَالَ أَرْسَيٰهَا"
        },
        {
            "chapter": 79,
            "verse": 33,
            "text": "مَتَٰعا ࣰ لَّكُمْ وَلِأَنْعَٰمِكُمْ ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 34,
            "text": "فَإِذَا جَآءَتِ اِ۬لطَّآمَّةُ اُ۬لْكُبْرَيٰ"
        },
        {
            "chapter": 79,
            "verse": 35,
            "text": "يَوْمَ يَتَذَكَّرُ اُ۬لْإِنسَٰنُ مَا سَعَيٰ"
        },
        {
            "chapter": 79,
            "verse": 36,
            "text": "وَبُرِّزَتِ اِ۬لْجَحِيمُ لِمَنْ يَّرَيٰ ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 37,
            "text": "فَأَمَّا مَن طَغَيٰ"
        },
        {
            "chapter": 79,
            "verse": 38,
            "text": "وَءَاثَرَ اَ۬لْحَيَوٰةَ اَ۬لدُّنْيَا"
        },
        {
            "chapter": 79,
            "verse": 39,
            "text": "فَإِنَّ اَ۬لْجَحِيمَ هِيَ اَ۬لْمَأْوَيٰ ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 40,
            "text": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِۦ وَنَهَي اَ۬لنَّفْسَ عَنِ اِ۬لْهَوَيٰ"
        },
        {
            "chapter": 79,
            "verse": 41,
            "text": "فَإِنَّ اَ۬لْجَنَّةَ هِيَ اَ۬لْمَأْوَيٰ ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 42,
            "text": "۞يَسْـَٔلُونَكَ عَنِ اِ۬لسَّاعَةِ أَيَّانَ مُرْسَيٰهَا ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 43,
            "text": "فِيمَ أَنتَ مِن ذِكْرَيٰهَا ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 44,
            "text": "إِلَيٰ رَبِّكَ مُنتَهَيٰهَا ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 45,
            "text": "إِنَّمَا أَنتَ مُنذِرُ مَنْ يَّخْشَيٰهَا ۖ‏"
        },
        {
            "chapter": 79,
            "verse": 46,
            "text": "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُواْ إِلَّا عَشِيَّةً أَوْ ضُحَيٰهَا ۖ‏"
        }
    ]
}