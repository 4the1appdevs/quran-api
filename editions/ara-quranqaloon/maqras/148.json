{
    "maqras": [
        {
            "chapter": 26,
            "verse": 52,
            "text": "وَأَوْحَيْنَا إِلَيٰ مُوسَيٰ أَنِ اِ۪سْرِ بِعِبَادِيَ إِنَّكُم مُّتَّبَعُونَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 53,
            "text": "فَأَرْسَلَ فِرْعَوْنُ فِے اِ۬لْمَدَآئِنِ حَٰشِرِينَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 54,
            "text": "إِنَّ هَٰؤُلَآءِ لَشِرْذِمَة ࣱ قَلِيلُونَ"
        },
        {
            "chapter": 26,
            "verse": 55,
            "text": "وَإِنَّهُمْ لَنَا لَغَآئِظُونَ"
        },
        {
            "chapter": 26,
            "verse": 56,
            "text": "وَإِنَّا لَجَمِيعٌ حَذِرُونَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 57,
            "text": "فَأَخْرَجْنَٰهُم مِّن جَنَّٰت ࣲ وَعُيُون ࣲ‏"
        },
        {
            "chapter": 26,
            "verse": 58,
            "text": "وَكُنُوز ࣲ وَمَقَام ࣲ كَرِيم ࣲ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 59,
            "text": "كَذَٰلِكَ ۖ وَأَوْرَثْنَٰهَا بَنِے إِسْرَآءِيلَ"
        },
        {
            "chapter": 26,
            "verse": 60,
            "text": "فَأَتْبَعُوهُم مُّشْرِقِينَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 61,
            "text": "فَلَمَّا تَرَٰٓءَا اَ۬لْجَمْعَٰنِ قَالَ أَصْحَٰبُ مُوسَيٰ إِنَّا لَمُدْرَكُونَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 62,
            "text": "قَالَ كَلَّا ۖ إِنَّ مَعِے رَبِّے سَيَهْدِينِ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 63,
            "text": "فَأَوْحَيْنَا إِلَيٰ مُوسَيٰ أَنِ اِ۪ضْرِب بِّعَصَاكَ اَ۬لْبَحْرَ فَانفَلَقَ فَكَانَ كُلُّ فِرْق ࣲ كَالطَّوْدِ اِ۬لْعَظِيمِ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 64,
            "text": "وَأَزْلَفْنَا ثَمَّ اَ۬لْأٓخَرِينَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 65,
            "text": "وَأَنجَيْنَا مُوسَيٰ وَمَن مَّعَهُۥ أَجْمَعِينَ"
        },
        {
            "chapter": 26,
            "verse": 66,
            "text": "ثُمَّ أَغْرَقْنَا اَ۬لْأٓخَرِينَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 67,
            "text": "إِنَّ فِے ذَٰلِكَ لَأٓيَة ࣰ ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 68,
            "text": "وَإِنَّ رَبَّكَ لَهْوَ اَ۬لْعَزِيزُ اُ۬لرَّحِيمُ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 69,
            "text": "وَاتْلُ عَلَيْهِمْ نَبَأَ ا۪بْرَٰهِيمَ"
        },
        {
            "chapter": 26,
            "verse": 70,
            "text": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِۦ مَا تَعْبُدُونَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "قَالُواْ نَعْبُدُ أَصْنَاما ࣰ فَنَظَلُّ لَهَا عَٰكِفِينَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "قَالَ هَلْ يَسْمَعُونَكُمْ إِذْ تَدْعُونَ"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "أَوْ يَنفَعُونَكُمْ أَوْ يَضُرُّونَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "قَالُواْ بَلْ وَجَدْنَا ءَابَآءَنَا كَذَٰلِكَ يَفْعَلُونَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "قَالَ أَفَرَٰ۬يْتُم مَّا كُنتُمْ تَعْبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "أَنتُمْ وَءَابَآؤُكُمُ اُ۬لْأَقْدَمُونَ"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "فَإِنَّهُمْ عَدُوّ ࣱ لِّيَ ۖ إِلَّا رَبَّ اَ۬لْعَٰلَمِينَ"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "اَ۬لذِے خَلَقَنِے فَهْوَ يَهْدِينِ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "وَالذِے هُوَ يُطْعِمُنِے وَيَسْقِينِ"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "وَإِذَا مَرِضْتُ فَهْوَ يَشْفِينِ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "وَالذِے يُمِيتُنِے ثُمَّ يُحْيِينِ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "۞وَالذِے أَطْمَعُ أَنْ يَّغْفِرَ لِے خَطِيَٓٔتِے يَوْمَ اَ۬لدِّينِ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "رَبِّ هَبْ لِے حُكْما ࣰ وَأَلْحِقْنِے بِالصَّٰلِحِينَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "وَاجْعَل لِّے لِسَانَ صِدْق ࣲ فِے اِ۬لْأٓخِرِينَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "وَاجْعَلْنِے مِنْ وَّرَثَةِ جَنَّةِ اِ۬لنَّعِيمِ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "وَاغْفِرْ لِأَبِيَ إِنَّهُۥ كَانَ مِنَ اَ۬لضَّآلِّينَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "وَلَا تُخْزِنِے يَوْمَ يُبْعَثُونَ"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "يَوْمَ لَا يَنفَعُ مَال ࣱ وَلَا بَنُونَ"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "إِلَّا مَنْ أَتَي اَ۬للَّهَ بِقَلْب ࣲ سَلِيم ࣲ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "وَأُزْلِفَتِ اِ۬لْجَنَّةُ لِلْمُتَّقِينَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "وَبُرِّزَتِ اِ۬لْجَحِيمُ لِلْغَاوِينَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "وَقِيلَ لَهُمْ أَيْنَ مَا كُنتُمْ تَعْبُدُونَ"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "مِن دُونِ اِ۬للَّهِ هَلْ يَنصُرُونَكُمْ أَوْ يَنتَصِرُونَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "فَكُبْكِبُواْ فِيهَا هُمْ وَالْغَاوُۥنَ"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "وَجُنُودُ إِبْلِيسَ أَجْمَعُونَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "قَالُواْ وَهُمْ فِيهَا يَخْتَصِمُونَ"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "تَاللَّهِ إِن كُنَّا لَفِے ضَلَٰل ࣲ مُّبِينٍ"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "إِذْ نُسَوِّيكُم بِرَبِّ اِ۬لْعَٰلَمِينَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "وَمَا أَضَلَّنَا إِلَّا اَ۬لْمُجْرِمُونَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "فَمَا لَنَا مِن شَٰفِعِينَ"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "وَلَا صَدِيقٍ حَمِيم ࣲ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "فَلَوْ أَنَّ لَنَا كَرَّة ࣰ فَنَكُونَ مِنَ اَ۬لْمُؤْمِنِينَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "إِنَّ فِے ذَٰلِكَ لَأٓيَة ࣰ ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "وَإِنَّ رَبَّكَ لَهْوَ اَ۬لْعَزِيزُ اُ۬لرَّحِيمُ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "كَذَّبَتْ قَوْمُ نُوحٍ اِ۬لْمُرْسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "إِذْ قَالَ لَهُمْ أَخُوهُمْ نُوحٌ أَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "إِنِّے لَكُمْ رَسُولٌ أَمِين ࣱ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "وَمَا أَسْـَٔلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِيَ إِلَّا عَلَيٰ رَبِّ اِ۬لْعَٰلَمِينَ ۖ‏"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِ ۖ‏"
        }
    ]
}