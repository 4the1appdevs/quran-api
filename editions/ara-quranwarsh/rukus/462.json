{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "أَفَرَٰٓيْتَ اَ۬لذِے تَوَلّ۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "وَأَعْط۪يٰ قَلِيلا ࣰ وَأَكْد۪يٰٓ"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "أَعِندَهُۥ عِلْمُ اُ۬لْغَيْبِ فَهُوَ يَر۪يٰٓۖ"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "أَمْ لَمْ يُنَبَّأْ بِمَا فِے صُحُفِ مُوس۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "وَإِبْرَٰهِيمَ اَ۬لذِے وَفّ۪يٰٓ"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "أَلَّا تَزِرُ وَازِرَة ࣱ وِزْرَ أُخْر۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "وَأَن لَّيْسَ لِلِانسَٰنِ إِلَّا مَا سَع۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "وَأَنَّ سَعْيَهُۥ سَوْفَ يُر۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "ثُمَّ يُجْز۪يٰهُ اُ۬لْجَزَآءَ اَ۬لَاوْف۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "وَأَنَّ إِلَيٰ رَبِّكَ اَ۬لْمُنتَه۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "وَأَنَّهُۥ هُوَ أَضْحَكَ وَأَبْك۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "وَأَنَّهُۥ هُوَ أَمَاتَ وَأَحْي۪اۖ"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "وَأَنَّهُۥ خَلَقَ اَ۬لزَّوْجَيْنِ اِ۬لذَّكَرَ وَالُانث۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "مِن نُّطْفَةٍ اِذَا تُمْن۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "۞وَأَنَّ عَلَيْهِ اِ۬لنَّشْأَةَ اَ۬لُاخْر۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "وَأَنَّهُۥ هُوَ أَغْن۪يٰ وَأَقْن۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "وَأَنَّهُۥ هُوَ رَبُّ اُ۬لشِّعْر۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "وَأَنَّهُۥٓ أَهْلَكَ عَادا ࣰ اَ۬لُّاول۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "وَثَمُودا ࣰ فَمَآ أَبْق۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "وَقَوْمَ نُوح ࣲ مِّن قَبْلُۖ إِنَّهُمْ كَانُواْ هُمُۥٓ أَظْلَمَ وَأَطْغ۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "وَالْمُوتَفِكَةَ أَهْو۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "فَغَشّ۪يٰهَا مَا غَشّ۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "فَبِأَيِّ ءَالَآءِ رَبِّكَ تَتَمَار۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "هَٰذَا نَذِير ࣱ مِّنَ اَ۬لنُّذُرِ اِ۬لُاول۪يٰٓۖ"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "أَزِفَتِ اِ۬لَازِفَةُۖ"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "لَيْسَ لَهَا مِن دُونِ اِ۬للَّهِ كَاشِفَةٌۖ"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "اَفَمِنْ هَٰذَا اَ۬لْحَدِيثِ تَعْجَبُونَ"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "وَتَضْحَكُونَ وَلَا تَبْكُونَ"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "وَأَنتُمْ سَٰمِدُونَۖ"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "فَاسْجُدُواْ لِلهِ وَاعْبُدُواْ۩ۖ"
        }
    ]
}