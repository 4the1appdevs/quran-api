{
    "rukus": [
        {
            "chapter": 53,
            "verse": 1,
            "text": "وَالنَّجْمِ إِذَا هَو۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "مَا ضَلَّ صَٰحِبُكُمْ وَمَا غَو۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "وَمَا يَنطِقُ عَنِ اِ۬لْهَو۪يٰٓ"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "إِنْ هُوَ إِلَّا وَحْي ࣱ يُوح۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "عَلَّمَهُۥ شَدِيدُ اُ۬لْقُو۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "ذُو مِرَّة ࣲ ۖ فَاسْتَو۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "وَهُوَ بِالُافُقِ اِ۬لَاعْل۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "ثُمَّ دَنَا فَتَدَلّ۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "فَكَانَ قَابَ قَوْسَيْنِ أَوَ اَدْن۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "فَأَوْح۪يٰٓ إِلَيٰ عَبْدِهِۦ مَآ أَوْح۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "مَا كَذَبَ اَ۬لْفُؤَادُ مَا ر۪أ۪يٰٓۖ"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "أَفَتُمَٰرُونَهُۥ عَلَيٰ مَا يَر۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "وَلَقَدْ ر۪ء۪اهُ نَزْلَةً ا۟خْر۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "عِندَ سِدْرَةِ اِ۬لْمُنتَه۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "عِندَهَا جَنَّةُ اُ۬لْمَأْو۪يٰٓ"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "إِذْ يَغْشَي اَ۬لسِّدْرَةَ مَا يَغْش۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "مَا زَاغَ اَ۬لْبَصَرُ وَمَا طَغ۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "لَقَدْ ر۪أ۪يٰ مِنَ اٰيَٰتِ رَبِّهِ اِ۬لْكُبْر۪يٰٓۖ"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "أَفَرَٰٓيْتُمُ اُ۬للَّٰتَ وَالْعُزّ۪يٰ"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "وَمَنَوٰةَ اَ۬لثَّالِثَةَ اَ۬لُاخْر۪يٰٓ"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "أَلَكُمُ اُ۬لذَّكَرُ وَلَهُ اُ۬لُانث۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "تِلْكَ إِذا ࣰ قِسْمَة ࣱ ضِيز۪يٰٓۖ"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "إِنْ هِيَ إِلَّآ أَسْمَآء ࣱ سَمَّيْتُمُوهَآ أَنتُمْ وَءَابَآؤُكُم مَّآ أَنزَلَ اَ۬للَّهُ بِهَا مِن سُلْطَٰنٍۖ اِنْ يَّتَّبِعُونَ إِلَّا اَ۬لظَّنَّ وَمَا تَهْوَي اَ۬لَانفُسُۖ وَلَقَدْ جَآءَهُم مِّن رَّبِّهِمُ اُ۬لْهُد۪يٰٓۖ"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "أَمْ لِلِانسَٰنِ مَا تَمَنّ۪يٰۖ"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "فَلِلهِ اِ۬لَاخِرَةُ وَالُاول۪يٰۖ"
        }
    ]
}