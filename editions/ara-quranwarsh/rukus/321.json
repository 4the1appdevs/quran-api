{
    "rukus": [
        {
            "chapter": 26,
            "verse": 105,
            "text": "كَذَّبَتْ قَوْمُ نُوحٍ اِ۬لْمُرْسَلِينَ"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "إِذْ قَالَ لَهُمُۥٓ أَخُوهُمْ نُوحٌ اَلَا تَتَّقُونَ"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "إِنِّے لَكُمْ رَسُولٌ اَمِين ࣱ ۖ"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِۖ"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنَ اَجْرٍۖ اِنَ اَجْرِيَ إِلَّا عَلَيٰ رَبِّ اِ۬لْعَٰلَمِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "فَاتَّقُواْ اُ۬للَّهَ وَأَطِيعُونِۖ"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "۞قَالُوٓاْ أَنُومِنُ لَكَ وَاتَّبَعَكَ اَ۬لَارْذَلُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 112,
            "text": "قَالَ وَمَا عِلْمِے بِمَا كَانُواْ يَعْمَلُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 113,
            "text": "إِنْ حِسَابُهُمُۥٓ إِلَّا عَلَيٰ رَبِّے لَوْ تَشْعُرُونَۖ"
        },
        {
            "chapter": 26,
            "verse": 114,
            "text": "وَمَآ أَنَا بِطَارِدِ اِ۬لْمُومِنِينَ"
        },
        {
            "chapter": 26,
            "verse": 115,
            "text": "إِنَ اَنَا إِلَّا نَذِير ࣱ مُّبِين ࣱ ۖ"
        },
        {
            "chapter": 26,
            "verse": 116,
            "text": "قَالُواْ لَئِن لَّمْ تَنتَهِ يَٰنُوحُ لَتَكُونَنَّ مِنَ اَ۬لْمَرْجُومِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 117,
            "text": "قَالَ رَبِّ إِنَّ قَوْمِے كَذَّبُونِ"
        },
        {
            "chapter": 26,
            "verse": 118,
            "text": "فَافْتَحْ بَيْنِے وَبَيْنَهُمْ فَتْحا ࣰ وَنَجِّنِے وَمَن مَّعِيَ مِنَ اَ۬لْمُومِنِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 119,
            "text": "فَأَنجَيْنَٰهُ وَمَن مَّعَهُۥ فِے اِ۬لْفُلْكِ اِ۬لْمَشْحُونِۖ"
        },
        {
            "chapter": 26,
            "verse": 120,
            "text": "ثُمَّ أَغْرَقْنَا بَعْدُ اُ۬لْبَاقِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 121,
            "text": "إِنَّ فِے ذَٰلِكَ لَأٓيَة ࣰ ۖ وَمَا كَانَ أَكْثَرُهُم مُّومِنِينَۖ"
        },
        {
            "chapter": 26,
            "verse": 122,
            "text": "وَإِنَّ رَبَّكَ لَهُوَ اَ۬لْعَزِيزُ اُ۬لرَّحِيمُۖ"
        }
    ]
}