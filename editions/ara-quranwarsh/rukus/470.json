{
    "rukus": [
        {
            "chapter": 56,
            "verse": 39,
            "text": "ثُلَّة ࣱ مِّنَ اَ۬لَاوَّلِينَ"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "وَثُلَّة ࣱ مِّنَ اَ۬لَاخِرِينَۖ"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "وَأَصْحَٰبُ اُ۬لشِّمَالِ مَآ أَصْحَٰبُ اُ۬لشِّمَالِۖ"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "فِے سَمُوم ࣲ وَحَمِيم ࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "وَظِلّ ࣲ مِّنْ يَّحْمُوم ࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "لَّا بَارِد ࣲ وَلَا كَرِيمٍۖ"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "اِنَّهُمْ كَانُواْ قَبْلَ ذَٰلِكَ مُتْرَفِينَۖ"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "وَكَانُواْ يُصِرُّونَ عَلَي اَ۬لْحِنثِ اِ۬لْعَظِيمِۖ"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "وَكَانُواْ يَقُولُونَ أَئِذَا مِتْنَا وَكُنَّا تُرَابا ࣰ وَعِظَٰماً اِنَّا لَمَبْعُوثُونَ"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "أَوَءَابَآؤُنَا اَ۬لَاوَّلُونَۖ"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "۞قُلِ اِنَّ اَ۬لَاوَّلِينَ وَالَاخِرِينَ"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "لَمَجْمُوعُونَ إِلَيٰ مِيقَٰتِ يَوْم ࣲ مَّعْلُوم ࣲ ۖ"
        },
        {
            "chapter": 56,
            "verse": 51,
            "text": "ثُمَّ إِنَّكُمُۥٓ أَيُّهَا اَ۬لضَّآلُّونَ اَ۬لْمُكَذِّبُونَ"
        },
        {
            "chapter": 56,
            "verse": 52,
            "text": "لَأٓكِلُونَ مِن شَجَر ࣲ مِّن زَقُّوم ࣲ‏"
        },
        {
            "chapter": 56,
            "verse": 53,
            "text": "فَمَالِـُٔونَ مِنْهَا اَ۬لْبُطُونَ"
        },
        {
            "chapter": 56,
            "verse": 54,
            "text": "فَشَٰرِبُونَ عَلَيْهِ مِنَ اَ۬لْحَمِيمِ"
        },
        {
            "chapter": 56,
            "verse": 55,
            "text": "فَشَٰرِبُونَ شُرْبَ اَ۬لْهِيمِۖ"
        },
        {
            "chapter": 56,
            "verse": 56,
            "text": "هَٰذَا نُزُلُهُمْ يَوْمَ اَ۬لدِّينِۖ"
        },
        {
            "chapter": 56,
            "verse": 57,
            "text": "نَحْنُ خَلَقْنَٰكُمْۖ فَلَوْلَا تُصَدِّقُونَۖ"
        },
        {
            "chapter": 56,
            "verse": 58,
            "text": "أَفَرَٰٓيْتُم مَّا تُمْنُونَ"
        },
        {
            "chapter": 56,
            "verse": 59,
            "text": "ءَآنتُمْ تَخْلُقُونَهُۥٓ أَمْ نَحْنُ اُ۬لْخَٰلِقُونَۖ"
        },
        {
            "chapter": 56,
            "verse": 60,
            "text": "نَحْنُ قَدَّرْنَا بَيْنَكُمُ اُ۬لْمَوْتَ وَمَا نَحْنُ بِمَسْبُوقِينَ"
        },
        {
            "chapter": 56,
            "verse": 61,
            "text": "عَلَيٰٓ أَن نُّبَدِّلَ أَمْثَٰلَكُمْ وَنُنشِئَكُمْ فِے مَا لَا تَعْلَمُونَۖ"
        },
        {
            "chapter": 56,
            "verse": 62,
            "text": "وَلَقَدْ عَلِمْتُمُ اُ۬لنَّشْأَةَ اَ۬لُاول۪يٰۖ فَلَوْلَا تَذَّكَّرُونَۖ"
        },
        {
            "chapter": 56,
            "verse": 63,
            "text": "أَفَرَٰٓيْتُم مَّا تَحْرُثُونَ"
        },
        {
            "chapter": 56,
            "verse": 64,
            "text": "ءَآنتُمْ تَزْرَعُونَهُۥٓ أَمْ نَحْنُ اُ۬لزَّٰرِعُونَۖ"
        },
        {
            "chapter": 56,
            "verse": 65,
            "text": "لَوْ نَشَآءُ لَجَعَلْنَٰهُ حُطَٰما ࣰ فَظَلْتُمْ تَفَكَّهُونَ"
        },
        {
            "chapter": 56,
            "verse": 66,
            "text": "إِنَّا لَمُغْرَمُونَ"
        },
        {
            "chapter": 56,
            "verse": 67,
            "text": "بَلْ نَحْنُ مَحْرُومُونَۖ"
        },
        {
            "chapter": 56,
            "verse": 68,
            "text": "أَفَرَٰٓيْتُمُ اُ۬لْمَآءَ اَ۬لذِے تَشْرَبُونَ"
        },
        {
            "chapter": 56,
            "verse": 69,
            "text": "ءَآنتُمُۥٓ أَنزَلْتُمُوهُ مِنَ اَ۬لْمُزْنِ أَمْ نَحْنُ اُ۬لْمُنزِلُونَۖ"
        },
        {
            "chapter": 56,
            "verse": 70,
            "text": "لَوْ نَشَآءُ جَعَلْنَٰهُ أُجَاجا ࣰ ۖ فَلَوْلَا تَشْكُرُونَۖ"
        },
        {
            "chapter": 56,
            "verse": 71,
            "text": "أَفَرَٰٓيْتُمُ اُ۬لنَّارَ اَ۬لتِے تُورُونَ"
        },
        {
            "chapter": 56,
            "verse": 72,
            "text": "ءَآنتُمُۥٓ أَنشَأْتُمْ شَجَرَتَهَآ أَمْ نَحْنُ اُ۬لْمُنشِـُٔونَۖ"
        },
        {
            "chapter": 56,
            "verse": 73,
            "text": "نَحْنُ جَعَلْنَٰهَا تَذْكِرَة ࣰ وَمَتَٰعا ࣰ لِّلْمُقْوِينَۖ"
        },
        {
            "chapter": 56,
            "verse": 74,
            "text": "فَسَبِّحْ بِاسْمِ رَبِّكَ اَ۬لْعَظِيمِۖ"
        }
    ]
}