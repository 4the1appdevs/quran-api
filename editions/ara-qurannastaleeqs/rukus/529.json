{
    "rukus": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "ﶒ ؀سَبِّحِ اسۡمَ رَبِّكَ الۡاَعۡلَي"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "ﶫ الَّذِيۡ خَلَقَ فَسَوّٰي"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "ﶟ وَالَّذِيۡ قَدَّرَ فَهَدٰي"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "ﶟ وَالَّذِيۡ٘ اَخۡرَجَ الۡمَرۡعٰي"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "ﶟ فَجَعَلَهٗ غُثَآءً اَحۡوٰي"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "ﶠ سَنُقۡرِئُكَ فَلَا تَنۡسٰ٘ي"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "ﶫ اِلَّا مَا شَآءَ اللّٰهُﵧ اِنَّهٗ يَعۡلَمُ الۡجَهۡرَ وَمَا يَخۡفٰي"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "ﶠ وَنُيَسِّرُكَ لِلۡيُسۡرٰي"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "ﶗ فَذَكِّرۡ اِنۡ نَّفَعَتِ الذِّكۡرٰي"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "ﶠ سَيَذَّكَّرُ مَنۡ يَّخۡشٰي"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "ﶫ وَيَتَجَنَّبُهَا الۡاَشۡقَي"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "ﶫ الَّذِيۡ يَصۡلَي النَّارَ الۡكُبۡرٰي"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "ﶔ ثُمَّ لَا يَمُوۡتُ فِيۡهَا وَلَا يَحۡيٰي"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "ﶠ قَدۡ اَفۡلَحَ مَنۡ تَزَكّٰي"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "ﶫ وَذَكَرَ اسۡمَ رَبِّهٖ فَصَلّٰي"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "ﶠ بَلۡ تُؤۡثِرُوۡنَ الۡحَيٰوةَ الدُّنۡيَا"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "ﶫ وَالۡاٰخِرَةُ خَيۡرٌ وَّاَبۡقٰي"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "ﶠ اِنَّ هٰذَا لَفِي الصُّحُفِ الۡاُوۡلٰي"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "ﶫ صُحُفِ اِبۡرٰهِيۡمَ وَمُوۡسٰي"
        }
    ]
}