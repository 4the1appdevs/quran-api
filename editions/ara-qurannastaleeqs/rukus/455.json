{
    "rukus": [
        {
            "chapter": 51,
            "verse": 1,
            "text": "ﶒ ؀وَالذّٰرِيٰتِ ذَرۡوًا"
        },
        {
            "chapter": 51,
            "verse": 2,
            "text": "ﶫ فَالۡحٰمِلٰتِ وِقۡرًا"
        },
        {
            "chapter": 51,
            "verse": 3,
            "text": "ﶫ فَالۡجٰرِيٰتِ يُسۡرًا"
        },
        {
            "chapter": 51,
            "verse": 4,
            "text": "ﶫ فَالۡمُقَسِّمٰتِ اَمۡرًا"
        },
        {
            "chapter": 51,
            "verse": 5,
            "text": "ﶫ اِنَّمَا تُوۡعَدُوۡنَ لَصَادِقٌ"
        },
        {
            "chapter": 51,
            "verse": 6,
            "text": "ﶫ وَّاِنَّ الدِّيۡنَ لَوَاقِعٌ"
        },
        {
            "chapter": 51,
            "verse": 7,
            "text": "ﶠ وَالسَّمَآءِ ذَاتِ الۡحُبُكِ"
        },
        {
            "chapter": 51,
            "verse": 8,
            "text": "ﶫ اِنَّكُمۡ لَفِيۡ قَوۡلٍ مُّخۡتَلِفٍ"
        },
        {
            "chapter": 51,
            "verse": 9,
            "text": "ﶫ يُّؤۡفَكُ عَنۡهُ مَنۡ اُفِكَ"
        },
        {
            "chapter": 51,
            "verse": 10,
            "text": "ﶠ قُتِلَ الۡخَرّٰصُوۡنَ"
        },
        {
            "chapter": 51,
            "verse": 11,
            "text": "ﶫ الَّذِيۡنَ هُمۡ فِيۡ غَمۡرَةٍ سَاهُوۡنَ"
        },
        {
            "chapter": 51,
            "verse": 12,
            "text": "ﶫ يَسۡـَٔلُوۡنَ اَيَّانَ يَوۡمُ الدِّيۡنِ"
        },
        {
            "chapter": 51,
            "verse": 13,
            "text": "ﶠ يَوۡمَ هُمۡ عَلَي النَّارِ يُفۡتَنُوۡنَ"
        },
        {
            "chapter": 51,
            "verse": 14,
            "text": "ذُوۡقُوۡا فِتۡنَتَكُمۡﵧ هٰذَا الَّذِيۡ كُنۡتُمۡ بِهٖ تَسۡتَعۡجِلُوۡنَ"
        },
        {
            "chapter": 51,
            "verse": 15,
            "text": "اِنَّ الۡمُتَّقِيۡنَ فِيۡ جَنّٰتٍ وَّعُيُوۡنٍ"
        },
        {
            "chapter": 51,
            "verse": 16,
            "text": "ﶫ اٰخِذِيۡنَ مَا٘ اٰتٰىهُمۡ رَبُّهُمۡﵧ اِنَّهُمۡ كَانُوۡا قَبۡلَ ذٰلِكَ مُحۡسِنِيۡنَ"
        },
        {
            "chapter": 51,
            "verse": 17,
            "text": "ﶠ كَانُوۡا قَلِيۡلًا مِّنَ الَّيۡلِ مَا يَهۡجَعُوۡنَ"
        },
        {
            "chapter": 51,
            "verse": 18,
            "text": "وَبِالۡاَسۡحَارِ هُمۡ يَسۡتَغۡفِرُوۡنَ"
        },
        {
            "chapter": 51,
            "verse": 19,
            "text": "وَفِيۡ٘ اَمۡوَالِهِمۡ حَقٌّ لِّلسَّآئِلِ وَالۡمَحۡرُوۡمِ"
        },
        {
            "chapter": 51,
            "verse": 20,
            "text": "وَفِي الۡاَرۡضِ اٰيٰتٌ لِّلۡمُوۡقِنِيۡنَ"
        },
        {
            "chapter": 51,
            "verse": 21,
            "text": "ﶫ وَفِيۡ٘ اَنۡفُسِكُمۡﵧ اَفَلَا تُبۡصِرُوۡنَ"
        },
        {
            "chapter": 51,
            "verse": 22,
            "text": "وَفِي السَّمَآءِ رِزۡقُكُمۡ وَمَا تُوۡعَدُوۡنَ"
        },
        {
            "chapter": 51,
            "verse": 23,
            "text": "فَوَرَبِّ السَّمَآءِ وَالۡاَرۡضِ اِنَّهٗ لَحَقٌّ مِّثۡلَ مَا٘ اَنَّكُمۡ تَنۡطِقُوۡنَ"
        }
    ]
}