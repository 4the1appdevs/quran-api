{
    "rukus": [
        {
            "chapter": 53,
            "verse": 33,
            "text": "ﶒ اَفَرَءَيۡتَ الَّذِيۡ تَوَلّٰي"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "ﶫ وَاَعۡطٰي قَلِيۡلًا وَّاَكۡدٰي"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "اَعِنۡدَهٗ عِلۡمُ الۡغَيۡبِ فَهُوَ يَرٰي"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "اَمۡ لَمۡ يُنَبَّاۡ بِمَا فِيۡ صُحُفِ مُوۡسٰي"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "ﶫ وَاِبۡرٰهِيۡمَ الَّذِيۡ وَفّٰ٘ي"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "ﶫ اَلَّا تَزِرُ وَازِرَةٌ وِّزۡرَ اُخۡرٰي"
        },
        {
            "chapter": 53,
            "verse": 39,
            "text": "ﶫ وَاَنۡ لَّيۡسَ لِلۡاِنۡسَانِ اِلَّا مَا سَعٰي"
        },
        {
            "chapter": 53,
            "verse": 40,
            "text": "ﶫ وَاَنَّ سَعۡيَهٗ سَوۡفَ يُرٰي"
        },
        {
            "chapter": 53,
            "verse": 41,
            "text": "ثُمَّ يُجۡزٰىهُ الۡجَزَآءَ الۡاَوۡفٰي"
        },
        {
            "chapter": 53,
            "verse": 42,
            "text": "ﶫ وَاَنَّ اِلٰي رَبِّكَ الۡمُنۡتَهٰي"
        },
        {
            "chapter": 53,
            "verse": 43,
            "text": "ﶫ وَاَنَّهٗ هُوَ اَضۡحَكَ وَاَبۡكٰي"
        },
        {
            "chapter": 53,
            "verse": 44,
            "text": "ﶫ وَاَنَّهٗ هُوَ اَمَاتَ وَاَحۡيَا"
        },
        {
            "chapter": 53,
            "verse": 45,
            "text": "ﶫ وَاَنَّهٗ خَلَقَ الزَّوۡجَيۡنِ الذَّكَرَ وَالۡاُنۡثٰي"
        },
        {
            "chapter": 53,
            "verse": 46,
            "text": "ﶫ مِنۡ نُّطۡفَةٍ اِذَا تُمۡنٰي"
        },
        {
            "chapter": 53,
            "verse": 47,
            "text": "ﶝ وَاَنَّ عَلَيۡهِ النَّشۡاَةَ الۡاُخۡرٰي"
        },
        {
            "chapter": 53,
            "verse": 48,
            "text": "ﶫ وَاَنَّهٗ هُوَ اَغۡنٰي وَاَقۡنٰي"
        },
        {
            "chapter": 53,
            "verse": 49,
            "text": "ﶫ وَاَنَّهٗ هُوَ رَبُّ الشِّعۡرٰي"
        },
        {
            "chapter": 53,
            "verse": 50,
            "text": "ﶫ وَاَنَّهٗ٘ اَهۡلَكَ عَادَا اِۨلۡاُوۡلٰي"
        },
        {
            "chapter": 53,
            "verse": 51,
            "text": "ﶫ وَثَمُوۡدَا۠ فَمَا٘ اَبۡقٰي"
        },
        {
            "chapter": 53,
            "verse": 52,
            "text": "ﶫ وَقَوۡمَ نُوۡحٍ مِّنۡ قَبۡلُﵧ اِنَّهُمۡ كَانُوۡا هُمۡ اَظۡلَمَ وَاَطۡغٰي"
        },
        {
            "chapter": 53,
            "verse": 53,
            "text": "ﶠ وَالۡمُؤۡتَفِكَةَ اَهۡوٰي"
        },
        {
            "chapter": 53,
            "verse": 54,
            "text": "ﶫ فَغَشّٰىهَا مَا غَشّٰي"
        },
        {
            "chapter": 53,
            "verse": 55,
            "text": "ﶔ فَبِاَيِّ اٰلَآءِ رَبِّكَ تَتَمَارٰي"
        },
        {
            "chapter": 53,
            "verse": 56,
            "text": "هٰذَا نَذِيۡرٌ مِّنَ النُّذُرِ الۡاُوۡلٰي"
        },
        {
            "chapter": 53,
            "verse": 57,
            "text": "اَزِفَتِ الۡاٰزِفَةُ"
        },
        {
            "chapter": 53,
            "verse": 58,
            "text": "ﶔ لَيۡسَ لَهَا مِنۡ دُوۡنِ اللّٰهِ كَاشِفَةٌ"
        },
        {
            "chapter": 53,
            "verse": 59,
            "text": "ﶠ اَفَمِنۡ هٰذَا الۡحَدِيۡثِ تَعۡجَبُوۡنَ"
        },
        {
            "chapter": 53,
            "verse": 60,
            "text": "ﶫ وَتَضۡحَكُوۡنَ وَلَا تَبۡكُوۡنَ"
        },
        {
            "chapter": 53,
            "verse": 61,
            "text": "ﶫ وَاَنۡتُمۡ سٰمِدُوۡنَ"
        },
        {
            "chapter": 53,
            "verse": 62,
            "text": "فَاسۡجُدُوۡا لِلّٰهِ وَاعۡبُدُوۡا"
        }
    ]
}