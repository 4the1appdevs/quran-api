{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "56|17|Tadera asepase ghuraphira karabe cira - kisorera [1]"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "56|18|panapatra, jaga o prasrabana nihsrta surapurna peyala niye [1]."
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "56|19|Se sura pane tadera sirahpira habe na, tara jnanahara'o habe na--- [1]"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "56|20|Ara (ghoraphera karabe) tadera pachandamata phalamula niye [1]"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "56|21|ara tadera insita pakhira gosata niye [1]."
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "56|22|Ara tadera jan'ya thakabe dagara caksubisista hura"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "56|23|yena tara suraksita mukta [1]"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "56|24|tadera kajera puraskarasbarupa."
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "56|25|Sekhane tara sunabe na kona asara ba papabakya [1]"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "56|26|‘salama’ ara ‘salama’ bani chara."
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "56|27|Ara dana dikera dala, kata bhagyabana dana dikera dala"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "56|28|tara thakabe emana udyane, yate ache [1] kamtahina kulagacha [2]"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "56|29|ebam kamdi bhara kala gacha"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "56|30|ara samprasarita chaya [1]"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "56|31|ara sada prabahamana pani"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "56|32|o pracura phalamula"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "56|33|ya sesa habe na o ya nisid'dha'o habena [1]."
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "56|34|Ara samucca sayyasamuha [1]"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "56|35|niscaya amara taderake srsti karechi bisesarupe [1]"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "56|36|atahpara taderake karechi kumari [1]"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "56|37|sohagini [1] o samabayaska [2]"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "56|38|danadikera lokadera jan'ya."
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "56|39|Tadera aneke habe purbabartidera madhya theke"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "56|40|ebam aneke habe parabartidera madhya theke [1]."
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "56|41|Ara bama dikera dala, kata hatabhagya bamadikera dala"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "56|42|tara thakabe atyanta usna bayu o uttapta panite"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "56|43|ara kalobarnera dhumyara chayaya"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "56|44|ya sitala naya, aramadayaka'o naya."
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "56|45|Itopurbe tara to magna chila bhoga-bilase"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "56|46|ara tara abirama lipta chila ghoratara papakaje."
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "56|47|Ara tara balata, ‘mare asthi o matite parinata hale'o ki amaderake uthano habe"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "56|48|ebam amadera pitrpurusara'o?’"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "56|49|Baluna, ‘abasya'i purbabartira o parabartira"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "56|50|saba'ike ekatra kara habe eka nirdharita dinera nirdista samaye."
        }
    ]
}