{
    "chapter": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "80|1|Tini bhrakuncita karalena ebam mukha phiriye nilena [1]"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "80|2|karana tamra kache andha lokati asala."
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "80|3|Ara kise apanake janabe ye, --- se hayata parisud'dha hata"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "80|4|athaba upadesa grahana karata, phale se upadesa tara upakare asata [1]."
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "80|5|Ara ye paroya kare na"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "80|6|apani tara prati manoyoga diyechena."
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "80|7|Athaca se nije parisud'dha na hale apanara kono dayitba ne'i"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "80|8|aparadike ye apanara kache chute elo"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "80|9|ara se sasankacitta"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "80|10|apani tara theke udasina halena"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "80|11|kakhano naya, eta to upadesa bani [1]"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "80|12|kaje'i ye icche karabe se eta smarana rakhabe"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "80|13|eta ache maryada sampanna lipisamuhe"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "80|14|ya unnata, pabitra [1]"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "80|15|lekhaka ba dutadera hate [1]."
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "80|16|(Yara) mahasam'manita o nekakara."
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "80|17|Manusa dhbansa hoka! Se kata akrtajna [1]"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "80|18|tini take kona bastu theke srsti karechena"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "80|19|sukrabindu theke, tini take srsti karena, pare tara parimita bikasa sadhana karena [1]"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "80|20|tarapara tara jan'ya patha sahaja kare dena [1]"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "80|21|erapara tara mrtyu ghatana ebam take kabarastha karena."
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "80|22|Erapara yakhana icche tini take punarjibita karabena [1]."
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "80|23|Kakhano naya, tini take ya adesa karechena, se ekhano ta purna kareni."
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "80|24|Atahpara manusa yena tara khadyera prati laksya kare [1]"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "80|25|niscaya amara pracura bari barsana kari"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "80|26|tarapara amara yaminake yathayathabhabe bidirna kari"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "80|27|atahpara tate amara utpanna kari sasya"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "80|28|angura, saka-sabji"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "80|29|yayatuna, khejuragacha"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "80|30|aneka gachabisista udyana"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "80|31|phala ebam gabadi khadya [1]"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "80|32|egulo tomadera o tomadera catuspada jantudera bhogera jan'ya [1]."
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "80|33|Atahpara yakhana tiksna a'oyaja asabe [1]"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "80|34|sedina manusa paliye yabe tara bha'iyera kacha theke"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "80|35|ebam tara mata, tara pita"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "80|36|tara patni o tara santana theke [1]"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "80|37|sedina tadera pratyekera habe emana gurutara abastha ya take sampurnarupe byasta rakhabe [1]."
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "80|38|Aneka cehara sedina habe ujjbala"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "80|39|sahasya o praphulla"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "80|40|ara aneka cehara sedina habe dhulidhusara"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "80|41|Seguloke acchanna karabe kalima."
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "80|42|Era'i kaphira o papacari."
        }
    ]
}