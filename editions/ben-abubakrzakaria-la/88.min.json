{"chapter":[{"chapter":88,"verse":1,"text":"88|1|Apanara kache ki acchannakarira (kiyamatera) sambada eseche"},{"chapter":88,"verse":2,"text":"88|2|sedina aneka cehara habe abanata [1]"},{"chapter":88,"verse":3,"text":"88|3|klista, klanta [1]"},{"chapter":88,"verse":4,"text":"88|4|tara prabesa karabe jbalanta agune [1]"},{"chapter":88,"verse":5,"text":"88|5|taderake atyanta usna prasrabana theke pana karano habe"},{"chapter":88,"verse":6,"text":"88|6|tadera jan'ya khadya thakabe na kamtayukta gulma chara [1]"},{"chapter":88,"verse":7,"text":"88|7|ya taderake pusta karabe na ebam tadera ksudha nibrtta karabe na."},{"chapter":88,"verse":8,"text":"88|8|Aneka mukhamandala sedina habe anandojjbala"},{"chapter":88,"verse":9,"text":"88|9|nijedera kajera saphalye paritrpta [1]"},{"chapter":88,"verse":10,"text":"88|10|su'ucca jannate"},{"chapter":88,"verse":11,"text":"88|11|sekhane tara asara bakya sunabe na [1]"},{"chapter":88,"verse":12,"text":"88|12|sekhane thakabe bahamana prasrabana"},{"chapter":88,"verse":13,"text":"88|13|sekhane thakabe unnata [1] sayyasamuha"},{"chapter":88,"verse":14,"text":"88|14|ara prastuta thakabe panapatra"},{"chapter":88,"verse":15,"text":"88|15|sari sari upadhana"},{"chapter":88,"verse":16,"text":"88|16|ebam bichana galica"},{"chapter":88,"verse":17,"text":"88|17|tabe ki tara takiye dekhe na utera dike, kibhabe ta srsti kara hayeche"},{"chapter":88,"verse":18,"text":"88|18|ebam asamanera dike, kibhabe ta urdhbe sthapana kara hayeche"},{"chapter":88,"verse":19,"text":"88|19|ebam parbatamalara dike, kibhabe ta pratisthita kara hayeche"},{"chapter":88,"verse":20,"text":"88|20|ebam bhutalera dike, kibhabe ta bistrta kara hayeche [1]"},{"chapter":88,"verse":21,"text":"88|21|ata'eba apani upadesa dina; apani to kebala ekajana upadesa data [1]"},{"chapter":88,"verse":22,"text":"88|22|apani tadera upara sakti prayogakari nana."},{"chapter":88,"verse":23,"text":"88|23|Tabe ke'u mukha phiriye nile o kuphari karale"},{"chapter":88,"verse":24,"text":"88|24|allah taderake debena mahasasti [1]."},{"chapter":88,"verse":25,"text":"88|25|Niscaya tadera phire asa amadera'i kache"},{"chapter":88,"verse":26,"text":"88|26|tarapara tadera hiseba-nikesa amadera'i kaja."}]}