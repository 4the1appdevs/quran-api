        {
            "chapter": 94,
            "verse": 1,
            "text": "94|1|Amara ki apanara baksa apanara kalyane prasasta kare de'ini [1]"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "94|2|Ara amara apasarana karechi apanara bhara"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "94|3|ya apanara pitha bhenge dicchila [1]."
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "94|4|Ara amara apanara (maryada brd'dhira) jan'ya apanara smaranake samunnata karechi [1]."
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "94|5|Sutaram kastera sathe'i to sbasti ache"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "94|6|niscaya kastera sathe'i sbasti ache [1]."
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "94|7|Ata'eba apani yakhana'i abasara pana takhana'i kathora ibadate rata hona"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "94|8|ara apanara rabera prati gabhira manoyogi hona [1]."
        }
