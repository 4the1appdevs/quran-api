{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "100|1|Sapatha urdhbasbase dhabamana asbarajira [1]"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "100|2|atahpara yara ksurera aghate agni-sphulinga bicchurita kare [1]"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "100|3|atahpara yara abhiyana kare prabhatakale [1]"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "100|4|phale tara ta dbara dhuli utksipta kare [1]"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "100|5|atahpara ta dbara satru dalera abhyantare dhuke pare [1]."
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "100|6|Niscaya manusa tara rabera prati bara'i akrtajna [1]"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "100|7|ara niscaya se e bisaye saksi [1]"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "100|8|ara niscaya se dhana-sampadera asaktite prabala [1]."
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "100|9|Tabe ki se jane na yakhana kabare ya ache ta ut'thita habe"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "100|10|ara antare ya ache ta prakasa kara habe [1]"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "100|11|niscaya tadera raba sedina tadera byapare sabisesa abahita [1]."
        }
    ]
}