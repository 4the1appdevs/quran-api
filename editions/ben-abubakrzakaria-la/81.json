{
    "chapter": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "81|1|Suryake yakhana nisprabha kara habe [1]"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "81|2|ara yakhana naksatraraji khase parabe [1]"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "81|3|ara parbatasamuhake yakhana calamana kara habe [1]"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "81|4|ara yakhana purnagarbha madi uta upeksita habe [1]"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "81|5|ara yakhana ban'ya pasugulo ekatra kara habe"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "81|6|ara yakhana sagarake agni-uttala kara habe [1]"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "81|7|ara yakhana atmaguloke samagotriyadera sathe miliye deya habe [1]"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "81|8|ara yakhana jibanta samadhistha kan'yake [1] jijnesa kara habe"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "81|9|ki aparadhe take hatya kara hayechila [1]"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "81|10|ara yakhana amalanamagulo unmocita kara habe"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "81|11|ara yakhana asamanera abarana apasarita kara habe [1]"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "81|12|ara yakhana jahannamake bhisanabhabe prajjbalita kara habe"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "81|13|ara yakhana jannata nikatabarti kara habe"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "81|14|takhana pratyeka byakti'i janabe se ki upasthita kareche [1]."
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "81|15|Sutaram ami sapatha karachi pascadapasaranakari naksatrera"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "81|16|ya calamana, apasryamana"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "81|17|sapatha ratera yakhana ta sesa haya [1]"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "81|18|sapatha prabhatera yakhana tara abirbhaba haya"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "81|19|niscaya'i e kura'ana sam'manita rasulera anita bani [1]"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "81|20|ye samarthyasali, arasera malikera kache maryada sampanna"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "81|21|se man'ya sekhane, bisbasabhajana [1]."
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "81|22|Ara tomadera sathi unmada nana [1]"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "81|23|tini to take spasta digante dekhechena [1]"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "81|24|tini gayebi bisaya samparke krpana naya [1]."
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "81|25|Ara eta kono abhisapta sayatanera bakya naya."
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "81|26|Kaje'i tomara kothaya yaccha"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "81|27|e to sudhu srstikulera jan'ya upadesa"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "81|28|tomadera madhye ye sarala pathe calate caya, tara jan'ya [1]."
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "81|29|Ara tomara icche karate para na, yadi na srstikulera raba allah icche karena [1]."
        }
    ]
}