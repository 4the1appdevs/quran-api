{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "104|1|Durbhoga pratyekera, ye pichane o samane lokera ninda kare [1]"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "104|2|ye sampada jamaya o ta bara bara ganana kare [1]"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "104|3|se dharana kare ye, tara artha take amara kare rakhabe [1]"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "104|4|kakhana'o na, se abasya'i niksipta habe [1] hutamaya [2]"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "104|5|ara apanake kise janabe hutama ki"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "104|6|eta allahara prajbalita aguna [1]"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "104|7|ya hrdayake graasa karabe [1]"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "104|8|niscaya eta taderake paribestana kare rakhabe [1]"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "104|9|dirghayita stambhasamuhe [1]."
        }
    ]
}