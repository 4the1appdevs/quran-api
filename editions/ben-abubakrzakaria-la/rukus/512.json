{
    "rukus": [
        {
            "chapter": 75,
            "verse": 1,
            "text": "75|1|Ami sapatha karachi kiyamatera dinera [1]"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "75|2|ami ara'o sapatha karachi bhartsanakari atmara [1]."
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "75|3|Manusa ki mane kare ye, amara kakhano'i tara asthisamuha ekatra karate paraba na"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "75|4|abasya'i hyam, amara tara angulera aga paryanta punarbin'yasta karate saksama [1]."
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "75|5|Baram manusa tara bhabisyate'o papacara karate caya [1]."
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "75|6|Se prasna kare, ‘kakhana kiyamatera dina asabe?’"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "75|7|Yakhana cokha sthira haye yabe [1]"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "75|8|ebam camda haye parabe kiranahina [1]"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "75|9|ara yakhana surya o camdake ekatra kara habe [1] –"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "75|10|sedina manusa balabe, ‘aja palanora sthana kothaya?’"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "75|11|Kakhano'i naya, kono asrayasthala ne'i."
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "75|12|Sedina thami habe apanara rabera'i kache."
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "75|13|Sedina manusake abahita kara habe se ki age pathiyeche o ki pichane rekhe geche [1]."
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "75|14|Baram manusa nijera samparke samyaka abagata [1]"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "75|15|Yadi'o se nana ajuhatera abatarana kare."
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "75|16|Taratari ohi ayatta karara jan'ya apani ta niye apanara jihbake druta sancalana karabena na."
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "75|17|Niscaya era sanraksana o patha karanora dayitba amadera'i."
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "75|18|Kaje'i yakhana amara ta patha kari apani se pathera anusarana karuna"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "75|19|tarapara tara barnanara dayitba niscitabhabe amadera'i [1]."
        },
        {
            "chapter": 75,
            "verse": 20,
            "text": "75|20|Kakhano na, baram tomara duniyara jibanake bhalabasa"
        },
        {
            "chapter": 75,
            "verse": 21,
            "text": "75|21|ara tomara akheratake upeksa kara [1]."
        },
        {
            "chapter": 75,
            "verse": 22,
            "text": "75|22|Sedina kono kono mukhamandala ujjbala habe"
        },
        {
            "chapter": 75,
            "verse": 23,
            "text": "75|23|tara tadera rabera dike takiye thakabe [1]."
        },
        {
            "chapter": 75,
            "verse": 24,
            "text": "75|24|Ara kono kono mukhamandala haye parabe bibarna"
        },
        {
            "chapter": 75,
            "verse": 25,
            "text": "75|25|asanka karabe ye, eka dhbansakari biparyaya tadera upara apatita habe."
        },
        {
            "chapter": 75,
            "verse": 26,
            "text": "75|26|Abasya'i [1], yakhana praana kanthagata habe"
        },
        {
            "chapter": 75,
            "verse": 27,
            "text": "75|27|ebam bala habe, ‘ke take raksa karabe?’"
        },
        {
            "chapter": 75,
            "verse": 28,
            "text": "75|28|Takhana tara pratyaya habe ye, eta bidayaksana."
        },
        {
            "chapter": 75,
            "verse": 29,
            "text": "75|29|Ara payera gochara sange payera gocha jariye yabe [1]."
        },
        {
            "chapter": 75,
            "verse": 30,
            "text": "75|30|Sedina apanara rabera kache'i sakalake hamkiye neya habe."
        }
    ]
}