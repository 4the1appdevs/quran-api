{
    "rukus": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "78|1|tara eke an'yera kache ki bisaye jijnasabada karache"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "78|2|mahasambadatira bisaye [1]"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "78|3|ye bisaye tara matanaikya karache [1]"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "78|4|kakhano na [1], tara acire'i janate parabe"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "78|5|tarapara bali kakhano na, tara acire'i janate parabe."
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "78|6|Amara ki karini yaminake sayya"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "78|7|ara parbatasamuhake pereka"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "78|8|ara amara srsti karechi tomaderake joraya joraya"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "78|9|ara tomadera ghumake karechi bisraama [1]"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "78|10|ara karechi ratake abarana"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "78|11|ara karechi dinake jibika aharanera samaya"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "78|12|ara amara nirmana karechi tomadera upare sudrrha sata akasa [1]"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "78|13|ara amara srsti karechi projjbala dipa [1]."
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "78|14|Ara amara barsana karechi meghamala hate pracura bari [1]"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "78|15|yate ta dbara amara utpanna kari sasya, udbhida"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "78|16|o ghana sannibista udyana."
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "78|17|Niscaya nirdharita ache bicara dina [1]"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "78|18|sedina singaya phumka deya habe takhana tomara dale dale asabe [1]"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "78|19|ara akasa unmukta kara habe, phale ta habe bahu dbarabisista [1]."
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "78|20|Ara calamana kara habe parbatasamuhake, phale segulo haye yabe maricika [1]"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "78|21|niscaya jahannama ot pete apeksamana"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "78|22|simalanghanakaridera jan'ya pratyabartanasthala."
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "78|23|Sekhane tara yuga yuga dhare abasthana karabe [1]."
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "78|24|Sekhane tara asbadana karabe na sitalata, na kono paniya"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "78|25|phutanta pani o pumja chara [1]"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "78|26|Eta'i upayukta pratiphala [1]."
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "78|27|Niscaya tara kakhano hisebera asa karata na"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "78|28|ara tara amadera nidarsanabalite kathorabhabe mithyaropa karechila [1]."
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "78|29|Ara sabakichu'i amara sanraksana karechi likhitabhabe."
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "78|30|Atahpara tomara asbada grahana kara, amara to tomadera sasti'i sudhu brd'dhi karaba."
        }
    ]
}