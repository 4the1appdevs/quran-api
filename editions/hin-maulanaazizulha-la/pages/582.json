{
    "pages": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "78|1|ve aapas mein kis vishay mein prashn kar rahe hain"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "78|2|bahut badee soochana ke vishay mein."
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "78|3|jisamen matabhed kar rahe hain."
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "78|4|nishchay ve jaan lenge."
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "78|5|phir nishchay ve jaan lenge."
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "78|6|kya hamane dharatee ko paalana nahin banaaya"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "78|7|aur parvaton ko mekh"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "78|8|tatha tumhen jode-jode paida kiya."
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "78|9|tatha tumhaaree nidra ko sthirata (aaraam) banaaya."
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "78|10|aur raat ko vastr banaaya."
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "78|11|aur din ko kamaane ke lie banaaya."
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "78|12|tatha hamane tumhaare oopar saat drdh aakaash banaaye."
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "78|13|aur ek damakata deep (soory) banaaya."
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "78|14|aur baadalon se moosalaadhaar varsha kee."
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "78|15|taaki usase ann aur vanaspati upajaayen."
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "78|16|aur ghane-ghane baag."
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "78|17|nishchay nirnay (faisale) ka din nishchit hai."
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "78|18|jis din soor mein phoonka jaayega. phir tum dalon hee dalon mein chale aaoge."
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "78|19|aur aakaash khol diya jaayega, to usamen dvaar hee dvaar ho jaayenge."
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "78|20|aur parvat chala diye jaayenge, to ve marichika ban jaayenge."
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "78|21|vaastav mein, narak ghaat mein hai."
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "78|22|jo duraachaariyon ka sthaan hai."
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "78|23|jisamen ve asankhy varshon tak rahenge."
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "78|24|usamen thanadee tatha pey (peene kee cheez) nahin chakhenge."
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "78|25|sivaaye garm paanee aur peep rakt ke."
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "78|26|ye poora-poora pratiphal hai."
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "78|27|nihsandeh ve haî€¼isaab kee aasha nahin rakhate the."
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "78|28|tatha ve hamaaree aayaton ko jhuthalaate the."
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "78|29|aur hamane sab vishay likhakar surakshit kar liye hain."
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "78|30|to chakho, ham tumhaaree yaatana adhik hee karate rahenge."
        }
    ]
}