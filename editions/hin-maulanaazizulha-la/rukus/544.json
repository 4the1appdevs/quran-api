{
    "rukus": [
        {
            "chapter": 102,
            "verse": 1,
            "text": "102|1|tumhen adhik (dhan) ke lobh ne magan kar diya."
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "102|2|yahaan tak ki tum qabristaan ja pahunche."
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "102|3|nishchay tumhen gyaan ho jaayega."
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "102|4|phir nishchay hee tumhen gyaan ho jaayega."
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "102|5|vaastav mein, yadi tumhen vishvaas hota (to aisa na karate)."
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "102|6|tum narak ko avashy dekhoge."
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "102|7|phir use vishvaas kee aankh se dekhoge."
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "102|8|phir us din tumase sukh sampada ke vishay mein avashy poochh gachh hogee."
        }
    ]
}