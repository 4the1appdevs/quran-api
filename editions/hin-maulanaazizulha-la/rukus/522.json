{
    "rukus": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "80|1|(nabee ne) tyoree chadhaee tatha munh pher liya."
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "80|2|is kaaran ki usake paas ek andha aaya."
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "80|3|aur tum kya jaano shaayad vah pavitrata praapt kare."
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "80|4|ya naseehî€¼ta grahan kare, jo use laabh detee."
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "80|5|parantu, jo vimukh (nishchint) hai."
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "80|6|tum unakee or dhyaan de rahe ho."
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "80|7|jabaki tumapar koee dosh nahin, yadi vah pavitrata grahan na kare."
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "80|8|tatha jo tumhaare paas daudata aaya."
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "80|9|aur vah dar bhee raha hai."
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "80|10|tum usakee or dhyaan nahin dete."
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "80|11|kadaapi ye na karo, ye (arthaat quraan) ek smrti (yaad dahaanee) hai."
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "80|12|atah, jo chaahe smaran (yaad) kare."
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "80|13|maananeey shaastron mein hai."
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "80|14|jo oonche tatha pavitr hain."
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "80|15|aise lekhakon (farishton) ke haathon mein hai."
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "80|16|jo sammaanit aur aadaraneey hain."
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "80|17|insaan maara jaaye, vah kitana krtaghn (naashukra) hai."
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "80|18|use kis vastu se (allaah) ne paida kiya"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "80|19|use veery se paida kiya, phir usaka bhaagy banaaya."
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "80|20|phir usake lie maarg saral kiya."
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "80|21|phir maut dee, phir samaadhi mein daal diya."
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "80|22|phir jab chaahega, use jeevit kar lega."
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "80|23|vastutah, usane usakee aagya ka paalan nahin kiya."
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "80|24|insaan apane bhojan kee or dhyaan de."
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "80|25|hamane moosalaadhaar varsha kee."
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "80|26|phir dharatee ko cheera phaada."
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "80|27|phir usase ann ugaaya."
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "80|28|tatha angoor aur tarakaariyaan."
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "80|29|tatha zaitoon evan khajoor."
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "80|30|tatha ghane baag."
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "80|31|evan phal tatha vanaspatiyaan."
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "80|32|tumhaare tatha tumhaare pashuon ke lie."
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "80|33|to jab kaan phaad dene vaalee (pralay) aa jaayegee."
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "80|34|us din insaan apane bhaee se bhaagega."
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "80|35|tatha apane maata aur pita se."
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "80|36|evan apanee patnee tatha apane putron se."
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "80|37|pratyek vyakti ko us din apanee padee hogee."
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "80|38|us din bahut se chehare ujjval honge."
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "80|39|hansate evan prasann honge."
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "80|40|tatha bahut-se cheharon par dhool padee hogee."
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "80|41|unapar kaalima chhaee hogee."
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "80|42|vahee kaafir aur kukarmee log hain."
        }
    ]
}