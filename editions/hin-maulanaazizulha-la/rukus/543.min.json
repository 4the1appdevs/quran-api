{"rukus":[{"chapter":101,"verse":1,"text":"101|1|vah khadakhada dene vaalee."},{"chapter":101,"verse":2,"text":"101|2|kya hai vah khadakha dene vaalee"},{"chapter":101,"verse":3,"text":"101|3|aur tum kya jaano ki vah khadakhada dene vaalee kya hai"},{"chapter":101,"verse":4,"text":"101|4|jis din log, bikhare patingon ke samaan (vyaakool) honge."},{"chapter":101,"verse":5,"text":"101|5|aur parvat, dhunee huee oon ke samaan udenge."},{"chapter":101,"verse":6,"text":"101|6|to jisake palade bhaaree hue"},{"chapter":101,"verse":7,"text":"101|7|to vah manachaahe sukh mein hoga."},{"chapter":101,"verse":8,"text":"101|8|tatha jisake palad halke hue"},{"chapter":101,"verse":9,"text":"101|9|to usaka sthaan haaviya hai."},{"chapter":101,"verse":10,"text":"101|10|aur tum kya jaano ki vah (haaviya) kya hai"},{"chapter":101,"verse":11,"text":"101|11|vah dahaktee aag hai."}]}