{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "114|1|(he nabee!) kaho ki main insaanon ke paalanahaar kee sharan mein aata hoon."
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "114|2|jo saare insaanon ka svaamee hai."
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "114|3|jo saare insaanon ka poojy hai."
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "114|4|bhram daalane vaale aur chhup jaane vaale (raakshas) kee buraee se."
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "114|5|jo logon ke dilon mein bhram daalata rahata hai."
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "114|6|jo jinnon mein se hai aur manushyon mein se bhee."
        }
    ]
}