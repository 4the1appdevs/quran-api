{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "107|1|(napiye!) Kuli kotukkum nalaip poyyakkupavanai nir (kavanittup) parttira"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "107|2|(Ataravarra) anataikalai veruttupavan avantan"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "107|3|avan elaikalukku (unavalikkatatutan) unavalikkumpati (pirarait) tuntuvatumillai"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "107|4|(kavanamarra) tolukaiyalikalukkum ketutan"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "107|5|avarkal tankal tolukaikalai vittum marantavarkalaka irukkirarkal"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "107|6|(melum,) avarkal (tolutapotilum makkalukkuk) kanpikkave tolukirarkal"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "107|7|(melum, uci ponra) arpap porulaiyum (iraval kotukkatu) tatuttuk kolkirarkal"
        }
    ]
}