        {
            "chapter": 101,
            "verse": 1,
            "text": "101|1|(maranittavarkalaiyum) titukkitac ceyyum campavam"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "101|2|(attakaiya) titukkitac ceyyum campavam enna"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "101|3|(napiye!) Titukkitac ceyyum campavam innatenru nir'arivira"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "101|4|annalil manitarkal citarikkitakkum icalkalaip pol akivituvarkal"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "101|5|kottappatta pancukalaippol malaikal akivitum"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "101|6|evarutaiya (nanmaiyin) etai kanattato"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "101|7|Avar tiruptiyulla valkkaiyil (cukamaka) valntiruppar"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "101|8|evanutaiya (nanmaiyin) etai ilecaki(p pava etai kanattu) vittato"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "101|9|avan tankumitam haviyatan"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "101|10|anta haviya innatenru (napiye!) Nir arivira"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "101|11|(atutan) kanintu kontirukkum (naraka) neruppakum"
        }
