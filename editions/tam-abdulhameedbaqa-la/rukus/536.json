{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "94|1|(napiye!) Umatu ullattai nam umakku virivakkavillaiya"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "94|2|umatu cumaiyaiyum um'maivittum nam irakkivittom"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "94|3|atu, umatu ituppaiye murittuk kontiruntatu"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "94|4|umatu kirttiyaiyum nam uyarttinom"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "94|5|niccayamaka ciramattutan caukariyam irukkiratu"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "94|6|meyyakave ciramattutan caukariyam irukkiratu"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "94|7|akave, (markkap piraccarattiliruntu) nir vitupattatum, (iraivanai vanankuvatarku) cirattai etuppiraka"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "94|8|melum, (tunpattilum inpattilum) umatu iraivanaiye nir nokki nirpiraka"
        }
    ]
}