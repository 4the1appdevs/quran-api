{
    "rukus": [
        {
            "chapter": 75,
            "verse": 31,
            "text": "75|31|(avano allahvutaiya vacanankalai) unmaiyakkavumillai; tolavumillai"
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "75|32|ayinum (avan avarraip) poyyakki vaittu(t tolatum) vilakikkontan"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "75|33|pinnar, karvamkontu tan kutumpattutan (tan vittirkuc) cenru vittan"
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "75|34|(manitane!) Unakkuk ketutan; melum ketutan"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "75|35|pinnar, unakkuk ketutan! Melum ketutan (unakku)"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "75|36|(oru kelviyum tam'mitam ketkatu) cum'ma vittu vitappatuvom enru manitan ennik kontana"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "75|37|Avan (karppattil) celuttappatta or intiriyat tuliyaka irukka villaiya"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "75|38|(intiriyamaka irunta) pinnar, avan karuvaka marinan (avanai) allahtan pataittu mulumaiyana manitanaka akkivaittan"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "75|39|an, pen jotikalaiyum atiliruntu urpatti ceykiran"
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "75|40|(ivarraiyellam ceyta) avan maranittavarkalai uyirppikka arralutaiyavanaka illaiya"
        }
    ]
}