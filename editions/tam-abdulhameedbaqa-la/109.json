{
    "chapter": [
        {
            "chapter": 109,
            "verse": 1,
            "text": "109|1|(napiye! Nirakarikkum makkalai nokki) kuruviraka: Nirakarippavarkale"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "109|2|ninkal vanankupavarrai nan vanankamatten"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "109|3|nan vanankupavanai ninkal vanankavillai"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "109|4|(avvare) iniyum ninkal vanankupavarrai nan vanankupavan allan"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "109|5|nan vanankupavanai ini ninkalum vanankupavarkal allar"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "109|6|unkal (ceyalukkuriya) kuli unkalukkum; en (ceyalukkuriya) kuli enakkum (kitaikkum)"
        }
    ]
}