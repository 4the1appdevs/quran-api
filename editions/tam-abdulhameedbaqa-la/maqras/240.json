{
    "maqras": [
        {
            "chapter": 100,
            "verse": 9,
            "text": "100|9|avanaik kapriliruntu eluppappatum camayattil, avanutaiya ullattil ullavaiyellam ariyappattuvitum enpatai avan arintu kollavillaiya"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "100|10|avanaik kapriliruntu eluppappatum camayattil, avanutaiya ullattil ullavaiyellam ariyappattuvitum enpatai avan arintu kollavillaiya"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "100|11|niccayamaka manitarkalutaiya iraivan (tiyavan yar? Nallavan yar? Enru) avarkalai annalil nanku arintu kolvan"
        },
        {
            "chapter": 101,
            "verse": 1,
            "text": "101|1|(maranittavarkalaiyum) titukkitac ceyyum campavam"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "101|2|(attakaiya) titukkitac ceyyum campavam enna"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "101|3|(napiye!) Titukkitac ceyyum campavam innatenru nir'arivira"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "101|4|annalil manitarkal citarikkitakkum icalkalaip pol akivituvarkal"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "101|5|kottappatta pancukalaippol malaikal akivitum"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "101|6|evarutaiya (nanmaiyin) etai kanattato"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "101|7|Avar tiruptiyulla valkkaiyil (cukamaka) valntiruppar"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "101|8|evanutaiya (nanmaiyin) etai ilecaki(p pava etai kanattu) vittato"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "101|9|avan tankumitam haviyatan"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "101|10|anta haviya innatenru (napiye!) Nir arivira"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "101|11|(atutan) kanintu kontirukkum (naraka) neruppakum"
        },
        {
            "chapter": 102,
            "verse": 1,
            "text": "102|1|ninkal putaikkulikalaic cantikkum varai (porulai) atikappatuttik kollum peracai (allahvai vittum) unkalaip parakkakki vittatu (tiruppivittatu)"
        },
        {
            "chapter": 102,
            "verse": 2,
            "text": "102|2|ninkal putaikkulikalaic cantikkum varai (porulai) atikappatuttik kollum peracai (allahvai vittum) unkalaip parakkakki vittatu (tiruppivittatu)"
        },
        {
            "chapter": 102,
            "verse": 3,
            "text": "102|3|ninkal ninaippatu ponralla. (Avai enna enpatai) ninkal nankarintu kolvirkal"
        },
        {
            "chapter": 102,
            "verse": 4,
            "text": "102|4|pinnar, ninkal ninaippatu ponralla. (Avarrin palankalaiyum) ninkal nankarintu kolvirkal"
        },
        {
            "chapter": 102,
            "verse": 5,
            "text": "102|5|ninkal ninaippatu ponralla. (Atan palanaic) cantekamara urutiyaka ninkal arivirkalayin"
        },
        {
            "chapter": 102,
            "verse": 6,
            "text": "102|6|niccayamaka narakattaiye ninkal (unkal kanmun) kanpirkal"
        },
        {
            "chapter": 102,
            "verse": 7,
            "text": "102|7|piraku, cantekamara meyyakave atai ninkal unkal kankalal kantukolvirkal"
        },
        {
            "chapter": 102,
            "verse": 8,
            "text": "102|8|(unkalukku iraivan purinta) arulai (ninkal evvaliyil celavu ceytirkal enpatai)p parriyum, pinnar annalil niccayamaka ninkal ketkappatuvirkal"
        },
        {
            "chapter": 103,
            "verse": 1,
            "text": "103|1|kalattin mitu cattiyamaka"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "103|2|(tan ayulai vinil celavu ceytu) niccayamaka manitan nastamataintu vittan"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "103|3|ayinum, evarkal nampikkai kontu, narceyalkalaiyum ceytu, cattiyattai oruvarukkoruvar upatecam ceytu (pavankalai vituvatilum nanmaikalaic ceyvatilum erpatum ciramankalaic) cakittuk kollumarum oruvarukkoruvar upatecam ceytu vantarkalo, avarkalait tavira (ivarkal nastamataiyavillai)"
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "104|1|kurai kurip puram pecit tiripavarkalukkellam ketutan"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "104|2|Porulaic cekarittu (nalla valiyil celavu ceyyatu) atai ennikkonte iruppavan"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "104|3|tan celvam tannai enrenrume (ulakattil) nilaittirukkac ceyyumenrum ennik kontan"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "104|4|avvaralla! (Niccayamaka avan maranippan. Pinnar) niccayamaka avan ‘hutama' ennum narakattil eriyappatuvan"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "104|5|(napiye!) ‘Hutama' (enral) ennavenru nir arivira"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "104|6|(atutan) allahvutaiya (kattalaiyal) erikkappatta neruppu"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "104|7|(atu utalkalil pattavutan) ullankalil payntuvitum"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "104|8|(narakattilulla) uyarnta kampankalil avarkal kattappattu avarkal mitu atan vayilkalum mutappattuvitum"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "104|9|(narakattilulla) uyarnta kampankalil avarkal kattappattu avarkal mitu atan vayilkalum mutappattuvitum"
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "105|1|(napiye!) Yanaip pataiyinarai umatu iraivan evvaru (aliyac) ceytan enpatai nir (kavanittup) parkkavillaiya"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "105|2|avarkalutaiya culcciyai avan vinakkivita villaiya"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "105|3|avarkal mitu paravaikalai kuttankuttamaka anuppivaittan"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "105|4|(kettiyana) cutappatta ciriya karkalai avai avarkal mitu erintana"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "105|5|atanal, avan avarkalai(p paravaikalal) kottit tinnappatta katirkalaippol akki (alittu) vittan"
        },
        {
            "chapter": 106,
            "verse": 1,
            "text": "106|1|kuraisikalukku (pirayanattin mitu) viruppamuntakki"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "106|2|kulirkala payanattaiyum, kotaikala payanattaiyum avarkal virumpik kaikollumpati ceytatarkaka"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "106|3|(atarku nanri celuttuvatarkaka) ivvittin iraivanaiye avarkal vanankavum"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "106|4|avantan (avarkal ulavatittup payiritamale inta varttaka pirayanattin mulam) avarkalutaiya pacikku unavalittu varukiran. (Kolai, kalavu mutaliya kotiya) payattiliruntum avarkalukku apayamalittan"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "107|1|(napiye!) Kuli kotukkum nalaip poyyakkupavanai nir (kavanittup) parttira"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "107|2|(Ataravarra) anataikalai veruttupavan avantan"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "107|3|avan elaikalukku (unavalikkatatutan) unavalikkumpati (pirarait) tuntuvatumillai"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "107|4|(kavanamarra) tolukaiyalikalukkum ketutan"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "107|5|avarkal tankal tolukaikalai vittum marantavarkalaka irukkirarkal"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "107|6|(melum,) avarkal (tolutapotilum makkalukkuk) kanpikkave tolukirarkal"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "107|7|(melum, uci ponra) arpap porulaiyum (iraval kotukkatu) tatuttuk kolkirarkal"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "108|1|(napiye!) Niccayamaka nam umakku ‘kavsar' ennum nir tatakattai kotuttirukkirom"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "108|2|akave, (avarrukku nanri celuttuvatarkaka) nir umatu iraivanait tolutu, kurpani (pali) kotuttu varuviraka"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "108|3|niccayamaka umatu etiritan cantatiyarravan"
        },
        {
            "chapter": 109,
            "verse": 1,
            "text": "109|1|(napiye! Nirakarikkum makkalai nokki) kuruviraka: Nirakarippavarkale"
        },
        {
            "chapter": 109,
            "verse": 2,
            "text": "109|2|ninkal vanankupavarrai nan vanankamatten"
        },
        {
            "chapter": 109,
            "verse": 3,
            "text": "109|3|nan vanankupavanai ninkal vanankavillai"
        },
        {
            "chapter": 109,
            "verse": 4,
            "text": "109|4|(avvare) iniyum ninkal vanankupavarrai nan vanankupavan allan"
        },
        {
            "chapter": 109,
            "verse": 5,
            "text": "109|5|nan vanankupavanai ini ninkalum vanankupavarkal allar"
        },
        {
            "chapter": 109,
            "verse": 6,
            "text": "109|6|unkal (ceyalukkuriya) kuli unkalukkum; en (ceyalukkuriya) kuli enakkum (kitaikkum)"
        },
        {
            "chapter": 110,
            "verse": 1,
            "text": "110|1|(napiye! Umakku) allahvutaiya utaviyum, (makkavin) verriyum kitaittu"
        },
        {
            "chapter": 110,
            "verse": 2,
            "text": "110|2|allahvutaiya markkattil makkal kuttam kuttamaka nulaivataiyum nir kantal"
        },
        {
            "chapter": 110,
            "verse": 3,
            "text": "110|3|(atarku nanri celuttuvatarkaka) umatu iraivanaip pukalntu tuti ceytu, avanutaiya (arulaiyum) mannippaiyum koruviraka! Niccayamaka avan (pirarttanaikalai ankikarittu) mannippuk korutalai(yum) ankikarippavanaka irukkiran"
        },
        {
            "chapter": 111,
            "verse": 1,
            "text": "111|1|aliyattum apulahapin iru karankal; avanume aliyattum"
        },
        {
            "chapter": 111,
            "verse": 2,
            "text": "111|2|avanutaiya porulum, avan cekarittu vaittiruppavaikalum avanukku oru payanumalikkatu"
        },
        {
            "chapter": 111,
            "verse": 3,
            "text": "111|3|veku viraivil avan koluntuvitteriyum neruppil nulaivan"
        },
        {
            "chapter": 111,
            "verse": 4,
            "text": "111|4|Viraku cumakkum avanutaiya manaiviyo"
        },
        {
            "chapter": 111,
            "verse": 5,
            "text": "111|5|avalutaiya kaluttil murukkeriya iccankayirutan. (Atanal curukkitappattu avalum alintu vituval)"
        },
        {
            "chapter": 112,
            "verse": 1,
            "text": "112|1|(napiye! Manitarkalai nokki) kuruviraka: Allah oruvantan"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "112|2|(anta) allah (evarutaiya) tevaiyumarravan. (Anaittum avan arulaiye etirparttirukkinrana)"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "112|3|avan (evaraiyum) peravumillai; (evaralum) perappatavumillai. (Akave, avanukkut takappanumillai cantatiyumillai)"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "112|4|(tavira) avanukku oppakavum (nikarakavum) onrumillai"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "113|1|(napiye! Nir pirarttanai ceytu) kuruviraka atikalaiyin iraivanitam, nan patukappaik korukiren"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "113|2|avan pataittirukkum porulkalin tinkai vittum"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "113|3|(anaittaiyum) maraittuk kollum irunta irulin tinkai vittum"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "113|4|muticcup pottu utum (cutukkara) penkalin tinkaivittum"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "113|5|poramaikkaran poramaippatumpotu avanin tinkai vittum (patukakkak korukiren)"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "114|1|(napiye! Pirarttanai ceytu) kuruviraka: Manitarkalin iraivanitam nan patukappaik korukiren"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "114|2|(avantan) manitarkalin (unmaiyana) aracan"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "114|3|(avane) manitarkalin vanakkattirkuriya unmaiyana iraivan"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "114|4|manitarkalutaiya ullankalil (vinana) cantekankalai untu pannivittu, maraintu kollum visamikalin tinkai vittum (patukakkak korukiren)"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "114|5|manitarkalutaiya ullankalil (vinana) cantekankalai untu pannivittu, maraintu kollum visamikalin tinkai vittum (patukakkak korukiren)"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "114|6|(attakaiya visamikal) jinkalilum irukkinranar; manitarkalilum irukkinranar"
        }
    ]
}