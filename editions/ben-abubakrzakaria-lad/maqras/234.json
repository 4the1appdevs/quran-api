{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "80|1|Tini bhrakuñcita karalēna ēbaṁ mukha phiriẏē nilēna [1]"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "80|2|kāraṇa tām̐ra kāchē andha lōkaṭi āsala."
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "80|3|Āra kisē āpanākē jānābē yē, --- sē haẏata pariśud'dha hata"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "80|4|athabā upadēśa grahaṇa karata, phalē sē upadēśa tāra upakārē āsata [1]."
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "80|5|Āra yē parōẏā karē nā"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "80|6|āpani tāra prati manōyōga diẏēchēna."
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "80|7|Athaca sē nijē pariśud'dha nā halē āpanāra kōnō dāẏitba nē'i"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "80|8|aparadikē yē āpanāra kāchē chuṭē ēlō"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "80|9|āra sē saśaṅkacitta"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "80|10|āpani tāra thēkē udāsīna halēna"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "80|11|kakhanō naẏa, ēṭā tō upadēśa bāṇī [1]"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "80|12|kājē'i yē icchē karabē sē ēṭā smaraṇa rākhabē"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "80|13|ēṭā āchē maryādā sampanna lipisamūhē"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "80|14|yā unnata, pabitra [1]"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "80|15|lēkhaka bā dūtadēra hātē [1]."
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "80|16|(Yārā) mahāsam'mānita ō nēkakāra."
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "80|17|Mānuṣa dhbansa hōka! Sē kata akr̥tajña [1]"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "80|18|tini tākē kōna bastu thēkē sr̥ṣṭi karēchēna"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "80|19|śukrabindu thēkē, tini tākē sr̥ṣṭi karēna, parē tāra parimita bikāśa sādhana karēna [1]"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "80|20|tārapara tāra jan'ya patha sahaja karē dēna [1]"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "80|21|ērapara tāra mr̥tyu ghaṭāna ēbaṁ tākē kabarastha karēna."
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "80|22|Ērapara yakhana icchē tini tākē punarjībita karabēna [1]."
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "80|23|Kakhanō naẏa, tini tākē yā ādēśa karēchēna, sē ēkhanō tā pūrṇa karēni."
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "80|24|Ataḥpara mānuṣa yēna tāra khādyēra prati lakṣya karē [1]"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "80|25|niścaẏa āmarā pracura bāri barṣaṇa kari"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "80|26|tārapara āmarā yamīnakē yathāyathabhābē bidīrṇa kari"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "80|27|ataḥpara tātē āmarā uṯpanna kari śasya"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "80|28|āṅgura, śāka-sabji"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "80|29|yāẏatūna, khējuragācha"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "80|30|anēka gāchabiśiṣṭa udyāna"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "80|31|phala ēbaṁ gabādi khādya [1]"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "80|32|ēgulō tōmādēra ō tōmādēra catuspada jantudēra bhōgēra jan'ya [1]."
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "80|33|Ataḥpara yakhana tīkṣna ā'ōẏāja āsabē [1]"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "80|34|sēdina mānuṣa pāliẏē yābē tāra bhā'iẏēra kācha thēkē"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "80|35|ēbaṁ tāra mātā, tāra pitā"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "80|36|tāra patnī ō tāra santāna thēkē [1]"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "80|37|sēdina tādēra pratyēkēra habē ēmana gurutara abasthā yā tākē sampūrṇarūpē byasta rākhabē [1]."
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "80|38|Anēka cēhārā sēdina habē ujjbala"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "80|39|sahāsya ō praphulla"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "80|40|āra anēka cēhārā sēdina habē dhūlidhūsara"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "80|41|Sēgulōkē ācchanna karabē kālimā."
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "80|42|Ērā'i kāphira ō pāpācārī."
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "81|1|Sūryakē yakhana niṣprabha karā habē [1]"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "81|2|āra yakhana nakṣatrarāji khasē paṛabē [1]"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "81|3|āra parbatasamūhakē yakhana calamāna karā habē [1]"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "81|4|āra yakhana pūrṇagarbhā mādī uṭa upēkṣita habē [1]"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "81|5|āra yakhana ban'ya paśugulō ēkatra karā habē"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "81|6|āra yakhana sāgarakē agni-uttāla karā habē [1]"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "81|7|āra yakhana ātmāgulōkē samagōtrīẏadēra sāthē miliẏē dēẏā habē [1]"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "81|8|āra yakhana jībanta samādhistha kan'yākē [1] jijñēsa karā habē"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "81|9|kī aparādhē tākē hatyā karā haẏēchila [1]"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "81|10|āra yakhana āmalanāmāgulō unmōcita karā habē"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "81|11|āra yakhana āsamānēra ābaraṇa apasārita karā habē [1]"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "81|12|āra yakhana jāhānnāmakē bhīṣaṇabhābē prajjbalita karā habē"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "81|13|āra yakhana jānnāta nikaṭabartī karā habē"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "81|14|takhana pratyēka byakti'i jānabē sē ki upasthita karēchē [1]."
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "81|15|Sutarāṁ āmi śapatha karachi paścādapasaraṇakārī nakṣatrēra"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "81|16|yā calamāna, apasr̥ẏamāṇa"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "81|17|śapatha rātēra yakhana tā śēṣa haẏa [1]"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "81|18|śapatha prabhātēra yakhana tāra ābirbhāba haẏa"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "81|19|niścaẏa'i ē kura'āna sam'mānita rāsūlēra ānīta bāṇī [1]"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "81|20|yē sāmarthyaśālī, āraśēra mālikēra kāchē maryādā sampanna"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "81|21|sē mān'ya sēkhānē, biśbāsabhājana [1]."
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "81|22|Āra tōmādēra sāthī unmāda nana [1]"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "81|23|tini tō tākē spaṣṭa digantē dēkhēchēna [1]"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "81|24|tini gāẏēbī biṣaẏa samparkē kr̥paṇa naẏa [1]."
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "81|25|Āra ēṭā kōnō abhiśapta śaẏatānēra bākya naẏa."
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "81|26|Kājē'i tōmarā kōthāẏa yāccha"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "81|27|ē tō śudhu sr̥ṣṭikulēra jan'ya upadēśa"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "81|28|tōmādēra madhyē yē sarala pathē calatē cāẏa, tāra jan'ya [1]."
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "81|29|Āra tōmarā icchē karatē pāra nā, yadi nā sr̥ṣṭikulēra raba āllāh icchē karēna [1]."
        }
    ]
}