            "text": "101|1|Bhītiprada mahā bipada [1]"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "101|2|bhītiprada mahā bipada kī"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "101|3|āra bhītiprada mahā bipada samparkē āpanākē kisē jānābē"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "101|4|sēdina mānuṣa habē bikṣipta pataṅgēra mata [1]"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "101|5|āra parbatasamūha habē dhunita raṅgina paśamēra mata [1]."
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "101|6|Ataḥpara yāra pāllāsamūha [1] bhārī habē [2]"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "101|7|sē tō thākabē santōṣajanaka jībanē."
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "101|8|Āra yāra pāllāsamūha hālkā habē [1]"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "101|9|tāra sthāna habē ‘hā'ōẏiẏāh’ [1]."
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "101|10|Āra āpanākē kisē jānābē sēṭā kī"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "101|11|atyanta uttapta āguna [1]."
        }
    ]
