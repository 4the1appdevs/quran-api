{"rukus":[{"chapter":56,"verse":1,"text":"56|1|yakhana saṅghaṭita habē kiẏāmata [1]"},{"chapter":56,"verse":2,"text":"56|2|(takhana) ēṭāra saṅghaṭana mithyā balāra kē'u thākabē nā [1]."},{"chapter":56,"verse":3,"text":"56|3|Ēṭā kā'ukē karabē nīca, kā'ukē karabē samunnata [1]"},{"chapter":56,"verse":4,"text":"56|4|yakhana prabala kampanē prakampita habē yamīna"},{"chapter":56,"verse":5,"text":"56|5|ēbaṁ cūrṇa-bicūrṇa haẏē paṛabē parbatamālā"},{"chapter":56,"verse":6,"text":"56|6|ataḥpara tā paryabasita habē uṯkṣipta dhūlikaṇāẏa"},{"chapter":56,"verse":7,"text":"56|7|āra tōmārā bibhakta haẏē paṛabē tina dalē --- [1]"},{"chapter":56,"verse":8,"text":"56|8|ataḥpara ḍāna dikēra dala; ḍāna dikēra dalaṭi kata saubhāgyabāna [1]"},{"chapter":56,"verse":9,"text":"56|9|ēbaṁ bāma dikēra dala; āra bāma dikēra dalaṭi kata hatabhāgā [1]"},{"chapter":56,"verse":10,"text":"56|10|āra agrabartigaṇa'i tō agrabartī [1]"},{"chapter":56,"verse":11,"text":"56|11|tārā'i naikaṭyapraāpta"},{"chapter":56,"verse":12,"text":"56|12|ni'āmatapūrṇa udyānē"},{"chapter":56,"verse":13,"text":"56|13|bahu saṅkhyaka habē pūrbabartīdēra madhya thēkē"},{"chapter":56,"verse":14,"text":"56|14|ēbaṁ alpa saṅkhyaka habē parabartīdēra madhya thēkē [1]."},{"chapter":56,"verse":15,"text":"56|15|Sbarṇa ō dāmī pāthara khacita āsanē"},{"chapter":56,"verse":16,"text":"56|16|tārā hēlāna diẏē basabē, paraspara mukhōmukhi haẏē [1]."},{"chapter":56,"verse":17,"text":"56|17|Tādēra āśēpāśē ghurāphirā karabē cira - kiśōrērā [1]"},{"chapter":56,"verse":18,"text":"56|18|pānapātra, jaga ō prasrabaṇa niḥsr̥ta surāpūrṇa pēẏālā niẏē [1]."},{"chapter":56,"verse":19,"text":"56|19|Sē surā pānē tādēra śiraḥpīṛā habē nā, tārā jñānahārā'ō habē nā--- [1]"},{"chapter":56,"verse":20,"text":"56|20|Āra (ghōrāphērā karabē) tādēra pachandamata phalamūla niẏē [1]"},{"chapter":56,"verse":21,"text":"56|21|āra tādēra īnsita pākhīra gōśata niẏē [1]."},{"chapter":56,"verse":22,"text":"56|22|Āra tādēra jan'ya thākabē ḍāgara cakṣubiśiṣṭā hūra"},{"chapter":56,"verse":23,"text":"56|23|yēna tārā surakṣita muktā [1]"},{"chapter":56,"verse":24,"text":"56|24|tādēra kājēra puraskārasbarūpa."},{"chapter":56,"verse":25,"text":"56|25|Sēkhānē tārā śunabē nā kōna asāra bā pāpabākya [1]"},{"chapter":56,"verse":26,"text":"56|26|‘sālāma’ āra ‘sālāma’ bāṇī chāṛā."},{"chapter":56,"verse":27,"text":"56|27|Āra ḍāna dikēra dala, kata bhāgyabāna ḍāna dikēra dala"},{"chapter":56,"verse":28,"text":"56|28|tārā thākabē ēmana udyānē, yātē āchē [1] kām̐ṭāhīna kulagācha [2]"},{"chapter":56,"verse":29,"text":"56|29|ēbaṁ kām̐di bharā kalā gācha"},{"chapter":56,"verse":30,"text":"56|30|āra samprasārita chāẏā [1]"},{"chapter":56,"verse":31,"text":"56|31|āra sadā prabāhamāna pāni"},{"chapter":56,"verse":32,"text":"56|32|ō pracura phalamūla"},{"chapter":56,"verse":33,"text":"56|33|yā śēṣa habē nā ō yā niṣid'dha'ō habēnā [1]."},{"chapter":56,"verse":34,"text":"56|34|Āra samucca śayyāsamūha [1]"},{"chapter":56,"verse":35,"text":"56|35|niścaẏa āmarā tādērakē sr̥ṣṭi karēchi biśēṣarūpē [1]"},{"chapter":56,"verse":36,"text":"56|36|ataḥpara tādērakē karēchi kumārī [1]"},{"chapter":56,"verse":37,"text":"56|37|sōhāginī [1] ō samabaẏaskā [2]"},{"chapter":56,"verse":38,"text":"56|38|ḍānadikēra lōkadēra jan'ya."}]}