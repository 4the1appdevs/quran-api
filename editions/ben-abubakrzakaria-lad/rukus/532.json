{
    "rukus": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "90|1|Āmi [1] śapatha karachi ē nagarēra [2]"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "90|2|āra āpani ē nagarēra adhibāsī [1]"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "90|3|śapatha janmadātāra ō yā sē janma diẏēchē [1]."
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "90|4|Niḥsandēhē āmarā mānuṣakē sr̥ṣṭi karēchi kaṣṭa-klēśēra madhyē [1]."
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "90|5|Sē ki manē karē yē, kakhanō tāra upara kē'u kṣamatābāna habē nā"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "90|6|sē balē, ‘āmi pracura artha niḥśēṣa karēchi [1].’"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "90|7|Sē ki manē karē yē, tākē kē'u dēkhēni"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "90|8|āmarā ki tāra jan'ya sr̥ṣṭi karini ducōkha"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "90|9|Āra jihbā ō du'i ṭhōm̐ṭa"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "90|10|āra āmarā tākē dēkhiẏēchi [1] du’ṭi patha [2]."
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "90|11|Tabē sē tō bandhura giripathē [1] prabēśa karēni."
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "90|12|Āra kisē āpanākē jānābē--- bandhura giripatha kī"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "90|13|ēṭā hacchē: Dāsamukti [1]"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "90|14|athabā durbhikṣēra dinē khādyadāna [1]—"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "90|15|iẏātīma ātmīẏakē [1]"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "90|16|athabā dāridra-niṣpēṣita niḥsbakē"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "90|17|tadupari sē tādēra antarbhukta haẏē yāẏa yārā īmāna ēnēchē ēbaṁ parasparakē upadēśa diẏēchē dhairya dhāraṇēra, āra paraspara upadēśa diẏēchē daẏā anugrahēra [1]"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "90|18|tārā'i saubhāgyaśālī."
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "90|19|Āra yārā āmādēra āẏātasamūhē kupharī karēchē, tārā'i hatabhāgya."
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "90|20|Tārā paribēṣṭita habē abarud'dha āgunē."
        }
    ]
}