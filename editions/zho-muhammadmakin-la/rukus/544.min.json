{"rukus":[{"chapter":102,"verse":1,"text":"102|1|Jingsai fushu, yi shi nimen shuhu,"},{"chapter":102,"verse":2,"text":"102|2|zhidao nimen qu you fendi."},{"chapter":102,"verse":3,"text":"102|3|Zhen de, nimen jianglai jiu zhidaole."},{"chapter":102,"verse":4,"text":"102|4|Zhen de, nimen jianglai jiu zhidaole."},{"chapter":102,"verse":5,"text":"102|5|Zhen de, jiaruo nimen you zhenzhizhuojian, (nimen bi bu shuhu),"},{"chapter":102,"verse":6,"text":"102|6|nimen biding kanjian huo yu,"},{"chapter":102,"verse":7,"text":"102|7|ranhou, nimen bi qinyan kanjian ta."},{"chapter":102,"verse":8,"text":"102|8|Zai na ri, nimen bi wei enze er bei shenwen."}]}