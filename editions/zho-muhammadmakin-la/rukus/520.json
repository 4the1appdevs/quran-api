{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "79|1|Shi yi ji che de,"
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "79|2|qing ye de,"
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "79|3|benchi de,"
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "79|4|xianqu de,"
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "79|5|juece de,"
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "79|6|dang zhendong zhe zhendong,"
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "79|7|er xu fa zhe xu fa zhi ri,"
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "79|8|zai na ri, xuduo xin jiang tantebu'an,"
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "79|9|xuduo yan jiang bu gan yangshi."
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "79|10|Tamen shuo: Women biding fuyuan ma?"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "79|11|Na shi zai women yibian cheng xiu gu de shihou ma?"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "79|12|Tamen shuo: Ranze, na shi yici kui she de fuyuan."
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "79|13|Na zhishi yici housheng,"
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "79|14|tamen huran zai dimian zhi shang."
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "79|15|Mu sa de gushi yi lailin nile ma?"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "79|16|Dangshi, ta de zhu, ceng zai sheng gu du wa zhong zhaohuan ta shuo:"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "79|17|“Ni dao falao na li qu ba! Ta que shi beini de."
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "79|18|Ni dui ta shuo:“Ni yuanyi chengwei chunjie de ren ma?"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "79|19|Ni yuanyi wo yindao ni renshi ni de zhu, er ni jingwei ta ma?”"
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "79|20|Ta ba na zuida de jixiang zhaoshile falao,"
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "79|21|dan ta fouren, erqie weikang."
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "79|22|Ranhou zhuanshen er benzou."
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "79|23|Yushi, zhaoji minzhong, erqie hanjiao,"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "79|24|shuo:“Wo shi nimen zhizun de zhu.”"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "79|25|Gu an la yihou shi he jinshi de xingfa chengzhi ta."
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "79|26|Duiyu weiju de renmen, ci zhong que youyi zhong jianjie."
        }
    ]
}