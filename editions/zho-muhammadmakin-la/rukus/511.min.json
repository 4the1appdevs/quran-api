{"rukus":[{"chapter":74,"verse":32,"text":"74|32|Zhen de, yi yueliang mengshi,"},{"chapter":74,"verse":33,"text":"74|33|yi shiqu de heiye mengshi,"},{"chapter":74,"verse":34,"text":"74|34|yi xian zhao di liming mengshi,"},{"chapter":74,"verse":35,"text":"74|35|huo yu que shi yige da nan,"},{"chapter":74,"verse":36,"text":"74|36|keyi jinggao renlei,"},{"chapter":74,"verse":37,"text":"74|37|jinggao nimen zhong yu qianjin zhe huo yu houtui zhe;"},{"chapter":74,"verse":38,"text":"74|38|geren jiang yin ziji de ying mou er zuo diya,"},{"chapter":74,"verse":39,"text":"74|39|wei xingfu de renmen chuwai."},{"chapter":74,"verse":40,"text":"74|40|Tamen zai leyuan zhong huxiang xunwen,"},{"chapter":74,"verse":41,"text":"74|41|wen fanzui renmen de qingzhuang,"},{"chapter":74,"verse":42,"text":"74|42|nimen weisheme duo ru huo yu ne? ”"},{"chapter":74,"verse":43,"text":"74|43|Tamen shuo:“Women meiyou libai,"},{"chapter":74,"verse":44,"text":"74|44|ye meiyou jipin,"},{"chapter":74,"verse":45,"text":"74|45|women yu wangyan de renmen yidao wangyan,"},{"chapter":74,"verse":46,"text":"74|46|women fouren baoying ri,"},{"chapter":74,"verse":47,"text":"74|47|yizhi dao siwang jianglinliao women.”"},{"chapter":74,"verse":48,"text":"74|48|Shuo qing zhe de shuo qing, jiang wuyi yu tamen."},{"chapter":74,"verse":49,"text":"74|49|Tamen zenme tuibi zhe jiaoxun ne?"},{"chapter":74,"verse":50,"text":"74|50|Tamen haoxiang yiqun jinghuang de luzi,"},{"chapter":74,"verse":51,"text":"74|51|gang taobile yi zhi shizi yiyang."},{"chapter":74,"verse":52,"text":"74|52|Buran, tamen zhong de mei ge ren dou xiwang huode yixie zhankai de tian jing."},{"chapter":74,"verse":53,"text":"74|53|Jue buran, tamen bu weiju houshi!"},{"chapter":74,"verse":54,"text":"74|54|Zhen de, zhe “gulanjing” que shi yige jiaohui!"},{"chapter":74,"verse":55,"text":"74|55|Shei yuanyi, shei jiyi ta,"},{"chapter":74,"verse":56,"text":"74|56|tamen zhi yin an la de yiyu er jiyi ta. Ta shi ying shou jingwei de, ta shi yi yu she you de."}]}