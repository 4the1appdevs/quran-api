{"chapter":[{"chapter":107,"verse":1,"text":"107|1|Ni ceng jian fouren baoying ri de ren ma?"},{"chapter":107,"verse":2,"text":"107|2|Ta jiushi nage hechi gu'er,"},{"chapter":107,"verse":3,"text":"107|3|qie bu mianli ren zhenji pinmin de ren."},{"chapter":107,"verse":4,"text":"107|4|Shang zai! Libai de renmen,"},{"chapter":107,"verse":5,"text":"107|5|tamen shi shuhu bai gong de,"},{"chapter":107,"verse":6,"text":"107|6|tamen shi gumingdiaoyu de,"},{"chapter":107,"verse":7,"text":"107|7|tamen shi bu ken jie ren shiwu de."}]}