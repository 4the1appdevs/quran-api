{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "86|1|Yi cangqiong he qi mingxing mengshi,"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "86|2|ni zen neng zhidao qi mingxing shi shenmo?"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "86|3|Shi na canlan de mingxing."
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "86|4|Mei geren, dou youyige baohu zhe."
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "86|5|Ren yingdang xiang yi xiang, ta ziji shi yong shenme zaocheng de?"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "86|6|Ta shi bei shechu de jingye zaocheng de."
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "86|7|Na jingye shi cong jizhu he leigu zhi jian fachu de."
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "86|8|An la que shi neng shi ta fuyuan de,"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "86|9|na shi yiqie mimi bei jiechuan zhi ri."
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "86|10|Ta jue meiyou nengli, ye meiyou yuanzhu zhe."
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "86|11|Yi han yu de yun,"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "86|12|he you feng di di fashi,"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "86|13|zhe que shi fenbie zhen wei de yanci,"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "86|14|zhe bushi xiaohua."
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "86|15|Tamen biding yao yong jimou,"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "86|16|wo ye yao yong jimou."
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "86|17|Suoyi ni dang kuanxian bu xindao de renmen, ni dang kuanxian tamen yixia."
        }
    ]
}