            "chapter": 75,
            "verse": 31,
            "text": "75|31|Tā méiyǒu xìndào, yě méiyǒu lǐbài."
        },
        {
            "chapter": 75,
            "verse": 32,
            "text": "75|32|Tā fǒurèn zhēnlǐ, bèi qì zhèngdào,"
        },
        {
            "chapter": 75,
            "verse": 33,
            "text": "75|33|ránhòu àomàn de zǒu huí jiā qù."
        },
        {
            "chapter": 75,
            "verse": 34,
            "text": "75|34|Huǐmiè jiànjiàn de línjìn nǐ,"
        },
        {
            "chapter": 75,
            "verse": 35,
            "text": "75|35|huǐmiè yǐ jiànjiàn de línjìn nǐ.”"
        },
        {
            "chapter": 75,
            "verse": 36,
            "text": "75|36|Nándào ren cāixiǎng zìjǐ shì bèi fàngrèn de ma?"
        },
        {
            "chapter": 75,
            "verse": 37,
            "text": "75|37|Nándào tā bùcéng shì bèi shèchū jīngyè ma?"
        },
        {
            "chapter": 75,
            "verse": 38,
            "text": "75|38|Ránhòu, tā biàn chéng xiěkuài, ér ān lā jiāyǐ chuàngzào tā, shǐ zhī chéngwéi zhītǐ wánquán de rén ma?"
        },
        {
            "chapter": 75,
            "verse": 39,
            "text": "75|39|Tā yòng jīngyè zàohuà liǎngxìng, nán de hé nǚ de."
        },
        {
            "chapter": 75,
            "verse": 40,
            "text": "75|40|Nándào nàyàng de zàohuà zhě bùnéng shǐ sǐrén fùhuó ma?"
