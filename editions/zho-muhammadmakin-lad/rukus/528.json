{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "86|1|Yǐ cāngqióng hé qǐ míngxīng méngshì,"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "86|2|nǐ zěn néng zhīdào qǐ míngxīng shì shénmó?"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "86|3|Shì nà cànlàn de míngxīng."
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "86|4|Měi gèrén, dōu yǒuyīgè bǎohù zhě."
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "86|5|Rén yīngdāng xiǎng yī xiǎng, tā zìjǐ shì yòng shénme zàochéng de?"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "86|6|Tā shì bèi shèchū de jīngyè zàochéng de."
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "86|7|Nà jīngyè shì cóng jǐzhù hé lèigǔ zhī jiān fāchū de."
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "86|8|Ān lā què shì néng shǐ tā fùyuán de,"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "86|9|nà shì yīqiè mìmì bèi jiēchuān zhī rì."
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "86|10|Tā jué méiyǒu nénglì, yě méiyǒu yuánzhù zhě."
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "86|11|Yǐ hán yǔ de yún,"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "86|12|hé yǒu fèng dì dì fāshì,"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "86|13|zhè què shì fēnbié zhēn wěi de yáncí,"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "86|14|zhè bùshì xiàohuà."
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "86|15|Tāmen bìdìng yào yòng jìmóu,"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "86|16|wǒ yě yào yòng jìmóu."
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "86|17|Suǒyǐ nǐ dāng kuānxiàn bù xìndào de rénmen, nǐ dāng kuānxiàn tāmen yīxià."
        }
    ]
}