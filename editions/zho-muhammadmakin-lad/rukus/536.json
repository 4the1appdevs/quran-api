    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "94|1|Nándào wǒ méiyǒu wéi nǐ ér kāità nǐ de xiōngjīn ma?"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "94|2|Wǒ xiè xiàle nǐ de zhòngrèn,"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "94|3|jí shǐ nǐ de bèi dānfùguò zhòng de,"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "94|4|ér tígāole nǐ de shēngwàng,"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "94|5|yǔ jiān nàn xiāngbàn de, què shì róngyì,"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "94|6|yǔ jiān nàn xiāngbàn de, què shì róngyì,"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "94|7|dāng nǐ de shìwù wánbì shí,"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "94|8|nǐ yīngdāng qínláo, nǐ yīngdāng xiàng nǐ de zhǔ kěnqiú."
