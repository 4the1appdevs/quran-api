{
    "chapter": [
        {
            "chapter": 74,
            "verse": 1,
            "text": "74|1|Gài bèi de rén a!"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "74|2|Nǐ yīngdāng qǐlái, nǐ yīngdāng jǐnggào,"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "74|3|nǐ yīngdāng sòngyáng nǐ de zhǔzǎi,"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "74|4|nǐ yīngdāng xǐdí nǐ de yīfú,"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "74|5|nǐ yīngdāng yuǎnlí wūhuì,"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "74|6|nǐ bùyào shī ēn ér qiú hòu bào,"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "74|7|nǐ yīngdāng wèi nǐ de zhǔ ér jiānrěn."
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "74|8|Dāng hàojiǎo bèi chuī xiàng de shíhòu,"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "74|9|zài nà shí, jiāng yǒu yīgè jiānnán de rìzi."
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "74|10|Nàgè rìzi duì bù xìndào de rénmen, shì bù róngyì dùguò de."
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "74|11|Nǐ ràng wǒ dúzì chǔzhì wǒ suǒ chuàngzào dì nàgè rén ba!"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "74|12|Wǒ shǎngcì tā fēngfù de cáichǎn,"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "74|13|hé zài gēnqián de zǐsì,"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "74|14|wǒ tígāole tā de shēngwàng,"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "74|15|ér tā hái qǐwàng wǒ zài duōjiā shǎngcì."
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "74|16|Jué bùrán! Tā quèshí shì fǎnduì wǒ de jīxiàng de,"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "74|17|wǒ jiāng shǐ tā zāoshòu kǔnàn."
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "74|18|Tā duì yǐ sīkǎo, què yǐ jìhuà."
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "74|19|Dàn wúlùn tā zěnyàng jìhuà, tā shì bèiqì jué de."
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "74|20|Wúlùn tā zěnyàng jìhuà, tā zhōng shì bèi qì jué de."
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "74|21|Tā kàn yī kàn,"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "74|22|ránhòu zhòuméi cù'é,"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "74|23|ránhòu gāo'ào dì zhuàn guò shēn qù,"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "74|24|érqiě shuō:“Zhè zhǐshì chuánxí de móshù,"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "74|25|zhè zhǐshì fánrén de yáncí.”"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "74|26|Wǒ jiāng shǐ tā duò rù huǒ yù,"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "74|27|nǐ zěn néng zhīdào huǒ yù shì shénme?"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "74|28|Tā bù ràng rènhé wù cúnzài, bùxǔ rènhé wù liú xià,"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "74|29|tā shāozhuó jīfū."
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "74|30|Guǎnlǐ tā de, gòngjì shíjiǔ míng."
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "74|31|Wǒ zhǐ jiāng guǎnlǐ huǒ yù de zàochéng tiānshǐ, wǒ zhǐ yǐ tāmen de shùmù, kǎoyàn bùxìndào de rénmen, yǐbiàn céng shòu tiān jīng de rénmen rèn qīng, ér xìndào de rénmen gèngjiā dǔxìn; yǐmiǎn céng shòu tiān jīng de rénmen hé xìndào de rénmen huáiyí; yǐbiàn xīnzhōng yǒu bìng zhě hé bù xìndào zhě shuō: Ān lā shè zhège pìyù zuò shénme? Ān lā zhèyàng shǐ tāsuǒ yìyù de rén wù rù mítú, shǐ tāsuǒ yìyù de rén zūnxún zhènglù. Zhǐyǒu nǐ de zhǔ, néng zhīdào tā de jūnduì, zhè zhǐshì rénlèi de jiàoxùn."
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "74|32|Zhēn de, yǐ yuèliàng méngshì,"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "74|33|yǐ shìqù de hēiyè méngshì,"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "74|34|yǐ xiǎn zhào dí límíng méngshì,"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "74|35|huǒ yù què shì yīgè dà nàn,"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "74|36|kěyǐ jǐnggào rénlèi,"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "74|37|jǐnggào nǐmen zhōng yù qiánjìn zhě huò yù hòutuì zhě;"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "74|38|gèrén jiāng yīn zìjǐ de yíng móu ér zuò dǐyā,"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "74|39|wéi xìngfú de rénmen chúwài."
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "74|40|Tāmen zài lèyuán zhōng hùxiāng xúnwèn,"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "74|41|wèn fànzuì rénmen de qíngzhuàng,"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "74|42|nǐmen wèishéme duò rù huǒ yù ne? ”"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "74|43|Tāmen shuō:“Wǒmen méiyǒu lǐbài,"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "74|44|yě méiyǒu jìpín,"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "74|45|wǒmen yǔ wàngyán de rénmen yīdào wàngyán,"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "74|46|wǒmen fǒurèn bàoyìng rì,"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "74|47|yīzhí dào sǐwáng jiànglínliǎo wǒmen.”"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "74|48|Shuō qíng zhě de shuō qíng, jiāng wúyì yú tāmen."
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "74|49|Tāmen zěnme tuìbì zhè jiàoxùn ne?"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "74|50|Tāmen hǎoxiàng yīqún jīnghuāng de lǘzi,"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "74|51|gāng táobìle yī zhǐ shīzi yīyàng."
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "74|52|Bùrán, tāmen zhōng de měi gè rén dōu xīwàng huòdé yīxiē zhǎnkāi de tiān jīng."
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "74|53|Jué bùrán, tāmen bù wèijù hòushì!"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "74|54|Zhēn de, zhè “gǔlánjīng” què shì yīgè jiàohuì!"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "74|55|Shéi yuànyì, shéi jìyì tā,"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "74|56|tāmen zhǐ yīn ān lā de yìyù ér jìyì tā. Tā shì yīng shòu jìngwèi de, tā shì yí yú shè yòu de."
        }
    ]
}