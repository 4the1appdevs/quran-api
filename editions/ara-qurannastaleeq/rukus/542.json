{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "ﶒ ؀وَالْعٰدِيٰتِ ضَبْحًا"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "ﶫ فَالْمُوْرِيٰتِ قَدْحًا"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "ﶫ فَالْمُغِيْرٰتِ صُبْحًا"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "ﶫ فَاَثَرْنَ بِهٖ نَقْعًا"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "ﶫ فَوَسَطْنَ بِهٖ جَمْعًا"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "ﶫ اِنَّ الْاِنْسَانَ لِرَبِّهٖ لَكَنُوْدٌ"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "ﶔ وَاِنَّهٗ عَلٰي ذٰلِكَ لَشَهِيْدٌ"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "ﶔ وَاِنَّهٗ لِحُبِّ الْخَيْرِ لَشَدِيْدٌ"
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "ﶠ اَفَلَا يَعْلَمُ اِذَا بُعْثِرَ مَا فِي الْقُبُوْرِ"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "ﶫ وَحُصِّلَ مَا فِي الصُّدُوْرِ"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "ﶫ اِنَّ رَبَّهُمْ بِهِمْ يَوْمَئِذٍ لَّخَبِيْرٌ"
        }
    ]
}