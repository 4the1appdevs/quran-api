{
    "rukus": [
        {
            "chapter": 104,
            "verse": 1,
            "text": "ﶒ ؀وَيْلٌ لِّكُلِّ هُمَزَةٍ لُّمَزَةِ"
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "ﶫ اِۨلَّذِيْ جَمَعَ مَالًا وَّعَدَّدَهٗ"
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "ﶫ يَحْسَبُ اَنَّ مَالَهٗ٘ اَخْلَدَهٗ"
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "ﶔ كَلَّا لَيُنْۣبَذَنَّ فِي الْحُطَمَةِ"
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "ﶛ وَمَا٘ اَدْرٰىكَ مَا الْحُطَمَةُ"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "ﶠ نَارُ اللّٰهِ الْمُوْقَدَةُ"
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "ﶫ الَّتِيْ تَطَّلِعُ عَلَي الْاَفْـِٕدَةِ"
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "ﶠ اِنَّهَا عَلَيْهِمْ مُّؤْصَدَةٌ"
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "ﶫ فِيْ عَمَدٍ مُّمَدَّدَةٍ"
        }
    ]
}