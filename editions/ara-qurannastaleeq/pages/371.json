{
    "pages": [
        {
            "chapter": 26,
            "verse": 84,
            "text": "ﶫ وَاجْعَلْ لِّيْ لِسَانَ صِدْقٍ فِي الْاٰخِرِيْنَ"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "ﶫ وَاجْعَلْنِيْ مِنْ وَّرَثَةِ جَنَّةِ النَّعِيْمِ"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "ﶫ وَاغْفِرْ لِاَبِيْ٘ اِنَّهٗ كَانَ مِنَ الضَّآلِّيْنَ"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "ﶫ وَلَا تُخْزِنِيْ يَوْمَ يُبْعَثُوْنَ"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "ﶫ يَوْمَ لَا يَنْفَعُ مَالٌ وَّلَا بَنُوْنَ"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "ﶫ اِلَّا مَنْ اَتَي اللّٰهَ بِقَلْبٍ سَلِيْمٍ"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "ﶠ وَاُزْلِفَتِ الْجَنَّةُ لِلْمُتَّقِيْنَ"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "ﶫ وَبُرِّزَتِ الْجَحِيْمُ لِلْغٰوِيْنَ"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "ﶫ وَقِيْلَ لَهُمْ اَيْنَمَا كُنْتُمْ تَعْبُدُوْنَ"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "ﶫ مِنْ دُوْنِ اللّٰهِﵧ هَلْ يَنْصُرُوْنَكُمْ اَوْ يَنْتَصِرُوْنَ"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "ﶠ فَكُبْكِبُوْا فِيْهَا هُمْ وَالْغَاوٗنَ"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "ﶫ وَجُنُوْدُ اِبْلِيْسَ اَجْمَعُوْنَ"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "ﶠ قَالُوْا وَهُمْ فِيْهَا يَخْتَصِمُوْنَ"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "ﶫ تَاللّٰهِ اِنْ كُنَّا لَفِيْ ضَلٰلٍ مُّبِيْنٍ"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "ﶫ اِذْ نُسَوِّيْكُمْ بِرَبِّ الْعٰلَمِيْنَ"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "وَمَا٘ اَضَلَّنَا٘ اِلَّا الْمُجْرِمُوْنَ"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "فَمَا لَنَا مِنْ شَافِعِيْنَ"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "ﶫ وَلَا صَدِيْقٍ حَمِيْمٍ"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "فَلَوْ اَنَّ لَنَا كَرَّةً فَنَكُوْنَ مِنَ الْمُؤْمِنِيْنَ"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "اِنَّ فِيْ ذٰلِكَ لَاٰيَةًﵧ وَمَا كَانَ اَكْثَرُهُمْ مُّؤْمِنِيْنَ"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "وَاِنَّ رَبَّكَ لَهُوَ الْعَزِيْزُ الرَّحِيْمُ"
        },
        {
            "chapter": 26,
            "verse": 105,
            "text": "ﶒ كَذَّبَتْ قَوْمُ نُوْحِ اِۨلْمُرْسَلِيْنَ"
        },
        {
            "chapter": 26,
            "verse": 106,
            "text": "ﶗ اِذْ قَالَ لَهُمْ اَخُوْهُمْ نُوْحٌ اَلَا تَتَّقُوْنَ"
        },
        {
            "chapter": 26,
            "verse": 107,
            "text": "ﶔ اِنِّيْ لَكُمْ رَسُوْلٌ اَمِيْنٌ"
        },
        {
            "chapter": 26,
            "verse": 108,
            "text": "ﶫ فَاتَّقُوا اللّٰهَ وَاَطِيْعُوْنِ"
        },
        {
            "chapter": 26,
            "verse": 109,
            "text": "ﶔ وَمَا٘ اَسْـَٔلُكُمْ عَلَيْهِ مِنْ اَجْرٍﵐ اِنْ اَجْرِيَ اِلَّا عَلٰي رَبِّ الْعٰلَمِيْنَ"
        },
        {
            "chapter": 26,
            "verse": 110,
            "text": "ﶔ فَاتَّقُوا اللّٰهَ وَاَطِيْعُوْنِ"
        },
        {
            "chapter": 26,
            "verse": 111,
            "text": "ﶠ قَالُوْ٘ا اَنُؤْمِنُ لَكَ وَاتَّبَعَكَ الْاَرْذَلُوْنَ"
        }
    ]
}