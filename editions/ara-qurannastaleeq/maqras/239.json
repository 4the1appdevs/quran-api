{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "ﶒ ؀اَلَمْ نَشْرَحْ لَكَ صَدْرَكَ"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "ﶫ وَوَضَعْنَا عَنْكَ وِزْرَكَ"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "ﶫ الَّذِيْ٘ اَنْقَضَ ظَهْرَكَ"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "ﶫ وَرَفَعْنَا لَكَ ذِكْرَكَ"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "ﶠ فَاِنَّ مَعَ الْعُسْرِ يُسْرًا"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "ﶫ اِنَّ مَعَ الْعُسْرِ يُسْرًا"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "ﶠ فَاِذَا فَرَغْتَ فَانْصَبْ"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "ﶫ وَاِلٰي رَبِّكَ فَارْغَبْ"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "ﶒ ؀وَالتِّيْنِ وَالزَّيْتُوْنِ"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "ﶫ وَطُوْرِ سِيْنِيْنَ"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "ﶫ وَهٰذَا الْبَلَدِ الْاَمِيْنِ"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "ﶫ لَقَدْ خَلَقْنَا الْاِنْسَانَ فِيْ٘ اَحْسَنِ تَقْوِيْمٍ"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "ﶚ ثُمَّ رَدَدْنٰهُ اَسْفَلَ سٰفِلِيْنَ"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "ﶫ اِلَّا الَّذِيْنَ اٰمَنُوْا وَعَمِلُوا الصّٰلِحٰتِ فَلَهُمْ اَجْرٌ غَيْرُ مَمْنُوْنٍ"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "ﶠ فَمَا يُكَذِّبُكَ بَعْدُ بِالدِّيْنِ"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "ﶠ اَلَيْسَ اللّٰهُ بِاَحْكَمِ الْحٰكِمِيْنَ"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "ﶒ ؀اِقْرَاْ بِاسْمِ رَبِّكَ الَّذِيْ خَلَقَ"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "ﶔ خَلَقَ الْاِنْسَانَ مِنْ عَلَقٍ"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "ﶔ اِقْرَاْ وَرَبُّكَ الْاَكْرَمُ"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "ﶫ الَّذِيْ عَلَّمَ بِالْقَلَمِ"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "ﶫ عَلَّمَ الْاِنْسَانَ مَا لَمْ يَعْلَمْ"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "ﶠ كَلَّا٘ اِنَّ الْاِنْسَانَ لَيَطْغٰ٘ي"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "ﶫ اَنْ رَّاٰهُ اسْتَغْنٰي"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "ﶠ اِنَّ اِلٰي رَبِّكَ الرُّجْعٰي"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "ﶠ اَرَءَيْتَ الَّذِيْ يَنْهٰي"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "ﶫ عَبْدًا اِذَا صَلّٰي"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "ﶠ اَرَءَيْتَ اِنْ كَانَ عَلَي الْهُدٰ٘ي"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "ﶫ اَوْ اَمَرَ بِالتَّقْوٰي"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "ﶠ اَرَءَيْتَ اِنْ كَذَّبَ وَتَوَلّٰي"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "ﶠ اَلَمْ يَعْلَمْ بِاَنَّ اللّٰهَ يَرٰي"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "ﶠ كَلَّا لَئِنْ لَّمْ يَنْتَهِﵿ لَنَسْفَعًاۣ بِالنَّاصِيَةِ"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "ﶫ نَاصِيَةٍ كَاذِبَةٍ خَاطِئَةٍ"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "ﶔ فَلْيَدْعُ نَادِيَهٗ"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "ﶫ سَنَدْعُ الزَّبَانِيَةَ"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "ﶫ كَلَّاﵧ لَا تُطِعْهُ وَاسْجُدْ وَاقْتَرِبْ"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "ﶶ ؀اِنَّا٘ اَنْزَلْنٰهُ فِيْ لَيْلَةِ الْقَدْرِ"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "ﶖ وَمَا٘ اَدْرٰىكَ مَا لَيْلَةُ الْقَدْرِ"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "ﶠ لَيْلَةُ الْقَدْرِﵿ خَيْرٌ مِّنْ اَلْفِ شَهْرٍ"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "ﶣ تَنَزَّلُ الْمَلٰٓئِكَةُ وَالرُّوْحُ فِيْهَا بِاِذْنِ رَبِّهِمْ مِّنْ كُلِّ اَمْرٍ"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "ﶬ سَلٰمٌﵵ هِيَ حَتّٰي مَطْلَعِ الْفَجْرِ"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "ﶒ ؀لَمْ يَكُنِ الَّذِيْنَ كَفَرُوْا مِنْ اَهْلِ الْكِتٰبِ وَالْمُشْرِكِيْنَ مُنْفَكِّيْنَ حَتّٰي تَاْتِيَهُمُ الْبَيِّنَةُ"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "ﶫ رَسُوْلٌ مِّنَ اللّٰهِ يَتْلُوْا صُحُفًا مُّطَهَّرَةً"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "ﶫ فِيْهَا كُتُبٌ قَيِّمَةٌ"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "ﶠ وَمَا تَفَرَّقَ الَّذِيْنَ اُوْتُوا الْكِتٰبَ اِلَّا مِنْۣ بَعْدِ مَا جَآءَتْهُمُ الْبَيِّنَةُ"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "ﶠ وَمَا٘ اُمِرُوْ٘ا اِلَّا لِيَعْبُدُوا اللّٰهَ مُخْلِصِيْنَ لَهُ الدِّيْنَﵿ حُنَفَآءَ وَيُقِيْمُوا الصَّلٰوةَ وَيُؤْتُوا الزَّكٰوةَ وَذٰلِكَ دِيْنُ الْقَيِّمَةِ"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "ﶠ اِنَّ الَّذِيْنَ كَفَرُوْا مِنْ اَهْلِ الْكِتٰبِ وَالْمُشْرِكِيْنَ فِيْ نَارِ جَهَنَّمَ خٰلِدِيْنَ فِيْهَاﵧ اُولٰٓئِكَ هُمْ شَرُّ الْبَرِيَّةِ"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "ﶠ اِنَّ الَّذِيْنَ اٰمَنُوْا وَعَمِلُوا الصّٰلِحٰتِﶈ اُولٰٓئِكَ هُمْ خَيْرُ الْبَرِيَّةِ"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "ﶠ جَزَآؤُهُمْ عِنْدَ رَبِّهِمْ جَنّٰتُ عَدْنٍ تَجْرِيْ مِنْ تَحْتِهَا الْاَنْهٰرُ خٰلِدِيْنَ فِيْهَا٘ اَبَدًاﵧ رَضِيَ اللّٰهُ عَنْهُمْ وَرَضُوْا عَنْهُﵧ ذٰلِكَ لِمَنْ خَشِيَ رَبَّهٗ"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "ﶒ ؀اِذَا زُلْزِلَتِ الْاَرْضُ زِلْزَالَهَا"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "ﶫ وَاَخْرَجَتِ الْاَرْضُ اَثْقَالَهَا"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "ﶫ وَقَالَ الْاِنْسَانُ مَا لَهَا"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "ﶔ يَوْمَئِذٍ تُحَدِّثُ اَخْبَارَهَا"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "ﶠ بِاَنَّ رَبَّكَ اَوْحٰي لَهَا"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "ﶠ يَوْمَئِذٍ يَّصْدُرُ النَّاسُ اَشْتَاتًاﵿ لِّيُرَوْا اَعْمَالَهُمْ"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "ﶠ فَمَنْ يَّعْمَلْ مِثْقَالَ ذَرَّةٍ خَيْرًا يَّرَهٗ"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "ﶠ وَمَنْ يَّعْمَلْ مِثْقَالَ ذَرَّةٍ شَرًّا يَّرَهٗ"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "ﶒ ؀وَالْعٰدِيٰتِ ضَبْحًا"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "ﶫ فَالْمُوْرِيٰتِ قَدْحًا"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "ﶫ فَالْمُغِيْرٰتِ صُبْحًا"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "ﶫ فَاَثَرْنَ بِهٖ نَقْعًا"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "ﶫ فَوَسَطْنَ بِهٖ جَمْعًا"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "ﶫ اِنَّ الْاِنْسَانَ لِرَبِّهٖ لَكَنُوْدٌ"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "ﶔ وَاِنَّهٗ عَلٰي ذٰلِكَ لَشَهِيْدٌ"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "ﶔ وَاِنَّهٗ لِحُبِّ الْخَيْرِ لَشَدِيْدٌ"
        }
    ]
}