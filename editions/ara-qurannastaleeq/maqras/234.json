{
    "maqras": [
        {
            "chapter": 80,
            "verse": 1,
            "text": "عَبَسَ وَتَوَلّٰ٘ي ﶫ"
        },
        {
            "chapter": 80,
            "verse": 2,
            "text": "اَنۡ جَآءَهُ الۡاَعۡمٰي ﶠ"
        },
        {
            "chapter": 80,
            "verse": 3,
            "text": "وَمَا يُدۡرِيۡكَ لَعَلَّهٗ يَزَّكّٰ٘ي ﶫ"
        },
        {
            "chapter": 80,
            "verse": 4,
            "text": "اَوۡ يَذَّكَّرُ فَتَنۡفَعَهُ الذِّكۡرٰي ﶠ"
        },
        {
            "chapter": 80,
            "verse": 5,
            "text": "اَمَّا مَنِ اسۡتَغۡنٰي ﶫ"
        },
        {
            "chapter": 80,
            "verse": 6,
            "text": "فَاَنۡتَ لَهٗ تَصَدّٰي ﶠ"
        },
        {
            "chapter": 80,
            "verse": 7,
            "text": "وَمَا عَلَيۡكَ اَلَّا يَزَّكّٰي ﶠ"
        },
        {
            "chapter": 80,
            "verse": 8,
            "text": "وَاَمَّا مَنۡ جَآءَكَ يَسۡعٰي ﶫ"
        },
        {
            "chapter": 80,
            "verse": 9,
            "text": "وَهُوَ يَخۡشٰي ﶫ"
        },
        {
            "chapter": 80,
            "verse": 10,
            "text": "فَاَنۡتَ عَنۡهُ تَلَهّٰي ﶔ"
        },
        {
            "chapter": 80,
            "verse": 11,
            "text": "كَلَّا٘ اِنَّهَا تَذۡكِرَةٌ ﶔ"
        },
        {
            "chapter": 80,
            "verse": 12,
            "text": "فَمَنۡ شَآءَ ذَكَرَهٗ ﶭ"
        },
        {
            "chapter": 80,
            "verse": 13,
            "text": "فِيۡ صُحُفٍ مُّكَرَّمَةٍ ﶫ"
        },
        {
            "chapter": 80,
            "verse": 14,
            "text": "مَّرۡفُوۡعَةٍ مُّطَهَّرَةٍ ﶫ"
        },
        {
            "chapter": 80,
            "verse": 15,
            "text": "ۣبِاَيۡدِيۡ سَفَرَةٍ ﶫ"
        },
        {
            "chapter": 80,
            "verse": 16,
            "text": "كِرَامٍۣ بَرَرَةٍ ﶠ"
        },
        {
            "chapter": 80,
            "verse": 17,
            "text": "قُتِلَ الۡاِنۡسَانُ مَا٘ اَكۡفَرَهٗ ﶠ"
        },
        {
            "chapter": 80,
            "verse": 18,
            "text": "مِنۡ اَيِّ شَيۡءٍ خَلَقَهٗ ﶠ"
        },
        {
            "chapter": 80,
            "verse": 19,
            "text": "مِنۡ نُّطۡفَةٍﵧ خَلَقَهٗ فَقَدَّرَهٗ ﶫ"
        },
        {
            "chapter": 80,
            "verse": 20,
            "text": "ثُمَّ السَّبِيۡلَ يَسَّرَهٗ ﶫ"
        },
        {
            "chapter": 80,
            "verse": 21,
            "text": "ثُمَّ اَمَاتَهٗ فَاَقۡبَرَهٗ ﶫ"
        },
        {
            "chapter": 80,
            "verse": 22,
            "text": "ثُمَّ اِذَا شَآءَ اَنۡشَرَهٗ ﶠ"
        },
        {
            "chapter": 80,
            "verse": 23,
            "text": "كَلَّا لَمَّا يَقۡضِ مَا٘ اَمَرَهٗ ﶠ"
        },
        {
            "chapter": 80,
            "verse": 24,
            "text": "فَلۡيَنۡظُرِ الۡاِنۡسَانُ اِلٰي طَعَامِهٖ٘ ﶫ"
        },
        {
            "chapter": 80,
            "verse": 25,
            "text": "اَنَّا صَبَبۡنَا الۡمَآءَ صَبًّا ﶫ"
        },
        {
            "chapter": 80,
            "verse": 26,
            "text": "ثُمَّ شَقَقۡنَا الۡاَرۡضَ شَقًّا ﶫ"
        },
        {
            "chapter": 80,
            "verse": 27,
            "text": "فَاَنۣۡبَتۡنَا فِيۡهَا حَبًّا ﶫ"
        },
        {
            "chapter": 80,
            "verse": 28,
            "text": "وَّعِنَبًا وَّقَضۡبًا ﶫ"
        },
        {
            "chapter": 80,
            "verse": 29,
            "text": "وَّزَيۡتُوۡنًا وَّنَخۡلًا ﶫ"
        },
        {
            "chapter": 80,
            "verse": 30,
            "text": "وَّحَدَآئِقَ غُلۡبًا ﶫ"
        },
        {
            "chapter": 80,
            "verse": 31,
            "text": "وَّفَاكِهَةً وَّاَبًّا ﶫ"
        },
        {
            "chapter": 80,
            "verse": 32,
            "text": "مَّتَاعًا لَّكُمۡ وَلِاَنۡعَامِكُمۡ ﶠ"
        },
        {
            "chapter": 80,
            "verse": 33,
            "text": "فَاِذَا جَآءَتِ الصَّآخَّةُ ﶚ"
        },
        {
            "chapter": 80,
            "verse": 34,
            "text": "يَوۡمَ يَفِرُّ الۡمَرۡءُ مِنۡ اَخِيۡهِ ﶫ"
        },
        {
            "chapter": 80,
            "verse": 35,
            "text": "وَاُمِّهٖ وَاَبِيۡهِ ﶫ"
        },
        {
            "chapter": 80,
            "verse": 36,
            "text": "وَصَاحِبَتِهٖ وَبَنِيۡهِ ﶠ"
        },
        {
            "chapter": 80,
            "verse": 37,
            "text": "لِكُلِّ امۡرِئٍ مِّنۡهُمۡ يَوۡمَئِذٍ شَاۡنٌ يُّغۡنِيۡهِ ﶠ"
        },
        {
            "chapter": 80,
            "verse": 38,
            "text": "وُجُوۡهٌ يَّوۡمَئِذٍ مُّسۡفِرَةٌ ﶫ"
        },
        {
            "chapter": 80,
            "verse": 39,
            "text": "ضَاحِكَةٌ مُّسۡتَبۡشِرَةٌ ﶔ"
        },
        {
            "chapter": 80,
            "verse": 40,
            "text": "وَوُجُوۡهٌ يَّوۡمَئِذٍ عَلَيۡهَا غَبَرَةٌ ﶫ"
        },
        {
            "chapter": 80,
            "verse": 41,
            "text": "تَرۡهَقُهَا قَتَرَةٌ ﶠ"
        },
        {
            "chapter": 80,
            "verse": 42,
            "text": "اُولٰٓئِكَ هُمُ الۡكَفَرَةُ الۡفَجَرَةُ ﶒ"
        },
        {
            "chapter": 81,
            "verse": 1,
            "text": "اِذَا الشَّمۡسُ كُوِّرَتۡ ﶟ"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "وَاِذَا النُّجُوۡمُ انۡكَدَرَتۡ ﶟ"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "وَاِذَا الۡجِبَالُ سُيِّرَتۡ ﶟ"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "وَاِذَا الۡعِشَارُ عُطِّلَتۡ ﶟ"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "وَاِذَا الۡوُحُوۡشُ حُشِرَتۡ ﶟ"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "وَاِذَا الۡبِحَارُ سُجِّرَتۡ ﶟ"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "وَاِذَا النُّفُوۡسُ زُوِّجَتۡ ﶟ"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "وَاِذَا الۡمَوۡءٗدَةُ سُئِلَتۡ ﶟ"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "بِاَيِّ ذَنۣۡبٍ قُتِلَتۡ ﶔ"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "وَاِذَا الصُّحُفُ نُشِرَتۡ ﶟ"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "وَاِذَا السَّمَآءُ كُشِطَتۡ ﶟ"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "وَاِذَا الۡجَحِيۡمُ سُعِّرَتۡ ﶟ"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "وَاِذَا الۡجَنَّةُ اُزۡلِفَتۡ ﶟ"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "عَلِمَتۡ نَفۡسٌ مَّا٘ اَحۡضَرَتۡ ﶠ"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "فَلَا٘ اُقۡسِمُ بِالۡخُنَّسِ ﶫ"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "الۡجَوَارِ الۡكُنَّسِ ﶫ"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "وَالَّيۡلِ اِذَا عَسۡعَسَ ﶫ"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "وَالصُّبۡحِ اِذَا تَنَفَّسَ ﶫ"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "اِنَّهٗ لَقَوۡلُ رَسُوۡلٍ كَرِيۡمٍ ﶫ"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "ذِيۡ قُوَّةٍ عِنۡدَ ذِي الۡعَرۡشِ مَكِيۡنٍ ﶫ"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "مُّطَاعٍ ثَمَّ اَمِيۡنٍ ﶠ"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "وَمَا صَاحِبُكُمۡ بِمَجۡنُوۡنٍ ﶔ"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "وَلَقَدۡ رَاٰهُ بِالۡاُفُقِ الۡمُبِيۡنِ ﶔ"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "وَمَا هُوَ عَلَي الۡغَيۡبِ بِضَنِيۡنٍ ﶔ"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "وَمَا هُوَ بِقَوۡلِ شَيۡطٰنٍ رَّجِيۡمٍ ﶫ"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "فَاَيۡنَ تَذۡهَبُوۡنَ ﶠ"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "اِنۡ هُوَ اِلَّا ذِكۡرٌ لِّلۡعٰلَمِيۡنَ ﶫ"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "لِمَنۡ شَآءَ مِنۡكُمۡ اَنۡ يَّسۡتَقِيۡمَ ﶠ"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "وَمَا تَشَآءُوۡنَ اِلَّا٘ اَنۡ يَّشَآءَ اللّٰهُ رَبُّ الۡعٰلَمِيۡنَ ﶒ"
        }
    ]
}