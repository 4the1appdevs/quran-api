{
    "hizbs": [
        {
            "chapter": 87,
            "verse": 1,
            "text": "ﶒ ؀سَبِّحِ اسْمَ رَبِّكَ الْاَعْلَي"
        },
        {
            "chapter": 87,
            "verse": 2,
            "text": "ﶫ الَّذِيْ خَلَقَ فَسَوّٰي"
        },
        {
            "chapter": 87,
            "verse": 3,
            "text": "ﶟ وَالَّذِيْ قَدَّرَ فَهَدٰي"
        },
        {
            "chapter": 87,
            "verse": 4,
            "text": "ﶟ وَالَّذِيْ٘ اَخْرَجَ الْمَرْعٰي"
        },
        {
            "chapter": 87,
            "verse": 5,
            "text": "ﶟ فَجَعَلَهٗ غُثَآءً اَحْوٰي"
        },
        {
            "chapter": 87,
            "verse": 6,
            "text": "ﶠ سَنُقْرِئُكَ فَلَا تَنْسٰ٘ي"
        },
        {
            "chapter": 87,
            "verse": 7,
            "text": "ﶫ اِلَّا مَا شَآءَ اللّٰهُﵧ اِنَّهٗ يَعْلَمُ الْجَهْرَ وَمَا يَخْفٰي"
        },
        {
            "chapter": 87,
            "verse": 8,
            "text": "ﶠ وَنُيَسِّرُكَ لِلْيُسْرٰي"
        },
        {
            "chapter": 87,
            "verse": 9,
            "text": "ﶗ فَذَكِّرْ اِنْ نَّفَعَتِ الذِّكْرٰي"
        },
        {
            "chapter": 87,
            "verse": 10,
            "text": "ﶠ سَيَذَّكَّرُ مَنْ يَّخْشٰي"
        },
        {
            "chapter": 87,
            "verse": 11,
            "text": "ﶫ وَيَتَجَنَّبُهَا الْاَشْقَي"
        },
        {
            "chapter": 87,
            "verse": 12,
            "text": "ﶫ الَّذِيْ يَصْلَي النَّارَ الْكُبْرٰي"
        },
        {
            "chapter": 87,
            "verse": 13,
            "text": "ﶔ ثُمَّ لَا يَمُوْتُ فِيْهَا وَلَا يَحْيٰي"
        },
        {
            "chapter": 87,
            "verse": 14,
            "text": "ﶠ قَدْ اَفْلَحَ مَنْ تَزَكّٰي"
        },
        {
            "chapter": 87,
            "verse": 15,
            "text": "ﶫ وَذَكَرَ اسْمَ رَبِّهٖ فَصَلّٰي"
        },
        {
            "chapter": 87,
            "verse": 16,
            "text": "ﶠ بَلْ تُؤْثِرُوْنَ الْحَيٰوةَ الدُّنْيَا"
        },
        {
            "chapter": 87,
            "verse": 17,
            "text": "ﶫ وَالْاٰخِرَةُ خَيْرٌ وَّاَبْقٰي"
        },
        {
            "chapter": 87,
            "verse": 18,
            "text": "ﶠ اِنَّ هٰذَا لَفِي الصُّحُفِ الْاُوْلٰي"
        },
        {
            "chapter": 87,
            "verse": 19,
            "text": "ﶫ صُحُفِ اِبْرٰهِيْمَ وَمُوْسٰي"
        },
        {
            "chapter": 88,
            "verse": 1,
            "text": "ﶒ ؀هَلْ اَتٰىكَ حَدِيْثُ الْغَاشِيَةِ"
        },
        {
            "chapter": 88,
            "verse": 2,
            "text": "ﶠ وُجُوْهٌ يَّوْمَئِذٍ خَاشِعَةٌ"
        },
        {
            "chapter": 88,
            "verse": 3,
            "text": "ﶫ عَامِلَةٌ نَّاصِبَةٌ"
        },
        {
            "chapter": 88,
            "verse": 4,
            "text": "ﶫ تَصْلٰي نَارًا حَامِيَةً"
        },
        {
            "chapter": 88,
            "verse": 5,
            "text": "ﶫ تُسْقٰي مِنْ عَيْنٍ اٰنِيَةٍ"
        },
        {
            "chapter": 88,
            "verse": 6,
            "text": "ﶠ لَيْسَ لَهُمْ طَعَامٌ اِلَّا مِنْ ضَرِيْعٍ"
        },
        {
            "chapter": 88,
            "verse": 7,
            "text": "ﶫ لَّا يُسْمِنُ وَلَا يُغْنِيْ مِنْ جُوْعٍ"
        },
        {
            "chapter": 88,
            "verse": 8,
            "text": "ﶠ وُجُوْهٌ يَّوْمَئِذٍ نَّاعِمَةٌ"
        },
        {
            "chapter": 88,
            "verse": 9,
            "text": "ﶫ لِّسَعْيِهَا رَاضِيَةٌ"
        },
        {
            "chapter": 88,
            "verse": 10,
            "text": "ﶫ فِيْ جَنَّةٍ عَالِيَةٍ"
        },
        {
            "chapter": 88,
            "verse": 11,
            "text": "ﶫ لَّا تَسْمَعُ فِيْهَا لَاغِيَةً"
        },
        {
            "chapter": 88,
            "verse": 12,
            "text": "ﶠ فِيْهَا عَيْنٌ جَارِيَةٌ"
        },
        {
            "chapter": 88,
            "verse": 13,
            "text": "ﶭ فِيْهَا سُرُرٌ مَّرْفُوْعَةٌ"
        },
        {
            "chapter": 88,
            "verse": 14,
            "text": "ﶫ وَّاَكْوَابٌ مَّوْضُوْعَةٌ"
        },
        {
            "chapter": 88,
            "verse": 15,
            "text": "ﶫ وَّنَمَارِقُ مَصْفُوْفَةٌ"
        },
        {
            "chapter": 88,
            "verse": 16,
            "text": "ﶫ وَّزَرَابِيُّ مَبْثُوْثَةٌ"
        },
        {
            "chapter": 88,
            "verse": 17,
            "text": "ﶠ اَفَلَا يَنْظُرُوْنَ اِلَي الْاِبِلِ كَيْفَ خُلِقَتْ"
        },
        {
            "chapter": 88,
            "verse": 18,
            "text": "ﷃ وَاِلَي السَّمَآءِ كَيْفَ رُفِعَتْ"
        },
        {
            "chapter": 88,
            "verse": 19,
            "text": "ﷃ وَاِلَي الْجِبَالِ كَيْفَ نُصِبَتْ"
        },
        {
            "chapter": 88,
            "verse": 20,
            "text": "ﷃ وَاِلَي الْاَرْضِ كَيْفَ سُطِحَتْ"
        },
        {
            "chapter": 88,
            "verse": 21,
            "text": "فَذَكِّرْﵴ اِنَّمَا٘ اَنْتَ مُذَكِّرٌ"
        },
        {
            "chapter": 88,
            "verse": 22,
            "text": "ﶠ لَسْتَ عَلَيْهِمْ بِمُصَۜيْطِرٍ"
        },
        {
            "chapter": 88,
            "verse": 23,
            "text": "ﶫ اِلَّا مَنْ تَوَلّٰي وَكَفَرَ"
        },
        {
            "chapter": 88,
            "verse": 24,
            "text": "ﶫ فَيُعَذِّبُهُ اللّٰهُ الْعَذَابَ الْاَكْبَرَ"
        },
        {
            "chapter": 88,
            "verse": 25,
            "text": "ﶠ اِنَّ اِلَيْنَا٘ اِيَابَهُمْ"
        },
        {
            "chapter": 88,
            "verse": 26,
            "text": "ﶫ ثُمَّ اِنَّ عَلَيْنَا حِسَابَهُمْ"
        },
        {
            "chapter": 89,
            "verse": 1,
            "text": "ﶒ ؀وَالْفَجْرِ"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "ﶫ وَلَيَالٍ عَشْرٍ"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "ﶫ وَّالشَّفْعِ وَالْوَتْرِ"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "ﶫ وَالَّيْلِ اِذَا يَسْرِ"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "ﶔ هَلْ فِيْ ذٰلِكَ قَسَمٌ لِّذِيْ حِجْرٍ"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "ﶠ اَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "ﷱ اِرَمَ ذَاتِ الْعِمَادِ"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "ﷱ الَّتِيْ لَمْ يُخْلَقْ مِثْلُهَا فِي الْبِلَادِ"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "ﷱ وَثَمُوْدَ الَّذِيْنَ جَابُوا الصَّخْرَ بِالْوَادِ"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "ﷱ وَفِرْعَوْنَ ذِي الْاَوْتَادِ"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "ﷱ الَّذِيْنَ طَغَوْا فِي الْبِلَادِ"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "ﷱ فَاَكْثَرُوْا فِيْهَا الْفَسَادَ"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "ﷱ فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "ﷲ اِنَّ رَبَّكَ لَبِالْمِرْصَادِ"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "ﶠ فَاَمَّا الْاِنْسَانُ اِذَا مَا ابْتَلٰىهُ رَبُّهٗ فَاَكْرَمَهٗ وَنَعَّمَهٗﵿ فَيَقُوْلُ رَبِّيْ٘ اَكْرَمَنِ"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "ﶠ وَاَمَّا٘ اِذَا مَا ابْتَلٰىهُ فَقَدَرَ عَلَيْهِ رِزْقَهٗﵿ فَيَقُوْلُ رَبِّيْ٘ اَهَانَنِ"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "ﶔ كَلَّا بَلْ لَّا تُكْرِمُوْنَ الْيَتِيْمَ"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "ﶫ وَلَا تَحٰٓضُّوْنَ عَلٰي طَعَامِ الْمِسْكِيْنِ"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "ﶫ وَتَاْكُلُوْنَ التُّرَاثَ اَكْلًا لَّمًّا"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "ﶫ وَّتُحِبُّوْنَ الْمَالَ حُبًّا جَمًّا"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "ﶠ كَلَّا٘ اِذَا دُكَّتِ الْاَرْضُ دَكًّا دَكًّا"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "ﶫ وَّجَآءَ رَبُّكَ وَالْمَلَكُ صَفًّا صَفًّا"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "ﶔ وَجِايْٓءَ يَوْمَئِذٍۣ بِجَهَنَّمَﵿ يَوْمَئِذٍ يَّتَذَكَّرُ الْاِنْسَانُ وَاَنّٰي لَهُ الذِّكْرٰي"
        },
        {
            "chapter": 89,
            "verse": 24,
            "text": "ﶠ يَقُوْلُ يٰلَيْتَنِيْ قَدَّمْتُ لِحَيَاتِيْ"
        },
        {
            "chapter": 89,
            "verse": 25,
            "text": "ﶔ فَيَوْمَئِذٍ لَّا يُعَذِّبُ عَذَابَهٗ٘ اَحَدٌ"
        },
        {
            "chapter": 89,
            "verse": 26,
            "text": "ﶫ وَّلَا يُوْثِقُ وَثَاقَهٗ٘ اَحَدٌ"
        },
        {
            "chapter": 89,
            "verse": 27,
            "text": "ﶠ يٰ٘اَيَّتُهَا النَّفْسُ الْمُطْمَئِنَّةُ"
        },
        {
            "chapter": 89,
            "verse": 28,
            "text": "ﷄ ارْجِعِيْ٘ اِلٰي رَبِّكِ رَاضِيَةً مَّرْضِيَّةً"
        },
        {
            "chapter": 89,
            "verse": 29,
            "text": "ﶔ فَادْخُلِيْ فِيْ عِبٰدِيْ"
        },
        {
            "chapter": 89,
            "verse": 30,
            "text": "ﶫ وَادْخُلِيْ جَنَّتِيْ"
        }
    ]
}