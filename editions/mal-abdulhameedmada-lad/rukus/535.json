{
    "rukus": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "93|1|pūrvvāhnaṁ tanneyāṇa satyaṁ"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "93|2|rātri tanneyāṇa satyaṁ; at śāntamāvumpēāḷ"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "93|3|(nabiyē,) ninṟe rakṣitāv ninne kaiviṭṭiṭṭilla. veṟuttiṭṭumilla"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "93|4|tīrccayāyuṁ paralēākamāṇ ninakk ihalēākattekkāḷ uttamamāyiṭṭuḷḷat‌"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "93|5|vaḻiye ninakk ninṟe rakṣitāv (anugrahaṅṅaḷ) nalkunnatuṁ appēāḷ nī tr̥ptippeṭunnatumāṇa.‌"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "93|6|ninne avan oru anāthayāyi kaṇeṭattukayuṁ, enniṭṭ (ninakk‌) āśrayaṁ nalkukayuṁ ceytillē"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "93|7|ninne avan vaḻi aṟiyāttavanāyi kaṇeṭattukayuṁ enniṭṭ (ninakk‌) mārgadarśanaṁ nalkukayuṁ ceytirikkunnu"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "93|8|ninne avan daridranāyi kaṇeṭattukayuṁ enniṭṭ avan aiśvaryaṁ nalkukayuṁ ceytirikkunnu"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "93|9|ennirikke anāthaye nī aṭiccamarttarut‌"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "93|10|cēādicc varunnavane nī viraṭṭi viṭukayuṁ ceyyarut‌"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "93|11|ninṟe rakṣitāvinṟe anugrahatte sambandhicc nī sansārikkuka"
        }
    ]
