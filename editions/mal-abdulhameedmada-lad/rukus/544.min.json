{"rukus":[{"chapter":102,"verse":1,"text":"102|1|parasparaṁ perumanaṭikkuka enna kāryaṁ niṅṅaḷe aśrad'dhayilākkiyirikkunnu"},{"chapter":102,"verse":2,"text":"102|2|niṅṅaḷ śavakuṭīraṅṅaḷ sandarśikkunnat varēkkuṁ"},{"chapter":102,"verse":3,"text":"102|3|nis'sanśayaṁ, niṅṅaḷ vaḻiye aṟiññ keāḷḷuṁ"},{"chapter":102,"verse":4,"text":"102|4|pinneyuṁ nis'sanśayaṁ niṅṅaḷ vaḻiye aṟiññ keāḷḷuṁ"},{"chapter":102,"verse":5,"text":"102|5|nis'sanśayaṁ, niṅṅaḷ dr̥ḍhamāya aṟiv aṟiyumāyirunneṅkil"},{"chapter":102,"verse":6,"text":"102|6|jvalikkunna narakatte niṅṅaḷ kāṇuka tanne ceyyuṁ"},{"chapter":102,"verse":7,"text":"102|7|pinne tīrccayāyuṁ niṅṅaḷ atine dr̥ḍhamāyuṁ kaṇṇāl kāṇuka tanne ceyyuṁ"},{"chapter":102,"verse":8,"text":"102|8|pinnīṭ ā divasattil sukhānubhavaṅṅaḷe paṟṟi tīrccayāyuṁ niṅṅaḷ cēādyaṁ ceyyappeṭukatanne ceyyuṁ"}]}