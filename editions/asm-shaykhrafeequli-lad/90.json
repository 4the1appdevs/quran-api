{
    "chapter": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "90|1|Ma'i śapata karaichō ē'i nagarakhanara"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "90|2|ārau tumi ē'i nagarara adhibāsī"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "90|3|śapata janmadātāra ārau yiṭō si janma dichē."
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "90|4|Nisandēhē āmi mānuhaka sr̥ṣṭi karaichō kaṭhōra dukha-kaṣṭara mājata."
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "90|5|Si bhābē nēki yē, kētiẏā'ō tāra ōparata kōnōraē'i kṣamatābāna naha’ba"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "90|6|si kaẏa, 'ma'i pracura dhana-sampada nāśa karaichō'."
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "90|7|Si bhābē nēki yē, kōnē'ō tāka dēkhā nā'i"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "90|8|tāra bābē āmi duṭā caku sr̥ṣṭi karaā nā'inē"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "90|9|ēkhana jibhā ārau duṭā ōm̐ṭha"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "90|10|ārau āmi tāka duṭā patha dēkhuraā'ichō."
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "90|11|Kintu si durgama giraipathata praraēśa karaā nā'i."
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "90|12|Ārau tōmāka kihē janāba yē, durgama giraipatha ki"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "90|13|sē'iṭō haichē, dāsamukti."
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "90|14|Athabā durbhikṣara dinata khādyadāna karaā"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "90|15|nikaṭātmīẏa ētīmaka"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "90|16|athabā daraidra-niṣpēṣita abhāragrastaka"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "90|17|tāra pichata si ēnēlōkara antarbhūkta hai yāẏa yisakalē īmāna ānichē ārau parasparaka dhairya dhāraṇa karaibalai upadēśa dichē ārau upadēśa dichē parasparaka daẏā-anugraha karaibalai"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "90|18|ē'ōm̐lōkē'i saubhāgyaśālī."
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "90|19|Ārau yisakalē āmāra āẏātasamūhaka asbīkāra karaichē, siham̐tē'i hatabhāgya."
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "90|20|Siham̐ta abaraud'dha ju'ita paraibēṣṭita thākiba."
        }
    ]
}