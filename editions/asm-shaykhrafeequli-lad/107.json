{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "107|1|Tumi tāka dēkhichānē yiẏē hicāpa pratidānaka asbīkāra karaē"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "107|2|si sē'ijanē'i, yiẏē anāthaka kaṭhōrabhāraē khēdi paṭhiẏāẏa"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "107|3|ārau si dukhiẏāka khānā khuraābalai uṯsāha nidiẏē."
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "107|4|Ētēkē dhbansa sē'ibidha chālāta ādāẏakāraīsakalara"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "107|5|yisakalē nijara chālātata amanōyōgī"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "107|6|yisakalē ānaka dēkhuraābalaihē karaē"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "107|7|ārau sarau-suraā gr̥hasāmagraī pradānata bādhā diẏē."
        }
    ]
}