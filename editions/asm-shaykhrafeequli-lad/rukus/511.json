{
    "rukus": [
        {
            "chapter": 74,
            "verse": 32,
            "text": "74|32|Kētiẏā'ō nahaẏa, candrara śapata"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "74|33|raātira śapata, yētiẏā iẏāra arasāna ghaṭē"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "74|34|prabhātara śapata, yētiẏā i ālōkōjjbala haẏa"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "74|35|niścaẏa jāhānnāma mahābipadasamūhara an'yatama"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "74|36|mānuhara bābē satarkakāraīsbaraūpa"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "74|37|Tōmālōkara mājata yiẏē agrasara ha’ba bicāraē athabā pichamuraā ha’ba bicāraē tāra bābē."
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "74|38|Pratyēka byakti nija kr̥takarmara bābē dāẏabad'dha"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "74|39|kintu sōm̐panthī lōkasakalara bāhiraē"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "74|40|jānnātata tē'ōm̐lōkē ijanē sijanaka sudhiba"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "74|41|aparaādhībilākara biṣaẏē"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "74|42|kihē tōmālōkaka chākbārata (jāhānnāmata) nikṣēpa karaichē"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "74|43|siham̐tē ka’ba, ‘āmi chālāta ādāẏakāraīsakalara antarbhukta nāchilō"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "74|44|‘ārau āmi dukhīẏāka khādya dāna karaā nāchilō"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "74|45|ārau āmi anarthaka ālōcanākāraīsakalara lagata bēhudā ālaca karaāta magna āchilō."
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "74|46|Ārau āmi pratidāna dirasaka asbīkāra karaichilō"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "74|47|araśēṣata āmāra ōcarata mr̥tyu āhi pālē’."
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "74|48|Ētēkē chupāraichakāraīsakalara chupāraichē siham̐tara kōnō upakāra nakaraiba."
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "74|49|Ētēkē siham̐tara ki haichē, kiẏa siham̐tē upadēśa bāṇīra paraā bimukha haichē"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "74|50|siham̐tē yēna bhīta-santrasta hai palāẏanarata banaraīẏā gādha."
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "74|51|Yibōraē sinhara bhaẏata palāẏana karaichē."
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "74|52|Baraṁ siham̐tara mājara pratyēka byaktiẏē'i kāmanā karaē yē tāka unmukta grantha pradāna karaā ha'ōka."
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "74|53|Kētiẏā'ō nahaẏa, baraṁ siham̐tē ākhiraātaka bhaẏa nakaraē."
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "74|54|Kētiẏā'ō nahaẏa, niścaẏa ē'i kōra'āna sakalōraē bābē upadēśabāṇī."
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "74|55|Ētēkē yāra icchā si iẏāra paraā upadēśa grahaṇa karaka."
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "74|56|Ārau āllāhara icchābihīna kōnōraē'i upadēśa grahaṇa karaiba nōraāraē tēraēm̐i bhaẏara yōgya, kērala tē'ōm̐raē'i tākbaraā aralambana karaā, ārau tēraēm̐i kṣamā karaāra adhikāraī."
        }
    ]
}