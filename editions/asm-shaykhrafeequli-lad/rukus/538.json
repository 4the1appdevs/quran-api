{
    "rukus": [
        {
            "chapter": 96,
            "verse": 1,
            "text": "96|1|paṛhā tōmāra pratipālakara nāmata, yiẏē sr̥ṣṭi karaichē"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "96|2|tē'ōm̐ mānuhaka ālākbara paraā sr̥ṣṭi karaichē."
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "96|3|Paṛhā, kāraṇa tōmāra pratipālaka mahāmahima."
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "96|4|Yijanē kalamara sahāẏata śikṣā dichē."
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "96|5|Tē'ōm̐ mānuhaka ēnē śikṣā dichē, yiṭō mānuhē janā nāchila."
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "96|6|Bāstaratē, mānuhē sīmālaṅghana karai thākē."
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "96|7|Kāraṇa si nijakē sbanirbharaśīla buli bhāraē."
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "96|8|Niścaẏa tōmāra pratipālakara ōcaratē'i pratyārartana."
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "96|9|Tumi tāka dēkhā nā'inē? Yiẏē niṣēdha karaē."
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "96|10|Ējana bāndāka, yētiẏā tē'ōm̐ chālāta ādāẏa karaē."
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "96|11|Kōraācōna, yadi'ō tē'ōm̐ hidāẏatara ōparata pratiṣṭhita"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "96|12|athabā tākbaraāra nirdēśa diẏē; (tathāpi'ō tē'ōm̐ka kēnēkai bādhā diẏē)"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "96|13|kōraācōna yadi si (niṣēdhakāraī) asbīkāra karaē ārau mukha ghūraā'i laẏa"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "96|14|si nājānē nēki, niścaẏa āllāhē dēkhi āchē"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "96|15|kētiẏā'ō nahaẏa, yadi si birata nāthākē, tēntē niścaẏa āmi tāka āgaculita dharai ṭāni ānima."
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "96|16|Michalīẏā, pāpiṣṭhara āgaculi (kapāla)."
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "96|17|Ētēkē si nija paraiṣadaka māti ānaka."
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "96|18|Anatipalamē āmi māti ānima jāhānnāmara kaṭhōra praharaīsakalaka."
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "96|19|Kētiẏā'ō nahaẏa, tumi tāra anusaraṇa nakaraibā. Ārau tumi (tōmāra pratipālakaka) chājadā karaā ārau naikaṭya lābha karaā."
        }
    ]
}