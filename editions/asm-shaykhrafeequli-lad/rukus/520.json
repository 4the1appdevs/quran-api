{
    "rukus": [
        {
            "chapter": 79,
            "verse": 1,
            "text": "79|1|Śapata nirmamabhāraē utpaṭanakāraīsakalara."
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "79|2|Ārau mr̥dubhāraē bandhanamūktakāraīsakalara."
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "79|3|Ārau tībragatita sām̐turai phuraāsakalara."
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "79|4|Ārau drautagatita āgabāṛhi yōraāsakalara."
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "79|5|Tāra pichata yāratīẏa kāma nirbāhakāraīsakalara."
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "79|6|Sē'idinā kampanakāraīẏē prakampita karaiba."
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "79|7|Tāka anusaraṇa karaiba parabartī kampanakāraī."
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "79|8|Sē'idinā kichumāna hr̥daẏa, bhaẏata santrasta ha’ba."
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "79|9|Siham̐tara dr̥ṣṭisamūha ātaṅkita hai nata ha’ba."
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "79|10|Siham̐tē kaẏa, ‘āmi ākau āgara arasthāta ubhati yāma nē"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "79|11|yētiẏā āmi cūrṇabicūrṇa asthita paraiṇata hama’"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "79|12|siham̐tē kaẏa, ‘yadi ē'iṭōraē haẏa tēntē ē'iṭō ēṭā kṣatikara pratyārartana’."
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "79|13|Prakr̥tatē ē'iṭō ēṭā biraāṭa śabda."
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "79|14|Tatkṣaṇāta bhū-pr̥ṣṭhata siham̐tara ābirbhāra ha’ba."
        },
        {
            "chapter": 79,
            "verse": 15,
            "text": "79|15|Mūchāra ghaṭanāṭō tumi pā'ichānē"
        },
        {
            "chapter": 79,
            "verse": 16,
            "text": "79|16|yētiẏā tē'ōm̐ra pratipālakē tē'ōm̐ka paraitra turaā upatyakāta māti kaichila"
        },
        {
            "chapter": 79,
            "verse": 17,
            "text": "79|17|‘phira'ā'unara ōcaralai yōraā niścaẏa si sīmālaṅghana karaichē"
        },
        {
            "chapter": 79,
            "verse": 18,
            "text": "79|18|tāra pichata tāka kōraā ‘tumi paraitra ha’ba bicaraā nē"
        },
        {
            "chapter": 79,
            "verse": 19,
            "text": "79|19|ārau ma'i tōmāka tōmāra pratipālakara pinē pathapradarśana karaō, yātē tumi tē'ōm̐ka bhaẏa karaā’."
        },
        {
            "chapter": 79,
            "verse": 20,
            "text": "79|20|Tāra pichata, tē'ōm̐ tāka mahā nidarśana dēkhuraālē."
        },
        {
            "chapter": 79,
            "verse": 21,
            "text": "79|21|Kintu si asbīkāra karailē ārau amān'ya karailē."
        },
        {
            "chapter": 79,
            "verse": 22,
            "text": "79|22|Tāra pichata tē'ōm̐ si pichaphālē ubhati pratibidhānata sacēṣṭa ha’la."
        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "79|23|Tāra pichata sakalōkē ēkatraita karai ghōṣaṇā karailē"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "79|24|ārau ka’lē, ‘maẏē tōmālōkara sarbōcca pratipālaka’."
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "79|25|Araśēṣata āllāhē tāka ākhiraāta ārau pr̥thiraīra dr̥ṣṭāntamūlaka śāsti pradāna karailē."
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "79|26|Niścaẏa yiẏē bhaẏa karaē tāra bābē iẏāta śikṣā āchē."
        }
    ]
}