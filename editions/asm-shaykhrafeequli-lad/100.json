{
    "chapter": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "100|1|Śapata ūrdhbaśbāsata daurai yōraā ghōm̐raāsamūhara"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "100|2|yibōraē kṣurara āghātata phiraiṅati urauraāẏa"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "100|3|yibōraē prabhātakālata ākramaṇa karaē"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "100|4|phalata siham̐tē iẏāra dbāraā dhūli urauraāẏa"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "100|5|ārau iẏāra dbāraā śatrau dalara abhyantarata sōmā'i paraē."
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "100|6|Niścaẏa mānuha tāra pratipālakara prati atikai akr̥tajña"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "100|7|niścaẏa si ē'i biṣaẏē sākṣī"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "100|8|ārau niścaẏa si dhana-sampadara lōbhata prabala."
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "100|9|Si nājānē nēki yē, kabarata yi āchē sēẏā ut'thita ha’ba"
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "100|10|Ārau antarata yi āchē sēẏā prakāśita ha’ba"
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "100|11|niścaẏa siham̐tara pratipālaka sē'idinā siham̐tara arasthā samparkē sabiśēṣa arahita."
        }
    ]
}