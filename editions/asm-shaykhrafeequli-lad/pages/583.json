    "pages": [
        {
            "chapter": 78,
            "verse": 31,
            "text": "78|31|Niścaẏa muttākbīsakalara bābē āchē saphalatā."
        },
        {
            "chapter": 78,
            "verse": 32,
            "text": "78|32|Udyānasamūha ārau āṅgurasamūha."
        },
        {
            "chapter": 78,
            "verse": 33,
            "text": "78|33|Ārau samabaẏasīẏā uda`bhinna nara-yuratī tarauṇī."
        },
        {
            "chapter": 78,
            "verse": 34,
            "text": "78|34|Ārau paraipūrṇa pānapātra."
        },
        {
            "chapter": 78,
            "verse": 35,
            "text": "78|35|Tāta tē'ōm̐lōkē kōnō anarthaka kathā ārau michā bākya'ō śunā nāpāba."
        },
        {
            "chapter": 78,
            "verse": 36,
            "text": "78|36|Āpōnāra pratipālakara phālara paraā puraṣkāra, yathōcita dānasbaraūpa."
        },
        {
            "chapter": 78,
            "verse": 37,
            "text": "78|37|Yijana ākāśasamūha ārau pr̥thiraī ārau ē'i duṭāra mājata yi āchē sakalōraē pratipālaka, parama karauṇāmaẏa, tē'ōm̐ra ōcarata ābēdana-nibēdanara śakti siham̐tara kāraō nāthākiba."
        },
        {
            "chapter": 78,
            "verse": 38,
            "text": "78|38|Sē'idinā raūha ārau phiraistāsakala śāraībad'dhabhāraē thiẏa ha’ba; rahamānē yāka anumati diba tāra bāhiraē kōnē'ō kathā naka’ba, ārau tē'ōm̐ saṭhika kathā ka’ba."
        },
        {
            "chapter": 78,
            "verse": 39,
            "text": "78|39|Ē'i dinaṭō satya, ētēkē yāra icchā siẏē tāra pratipālakara ōcarata āśraẏa grahaṇa karaka."
        },
        {
            "chapter": 78,
            "verse": 40,
            "text": "78|40|Niścaẏa āmi tōmālōkaka ati nikaṭarartī śāsti samparkē satarka karailō. Yidinā mānuhē nija kr̥takarma dēkhibalai pāba ārau kāphiraē ka’ba, hāẏa! Ma'i yadi māṭi ha'lōhēm̐tēna."
        },
        {
            "chapter": 79,
            "verse": 1,
            "text": "79|1|Śapata nirmamabhāraē utpaṭanakāraīsakalara."
        },
        {
            "chapter": 79,
            "verse": 2,
            "text": "79|2|Ārau mr̥dubhāraē bandhanamūktakāraīsakalara."
        },
        {
            "chapter": 79,
            "verse": 3,
            "text": "79|3|Ārau tībragatita sām̐turai phuraāsakalara."
        },
        {
            "chapter": 79,
            "verse": 4,
            "text": "79|4|Ārau drautagatita āgabāṛhi yōraāsakalara."
        },
        {
            "chapter": 79,
            "verse": 5,
            "text": "79|5|Tāra pichata yāratīẏa kāma nirbāhakāraīsakalara."
        },
        {
            "chapter": 79,
            "verse": 6,
            "text": "79|6|Sē'idinā kampanakāraīẏē prakampita karaiba."
        },
        {
            "chapter": 79,
            "verse": 7,
            "text": "79|7|Tāka anusaraṇa karaiba parabartī kampanakāraī."
        },
        {
            "chapter": 79,
            "verse": 8,
            "text": "79|8|Sē'idinā kichumāna hr̥daẏa, bhaẏata santrasta ha’ba."
        },
        {
            "chapter": 79,
            "verse": 9,
            "text": "79|9|Siham̐tara dr̥ṣṭisamūha ātaṅkita hai nata ha’ba."
        },
        {
            "chapter": 79,
            "verse": 10,
            "text": "79|10|Siham̐tē kaẏa, ‘āmi ākau āgara arasthāta ubhati yāma nē"
        },
        {
            "chapter": 79,
            "verse": 11,
            "text": "79|11|yētiẏā āmi cūrṇabicūrṇa asthita paraiṇata hama’"
        },
        {
            "chapter": 79,
            "verse": 12,
            "text": "79|12|siham̐tē kaẏa, ‘yadi ē'iṭōraē haẏa tēntē ē'iṭō ēṭā kṣatikara pratyārartana’."
        },
        {
            "chapter": 79,
            "verse": 13,
            "text": "79|13|Prakr̥tatē ē'iṭō ēṭā biraāṭa śabda."
        },
        {
            "chapter": 79,
            "verse": 14,
            "text": "79|14|Tatkṣaṇāta bhū-pr̥ṣṭhata siham̐tara ābirbhāra ha’ba."
        },
        {
