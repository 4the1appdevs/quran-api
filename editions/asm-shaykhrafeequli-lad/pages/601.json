{
    "pages": [
        {
            "chapter": 103,
            "verse": 1,
            "text": "103|1|Samaẏara śapata"
        },
        {
            "chapter": 103,
            "verse": 2,
            "text": "103|2|niścaẏa mānuha kṣatigrasthatāta nipatita"
        },
        {
            "chapter": 103,
            "verse": 3,
            "text": "103|3|kintu tē'ōm̐lōkara bāhiraē, yisakalē īmāna ānichē ārau satkāma karaichē lagatē (yisakalē) parasparaka satyara upadēśa diẏē ārau dhairya dhāraṇa karaibalai utsāha diẏē."
        },
        {
            "chapter": 104,
            "verse": 1,
            "text": "104|1|Durbhōga pratyēka sē'i byaktira bābē yiẏē sanmukhata ārau anupasthitita paranindā karaē."
        },
        {
            "chapter": 104,
            "verse": 2,
            "text": "104|2|Yiẏē sampada jamā karaē ārau bāraē bāraē gaṇanā karaē."
        },
        {
            "chapter": 104,
            "verse": 3,
            "text": "104|3|Si dhāraṇā karaē yē, tāra sampadē tāka cirajīrai karai raākhiba."
        },
        {
            "chapter": 104,
            "verse": 4,
            "text": "104|4|Kētiẏā'ō nahaẏa, niścaẏa si hutāmāta nikṣipta ha’ba."
        },
        {
            "chapter": 104,
            "verse": 5,
            "text": "104|5|Ārau tumi jānānē, hutāmā ki"
        },
        {
            "chapter": 104,
            "verse": 6,
            "text": "104|6|i āllāhara prajbalita ju'i."
        },
        {
            "chapter": 104,
            "verse": 7,
            "text": "104|7|Yiẏē hr̥dapiṇḍaka graāsa karaiba."
        },
        {
            "chapter": 104,
            "verse": 8,
            "text": "104|8|Niścaẏa i siham̐taka ārad'dha karai raākhiba."
        },
        {
            "chapter": 104,
            "verse": 9,
            "text": "104|9|Ōkha ōkha stambhasamūhata."
        },
        {
            "chapter": 105,
            "verse": 1,
            "text": "105|1|Tumi dēkhā nā'inē, tōmāra pratipālakē hātīra adhipatisakalara lagata ki karaichila"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "105|2|tē'ōm̐ siham̐tara ṣaṛhayantra byarthatāta paryabasita karaā nā'inē"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "105|3|Ārau siham̐tara biraud'dhē tē'ōm̐ jākē jākē caraā'i paṭhi'ōraā nāchilanē"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "105|4|yisakalē siham̐tara ōparata pakā ṭāna māṭira śilaguṭi nikṣēpa karaichila"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "105|5|phalata tē'ōm̐ siham̐taka bhakṣita tr̥ṇa-sadr̥śa karaichila."
        }
    ]
}