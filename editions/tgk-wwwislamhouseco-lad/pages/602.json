{
    "pages": [
        {
            "chapter": 106,
            "verse": 1,
            "text": "106|1|Baroi [siposguzorī az] hamʙastagī va unsi Qurajş"
        },
        {
            "chapter": 106,
            "verse": 2,
            "text": "106|2|[Hamon] Hamʙastagī va unsi onon dar safarhoi zimistonī [ʙa Jaman] va toʙistonī [ʙa Şom]"
        },
        {
            "chapter": 106,
            "verse": 3,
            "text": "106|3|Bojad Parvardigori xonai Ka'ʙaro ʙiparastand"
        },
        {
            "chapter": 106,
            "verse": 4,
            "text": "106|4|Hamon [Parvardigore], ki ononro az gurusnagī [rahonid, ʙa eşon] xūrok dod va az vahşatu tars emin namud"
        },
        {
            "chapter": 107,
            "verse": 1,
            "text": "107|1|[Ej Pajomʙar] Ojo kase, ki rūzi çazoro durūƣ meangorad, didaī"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "107|2|Pas ū [hamon] kasest, ki jatimro [ʙo duruştī az xud] meronad"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "107|3|Va [digaronro] ʙa it'omi tihidast taşviq namekunad"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "107|4|Pas, voj ʙar namozguzoron"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "107|5|Hamon kasone, ki az namozaşon ƣofiland"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "107|6|Kasone, ki xudnamoī mekunand"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "107|7|Va [az amonat dodani] vasoili zarurii zindagī dareƣ mevarzand"
        },
        {
            "chapter": 108,
            "verse": 1,
            "text": "108|1|[Ej Pajomʙar] Ba rostī, ki Mo ʙa tu xajri ʙenihojat ato kardem"
        },
        {
            "chapter": 108,
            "verse": 2,
            "text": "108|2|Pas, ʙaroi Parvardigorat namoz ʙixon va qurʙonī kun"
        },
        {
            "chapter": 108,
            "verse": 3,
            "text": "108|3|Be tardid, duşmanat [az hamai xajrotu ʙarakot ʙeʙahra va] ʙurida ast"
        }
    ]
}