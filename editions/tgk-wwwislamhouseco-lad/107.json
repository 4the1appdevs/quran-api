{
    "chapter": [
        {
            "chapter": 107,
            "verse": 1,
            "text": "107|1|[Ej Pajomʙar] Ojo kase, ki rūzi çazoro durūƣ meangorad, didaī"
        },
        {
            "chapter": 107,
            "verse": 2,
            "text": "107|2|Pas ū [hamon] kasest, ki jatimro [ʙo duruştī az xud] meronad"
        },
        {
            "chapter": 107,
            "verse": 3,
            "text": "107|3|Va [digaronro] ʙa it'omi tihidast taşviq namekunad"
        },
        {
            "chapter": 107,
            "verse": 4,
            "text": "107|4|Pas, voj ʙar namozguzoron"
        },
        {
            "chapter": 107,
            "verse": 5,
            "text": "107|5|Hamon kasone, ki az namozaşon ƣofiland"
        },
        {
            "chapter": 107,
            "verse": 6,
            "text": "107|6|Kasone, ki xudnamoī mekunand"
        },
        {
            "chapter": 107,
            "verse": 7,
            "text": "107|7|Va [az amonat dodani] vasoili zarurii zindagī dareƣ mevarzand"
        }
    ]
}