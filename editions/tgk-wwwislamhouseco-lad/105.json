{
    "chapter": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "105|1|[Ej Pajomʙar] Ojo nadidī Parvardigorat ʙo ashoʙi fil cī kard"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "105|2|Ojo [fireʙu] naqşai ononro dar taʙohī qaror nadod"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "105|3|Va parandagone gurūh-gurūh ʙar [sari] onon firistod"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "105|4|[Ki] Bo sanggil [ki dar minqoru cangol doştand] ononro sangʙoron mekardand"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "105|5|Pas, onhoro hamcun kohi çavidaşuda soxt"
        }
    ]
}