{
    "rukus": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "112|1|[Ej Pajomʙar] Bigū: «Ū Alloh taolo jaktost"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "112|2|Alloh taolo ʙenijoz ast [va hama nijozmandi Ū hastand]"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "112|3|Na [farzande] zoda va na zoda şudaast"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "112|4|Va heç kas hammonandi va hamtoi Ū naʙuda va nest»"
        }
    ]
}