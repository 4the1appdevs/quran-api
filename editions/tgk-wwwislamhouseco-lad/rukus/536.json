{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "94|1|[Ej pajomʙar] Ojo Mo sinaatro [ʙaroi nuzuli vahj] ʙarojat nakuşudem"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "94|2|Va sangini [-i ʙori gunoh va duşvorii risolat]-ro az [dūşi] tu ʙarnadoştem"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "94|3|Hamon [ʙore], ki ʙar puştat sanginī mekard"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "94|4|Va [nomu] ovozai turo ʙaland soxtem"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "94|5|Pas, musallaman, ʙo [har] duşvorī osonī ast"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "94|6|[Ore] Musallaman, ʙo [har] duşvorī osonī ast"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "94|7|Pas, hangome ki [az koru umuri dunjo] foriƣ şudī, [ʙa iʙodati Parvardigorat] ʙikūş"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "94|8|Va ʙa sūji Parvardigorat roƣiʙ [va muştoq] şav"
        }
    ]
}