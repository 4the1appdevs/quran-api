{
    "rukus": [
        {
            "chapter": 100,
            "verse": 1,
            "text": "100|1|Hakuiki araku shingeki suru (uma) ni oite (chikau)."
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "100|2|Hidzume ni hibana o chirashi,"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "100|3|akatsuki ni kyūshū shite,"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "100|4|sajin o makiage,"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "100|5|(teki no) gunzei no mattadanaka ni totsunyū suru toki."
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "100|6|Hontōni ningen wa, jibun no omo ni taishi onshirazudearu."
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "100|7|Sore ni tsuki, kare wa makotoni shōnindeari,"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "100|8|mata tomi o aisuru koto ni netchū suru."
        },
        {
            "chapter": 100,
            "verse": 9,
            "text": "100|9|Kare wa haka no naka no mono ga abaki dasa reru toki no koto o shiranai no ka."
        },
        {
            "chapter": 100,
            "verse": 10,
            "text": "100|10|Mata mune no naka ni aru mono ga, bakuro sa reru no o."
        },
        {
            "chapter": 100,
            "verse": 11,
            "text": "100|11|Hontōni omo wa, sonohi, kare-ra ni tsuite subete shitte ora reru."
        }
    ]
}