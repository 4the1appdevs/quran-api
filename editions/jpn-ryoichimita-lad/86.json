{
    "chapter": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "86|1|Ten to, yoruotozurerumono ni yotte (chikau)."
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "86|2|Yoru otozureru mono ga nandearu ka o, anata ni rikai sa seru mono wa nani ka."
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "86|3|(Soreha) kirameki kagayaku hoshi."
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "86|4|Dare mo jibun no ue ni gādian (tenshi) o motanai mono wanai."
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "86|5|Ningen wa, nani kara tsukura reta ka o kōsatsu sa se nasai."
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "86|6|Kare wa funshutsu suru mizu kara tsukura re,"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "86|7|(soreha) rokkotsu to koshi no ma kara detekuru."
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "86|8|Hontōni kare wa, kare o (aratana seimei ni) hikimodosu koto ga kanōdearu."
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "86|9|Kakusa reta koto ga bakuro sa reru hi,"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "86|10|(ningen ni wa) chikara mo naku, dare no tasuke mo nai."
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "86|11|(Kaiten shite) kaeru ten ni yotte,"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "86|12|sake wareru daichi ni yotte (chikau)."
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "86|13|Hontōni kore wa,(zen'aku o) shikibetsu suru okotoba,"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "86|14|soreha tawamure-gotode wanai."
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "86|15|Hontōni kare-ra wa, inbō o takurande iru."
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "86|16|Ware mo mata sakubō o megurasu."
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "86|17|Dakara fushin-sha-tachi o yūyo shi, shibaraku hōnin suruga ī."
        }
    ]
}