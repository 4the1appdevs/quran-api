{
    "chapter": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "101|1|Osore ononoku hi (Saigonoshinpan)"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "101|2|osore ononoku hi to wa nani ka."
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "101|3|Osore ononoku hi ga, nanidearu ka o anata ni rikai sa seru mono wa nani ka."
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "101|4|(Soreha) ningen ga hisan suru ga no yo ni naru hi."
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "101|5|Mata yamayama ga, suka reta yomo no yo ni naru (Ni~Tsudearu)."
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "101|6|Sore de, kare no hakari ga (zenko de) omoi mono wa,"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "101|7|kofuku de michitarite kurasudearou."
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "101|8|Daga hakari no karui mono wa,"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "101|9|naraku ga, kare no satodearou."
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "101|10|Sore ga nandearu ka o, anata ni rikai sa seru mono wa nani ka."
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "101|11|(Soreha) shonetsu (jigoku) no hi."
        }
    ]
}