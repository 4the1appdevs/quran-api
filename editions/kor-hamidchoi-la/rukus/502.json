{
    "rukus": [
        {
            "chapter": 70,
            "verse": 1,
            "text": "70|1|yoguhaneun jaga dagchyeool beol-eul yoguhayeossdoda"
        },
        {
            "chapter": 70,
            "verse": 2,
            "text": "70|2|(geu beol-eun) bulsinjadeul-eul wihan geos-euloseo, geugeos(beol)eul jeojihal jonjaeneun eobsnola"
        },
        {
            "chapter": 70,
            "verse": 3,
            "text": "70|3|(geu beol-eun) seungcheon-ui gil-eul soyuhasin hananim-eulobuteo (oneun geos-ila)"
        },
        {
            "chapter": 70,
            "verse": 4,
            "text": "70|4|geunal cheonsadeulgwa yeonghon-i geubunkkelo ollagani geunal-ui gigan-eun omannyeon-ila"
        },
        {
            "chapter": 70,
            "verse": 5,
            "text": "70|5|geuleoni geudaeneun aleumdabge innaehala"
        },
        {
            "chapter": 70,
            "verse": 6,
            "text": "70|6|sillo geudeul-eun geugeos(beol)eul meolge bojiman"
        },
        {
            "chapter": 70,
            "verse": 7,
            "text": "70|7|naneun geugeos(beol)eul gakkabge bonola"
        },
        {
            "chapter": 70,
            "verse": 8,
            "text": "70|8|geunal haneul-eun eong-gin gileumcheoleom doel geos-ila"
        },
        {
            "chapter": 70,
            "verse": 9,
            "text": "70|9|geuligo sandeul-eun yangteolcheoleom doel geos-ila"
        },
        {
            "chapter": 70,
            "verse": 10,
            "text": "70|10|ttohan hyeol-yug-eun (daleun) hyeol-yug-e daehae mudji anh-eul geos-ila"
        },
        {
            "chapter": 70,
            "verse": 11,
            "text": "70|11|geudeul(hyeol-yug)i geudeul(daleun hyeol-yug)eul boge doem-edo (seoloui anbuleul mudji anh-eul geos-ila). aghaeng-eul jeojileun janeun geunal-ui jingbeol-eul beos-eonagi wihae jasin-ui jason-eul momgabs-eulo bachil suman issdamyeon hago galmanghadeola"
        },
        {
            "chapter": 70,
            "verse": 12,
            "text": "70|12|jasin-ui baeujawa jasin-ui hyeongjewa"
        },
        {
            "chapter": 70,
            "verse": 13,
            "text": "70|13|jasin-eul gamssajudeon gakkaun chincheoggwa"
        },
        {
            "chapter": 70,
            "verse": 14,
            "text": "70|14|jigusang-e issneun moduleul bachyeoseolado geugeos(geudeul-ui momgabs)i geuleul guhae jugil (galmanghadeola)"
        },
        {
            "chapter": 70,
            "verse": 15,
            "text": "70|15|geuleohji anhnola. geugeos(jiog)eun jag-yeolhaneun bulgil-ila"
        },
        {
            "chapter": 70,
            "verse": 16,
            "text": "70|16|(jiog-eun) meoli kkeobjil-eul beosgyeo naemyeo"
        },
        {
            "chapter": 70,
            "verse": 17,
            "text": "70|17|kkongmunileul ppaemyeo dol-aseossdeon jaleul buleul geos-ila"
        },
        {
            "chapter": 70,
            "verse": 18,
            "text": "70|18|(jaesan-eul) mo-eugo ssah-a dudeon ja(leul buleul geos-ila)"
        },
        {
            "chapter": 70,
            "verse": 19,
            "text": "70|19|sillo ingan-eun bin-yaghage changjodoeeossnola"
        },
        {
            "chapter": 70,
            "verse": 20,
            "text": "70|20|geuege nappeun geos-i seuchimyeon keuge geunsimhago"
        },
        {
            "chapter": 70,
            "verse": 21,
            "text": "70|21|geuege joh-eun geos-i seuchimyeon insaeghadeola"
        },
        {
            "chapter": 70,
            "verse": 22,
            "text": "70|22|geuleona yebae deulineun jadeul-eun ye-oela"
        },
        {
            "chapter": 70,
            "verse": 23,
            "text": "70|23|geudeul-eun sillo geudeul-ui yebaee iss-eo bujileonhan jadeul-imyeo"
        },
        {
            "chapter": 70,
            "verse": 24,
            "text": "70|24|geudeul-ui jaesan-e iljeonghan mogs-i issneun jadeul-ini"
        },
        {
            "chapter": 70,
            "verse": 25,
            "text": "70|25|gugeolhaneun gananhan jawa gugeolhaji anhneun gananhan jaleul wiham-ila"
        },
        {
            "chapter": 70,
            "verse": 26,
            "text": "70|26|geuligo geudeul-eun simpan-ui nal-eul mid-eumyeo"
        },
        {
            "chapter": 70,
            "verse": 27,
            "text": "70|27|geudeul junim-ui beol-e daehayeo sillo geunsimhaneun jadeul-ila"
        },
        {
            "chapter": 70,
            "verse": 28,
            "text": "70|28|sillo geudeul junim-ui beol-ilan anjeonhae hal su eobsneun geos-ila"
        },
        {
            "chapter": 70,
            "verse": 29,
            "text": "70|29|geuligo geudeul-eun sillo jasin-ui sungyeol-eul jikineun jadeul-ila"
        },
        {
            "chapter": 70,
            "verse": 30,
            "text": "70|30|geuleona geudeul-ui baeujana geudeul-ui oleunson-i soyuhan ja(noye)e gwanhayeoneun geudeul-i chaegmangbadji anhnola"
        },
        {
            "chapter": 70,
            "verse": 31,
            "text": "70|31|geuleondedo geu ioeui geos-eul domohaneun jadeul-i issdamyeon jeodeul-eun sillo gyeong-gyeleul neom-eun jadeul-ila"
        },
        {
            "chapter": 70,
            "verse": 32,
            "text": "70|32|geuligo geudeul-eun sillo geudeul-ui sutagmulgwa gyeyag-eul jung-yohasineun jadeul-ila"
        },
        {
            "chapter": 70,
            "verse": 33,
            "text": "70|33|geuligo geudeul-eun sillo gudgeonhi geudeul-ui jeung-eon-eul seoneun jadeul-ila"
        },
        {
            "chapter": 70,
            "verse": 34,
            "text": "70|34|geuligo geudeul-eun sillo geudeul-ui yebaeleul jikim-e kkujunhadeola"
        },
        {
            "chapter": 70,
            "verse": 35,
            "text": "70|35|geudeul-eun cheongug-e iss-eul myeong-yeloun jadeul-ila"
        }
    ]
}