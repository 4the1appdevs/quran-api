{
    "rukus": [
        {
            "chapter": 26,
            "verse": 123,
            "text": "26|123|adeuui baegseongdo seonjijadeul-eul bujeonghaess-euni"
        },
        {
            "chapter": 26,
            "verse": 124,
            "text": "26|124|geudeul-ui hyeongje hudeuga malhagil neohuineun hananim-eul dulyeowo haji anh neunyo"
        },
        {
            "chapter": 26,
            "verse": 125,
            "text": "26|125|sillo naega neohuileul wihan mid eum-ui seonjijala"
        },
        {
            "chapter": 26,
            "verse": 126,
            "text": "26|126|geuleomeulo hananim-eul dulyeowo hago na-ege sunjonghala"
        },
        {
            "chapter": 26,
            "verse": 127,
            "text": "26|127|geulo inhayeo naega bosang-eul yoguhaji anihani naui bosang-eun man-yuui junim-eulobuteo issnola"
        },
        {
            "chapter": 26,
            "verse": 128,
            "text": "26|128|neohuineun nop-eun gosmada gongheohanhyanglag-eul wihae geonmul-eul se-uneunyo"
        },
        {
            "chapter": 26,
            "verse": 129,
            "text": "26|129|neohuiga yeongsaenghagoja gungjeon eul se-uneunyo"
        },
        {
            "chapter": 26,
            "verse": 130,
            "text": "26|130|neohuineun tain-eul gong-gyeogham-e pog guncheoleom gong-gyeoghaessnola"
        },
        {
            "chapter": 26,
            "verse": 131,
            "text": "26|131|hananim-eul dulyeowo hago na-e ge sunjonghala"
        },
        {
            "chapter": 26,
            "verse": 132,
            "text": "26|132|neohuiga algo issneun modeun geos eul jusin bun-eul dulyeowo hala"
        },
        {
            "chapter": 26,
            "verse": 133,
            "text": "26|133|geubunkkeseoneun gachugdeulgwa jason deul-eul neohuiege jusyeoss-eumyeo"
        },
        {
            "chapter": 26,
            "verse": 134,
            "text": "26|134|nag-wongwa umuldo jusyeossnola"
        },
        {
            "chapter": 26,
            "verse": 135,
            "text": "26|135|sillo naneun simpan-ui nal neohui ege naelyeojil beol-i dulyeobdoda"
        },
        {
            "chapter": 26,
            "verse": 136,
            "text": "26|136|geudeul-i daedabhagil nega seolgyo hadeun anihadeun uliegeneun machanga jila"
        },
        {
            "chapter": 26,
            "verse": 137,
            "text": "26|137|sillo igeos-eun yes seonjodeul-ui gwanseub-e bulgwahamyeo"
        },
        {
            "chapter": 26,
            "verse": 138,
            "text": "26|138|ulineun beol-eul badji anihadoda"
        },
        {
            "chapter": 26,
            "verse": 139,
            "text": "26|139|ileohge geudeul-i geuleul bujeong hamae hananim-eun geudeul-eul myeolmangsikyeossno la geu an-eneun yejeung-i iss-euna geudeul daedasuneun midji ani hadeola"
        },
        {
            "chapter": 26,
            "verse": 140,
            "text": "26|140|sillo geudaeui junim-eun jeonneung-gwa jabilo chungmanhasim-ila"
        }
    ]
}