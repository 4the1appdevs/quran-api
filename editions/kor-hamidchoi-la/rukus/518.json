{
    "rukus": [
        {
            "chapter": 78,
            "verse": 1,
            "text": "78|1|geudeul-eun mueos-e gwanhayeo seolo-ege mudneun geos-inga"
        },
        {
            "chapter": 78,
            "verse": 2,
            "text": "78|2|widaehan sosig-e gwanhaeseoni"
        },
        {
            "chapter": 78,
            "verse": 3,
            "text": "78|3|geudeul-eun geue gwanhae uigyeon-eul dallihago issnola"
        },
        {
            "chapter": 78,
            "verse": 4,
            "text": "78|4|geuleohji anhnola. geudeul-eun god alge doel geos-ila"
        },
        {
            "chapter": 78,
            "verse": 5,
            "text": "78|5|dasi hanbeon, geuleohji anhnola. geudeul-eun god alge doel geos-ila"
        },
        {
            "chapter": 78,
            "verse": 6,
            "text": "78|6|naega daejileul chimsang-eulo duji anh-assdeonga"
        },
        {
            "chapter": 78,
            "verse": 7,
            "text": "78|7|ttohan san-eul malttug-eulo duji anh-assdeonga"
        },
        {
            "chapter": 78,
            "verse": 8,
            "text": "78|8|naneun geudaedeul-eul namnyeolo changjohayeoss-eumyeo"
        },
        {
            "chapter": 78,
            "verse": 9,
            "text": "78|9|geudaedeul-ui jam-eul danjeol-ui sigan-eulo dueossnola"
        },
        {
            "chapter": 78,
            "verse": 10,
            "text": "78|10|geuligo bam-eul os-eulo dueossgo"
        },
        {
            "chapter": 78,
            "verse": 11,
            "text": "78|11|naj-eul saeng-gyelo dueossnola"
        },
        {
            "chapter": 78,
            "verse": 12,
            "text": "78|12|geuligo geudaedeul wie ilgob gaeui ganghan geos(haneul)eul jieoss-eumyeo"
        },
        {
            "chapter": 78,
            "verse": 13,
            "text": "78|13|chanlanhan deungbul(taeyang)eul dueossnola"
        },
        {
            "chapter": 78,
            "verse": 14,
            "text": "78|14|geuligo bisbang-ul-eul jjanaeneun guleum-eseo peobusneun bileul naelyeoss-euni"
        },
        {
            "chapter": 78,
            "verse": 15,
            "text": "78|15|geugeos-eulosseo gogmulgwa yacholeul naego"
        },
        {
            "chapter": 78,
            "verse": 16,
            "text": "78|16|namuga ugeojin jeong-wondo naenola"
        },
        {
            "chapter": 78,
            "verse": 17,
            "text": "78|17|sillo bunliui nal-eun giyagdoeeo issnola"
        },
        {
            "chapter": 78,
            "verse": 18,
            "text": "78|18|geunal napal-i bullimyeon geudaedeul-eun ttejieo ol geos-imyeo"
        },
        {
            "chapter": 78,
            "verse": 19,
            "text": "78|19|haneul-eun yeollieo mun(mun)cheoleom doego"
        },
        {
            "chapter": 78,
            "verse": 20,
            "text": "78|20|sandeul-eun heulleonaelieo singilucheoleom doel geos-ila"
        },
        {
            "chapter": 78,
            "verse": 21,
            "text": "78|21|sillo jiog-eun jamboghae iss-euni"
        },
        {
            "chapter": 78,
            "verse": 22,
            "text": "78|22|gyeong-gyeleul neom-eoseon jadeul-eul wihan gwichagjila"
        },
        {
            "chapter": 78,
            "verse": 23,
            "text": "78|23|geugos-eseo geudeul-eun yeong-guui sigan-eul meomul geos-imyeo"
        },
        {
            "chapter": 78,
            "verse": 24,
            "text": "78|24|geudeul-eun geugos-eseo siwonhamdo, eumlyodo masboji moshal geos-ini"
        },
        {
            "chapter": 78,
            "verse": 25,
            "text": "78|25|ojig bugeulgeolineun mulgwa goleum ppun-ila"
        },
        {
            "chapter": 78,
            "verse": 26,
            "text": "78|26|ineun sang-eunghaneun jingbeolloseo"
        },
        {
            "chapter": 78,
            "verse": 27,
            "text": "78|27|sillo geudeul-eun simpan-eul gaeuichi anh-ass-eumyeo"
        },
        {
            "chapter": 78,
            "verse": 28,
            "text": "78|28|naui jingpyoleul gojibseule bu-inhayeossnola"
        },
        {
            "chapter": 78,
            "verse": 29,
            "text": "78|29|modeun geos-eul naneun gilogbue gijaehayeossnola"
        },
        {
            "chapter": 78,
            "verse": 30,
            "text": "78|30|geuleoni neohuineun masbola, naega neohuiege jeung-gasikil geos-eun ojig beol ppun-ila"
        }
    ]
}