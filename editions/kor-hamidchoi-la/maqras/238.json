{
    "maqras": [
        {
            "chapter": 90,
            "verse": 1,
            "text": "90|1|i do-eub-eul dugo maengsehasa"
        },
        {
            "chapter": 90,
            "verse": 2,
            "text": "90|2|geudaeneun i do-eub-ui jayuloun geoju-in-ila"
        },
        {
            "chapter": 90,
            "verse": 3,
            "text": "90|3|seonjowa jason-eul dugo maengsehasa"
        },
        {
            "chapter": 90,
            "verse": 4,
            "text": "90|4|sillo hananim-eun ingan-eulo hayeogeum nolyeoggwa silyeonsog-eseo saldolog chang johayeossnani"
        },
        {
            "chapter": 90,
            "verse": 5,
            "text": "90|5|eoneu nugudo bulsinjaleul jeabhal su eobsdago saeng-gag haneunyo"
        },
        {
            "chapter": 90,
            "verse": 6,
            "text": "90|6|sillo naneun manh-eun jaesan-eul tangjin hayeossseubnida lago bulsinjaneun malhal geos-ila"
        },
        {
            "chapter": 90,
            "verse": 7,
            "text": "90|7|eoneu nugudo geuleul jikyeoboji anihandago bulsinjaneun saeng-gaghaneunyo"
        },
        {
            "chapter": 90,
            "verse": 8,
            "text": "90|8|hananim-i geuege du nun-eul ju ji ani haessdeonyo"
        },
        {
            "chapter": 90,
            "verse": 9,
            "text": "90|9|hanaui hyeowa du ibsul-eul juji ani haessdeonyo"
        },
        {
            "chapter": 90,
            "verse": 10,
            "text": "90|10|hananim-eun geuege du gil-eul seolmyeonghayeossnola"
        },
        {
            "chapter": 90,
            "verse": 11,
            "text": "90|11|geuneun himdeun gil-eseo sugohalyeo ani haneunyo"
        },
        {
            "chapter": 90,
            "verse": 12,
            "text": "90|12|geu himdeun gil-i mueos-inji mueos-i geudaeege seolmyeonghayeo juliyo"
        },
        {
            "chapter": 90,
            "verse": 13,
            "text": "90|13|geugeos-eun noyeleul haebangsikyeo ju neun il-iyo"
        },
        {
            "chapter": 90,
            "verse": 14,
            "text": "90|14|baegopeun ja-ege eumsig-eul bepu neun geos-imyeo"
        },
        {
            "chapter": 90,
            "verse": 15,
            "text": "90|15|chincheog-ui goadeulgwa"
        },
        {
            "chapter": 90,
            "verse": 16,
            "text": "90|16|meonji tuseong-iga doen gananhan jadeul-ege jaseon-eul bepuneun geos-ila"
        },
        {
            "chapter": 90,
            "verse": 17,
            "text": "90|17|geuleon hu mid-eum-eulo seologa seolo-ege innaehago seologa seolo-ege salang-eul bepuneun geos-eulo"
        },
        {
            "chapter": 90,
            "verse": 18,
            "text": "90|18|ideulman-i upyeon-e issneun donglyo deul-ila"
        },
        {
            "chapter": 90,
            "verse": 19,
            "text": "90|19|geuleona hananim-ui malsseum-eul geoyeoghaneun jadeul-eun jwapyeon-e issneun donglyo deullo"
        },
        {
            "chapter": 90,
            "verse": 20,
            "text": "90|20|geudeul wieneun dadhyeojin buljiog man-i iss-eul ppun-ila"
        },
        {
            "chapter": 91,
            "verse": 1,
            "text": "91|1|taeyang-gwa geu bich-eul dugo maengsehasa"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "91|2|geu dwileul ieun dal-eul dugo maengsehamyeo"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "91|3|taeyang-ui yeong-gwang-eul boyeojuneun naj-euldugo maengsehamyeo"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "91|4|geugeos-eul sumgineun bam-eul dugo maeng sehamyeo"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "91|5|haneulgwa geugeos-eul se-un junim-eul dugo maengsehasa"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "91|6|daejiwa geugeos-eul pyeolchyeo noh-eusin junim-eul dugo maengsehamyeo"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "91|7|ingangwa geugeos-eul changjohan junim-euldugo maengsehasa"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "91|8|seongwa ag-i mueos-ingaleul ihaeha dolog hasyeossnani"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "91|9|seuseuloleul sungyeolhage haneun jaga seunglijaimyeo"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "91|10|seuseuloleul bulgyeolhage haneun ja silpaehan jala"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "91|11|samudeu baegseong-eun geudeul-ui oman eulo yeeonjaleul geoyeoghayeoss-eumae"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "91|12|geudeul jung gajang saaghan jaga il-eoseoss-eul ttae"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "91|13|seonjijaga geudeul-ege malhagil geugeos-eun hananim-ui amnagtaini geugeos i mul-eul masimae banghaehaji malla ha yeoss-euna"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "91|14|geudeul-eun geuleul geoyeoghan hu geu amnagtaleul salhaehayeoss-euni geu joeag-eu lo inhayeo junim-eun geudeul-eul myeolmangke hasa gubyeol-eobs-i wanjeon myeolmangke hasyeoss nola"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "91|15|sillo geubun-eun geu gyeolgwa-e daehayeo jogeumdo yeomlyeohaji ani hasinila"
        },
        {
            "chapter": 92,
            "verse": 1,
            "text": "92|1|eoduwojineun bam-eul dugo maengsehasa"
        },
        {
            "chapter": 92,
            "verse": 2,
            "text": "92|2|bich-eul bichu-ineun naj-eul dugo maeng sehamyeo"
        },
        {
            "chapter": 92,
            "verse": 3,
            "text": "92|3|namnyeoleul changjohasin junim-eul dugo maengsehanani"
        },
        {
            "chapter": 92,
            "verse": 4,
            "text": "92|4|sillo neohuideul-ui haeng-wineun yeoleoga jila"
        },
        {
            "chapter": 92,
            "verse": 5,
            "text": "92|5|jaseon-eul bepulgo dulyeowo hamyeo"
        },
        {
            "chapter": 92,
            "verse": 6,
            "text": "92|6|jinlileul jeung-eonhaneun jaleul wihae"
        },
        {
            "chapter": 92,
            "verse": 7,
            "text": "92|7|hananim-eun geuga chugbog-eulo ganeun gil-eul swibge hayeo jusinola"
        },
        {
            "chapter": 92,
            "verse": 8,
            "text": "92|8|geuleona insaeghago jagijasin-i chungmanhadago saeng-gaghamyeo"
        },
        {
            "chapter": 92,
            "verse": 9,
            "text": "92|9|jinlileul geoyeoghaneunja"
        },
        {
            "chapter": 92,
            "verse": 10,
            "text": "92|10|hananim-eun geuga bulhaeng-eulo ga neun gil-eul swibge halini"
        },
        {
            "chapter": 92,
            "verse": 11,
            "text": "92|11|geuga jiog-eulo hyanghal ttaeneun geu ui jaesan-i geuleul yuighage moshanila"
        },
        {
            "chapter": 92,
            "verse": 12,
            "text": "92|12|indohaneun geos-eun hananim-ui il ila"
        },
        {
            "chapter": 92,
            "verse": 13,
            "text": "92|13|naesewa hyeonsedo geubunkke issnola"
        },
        {
            "chapter": 92,
            "verse": 14,
            "text": "92|14|geuleomeulo naega neohuiege gyeog lyeolhage taoleuneun buljiog-eul gyeong-goha nani"
        },
        {
            "chapter": 92,
            "verse": 15,
            "text": "92|15|gajang bulhaenghan jadeulman-i geugos e ileuge doenila"
        },
        {
            "chapter": 92,
            "verse": 16,
            "text": "92|16|geuneun balo jinlileul geoyeoghago oemyeonhaneun jala"
        },
        {
            "chapter": 92,
            "verse": 17,
            "text": "92|17|geuleona hananimkke heonsinhaneun janeun geugos-eulobuteo je-oe doegeoneul"
        },
        {
            "chapter": 92,
            "verse": 18,
            "text": "92|18|ideul-eun geudeul-ui jaesan-eul bachi myeo seuseuloleul sungyeolke hasa"
        },
        {
            "chapter": 92,
            "verse": 19,
            "text": "92|19|geuga bepun eunhyee daehan bosang-eul geuui ma-eumsog-e gaj-ji anihago"
        },
        {
            "chapter": 92,
            "verse": 20,
            "text": "92|20|ojig gajang wie gyesineun junim eul gippeuge hagoja haneun somang-eul ga jin jadeullo"
        },
        {
            "chapter": 92,
            "verse": 21,
            "text": "92|21|ideul-eun god wanjeonhan gippeum-eul nu lige doelila"
        },
        {
            "chapter": 93,
            "verse": 1,
            "text": "93|1|achim-eul dugo maengsehago"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "93|2|eodum-i jit-eojineun bam-eul dugo maengsehasa"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "93|3|junimkkeseo geudaeleul beoliji anihaess-eumyeo miwohajido anh-eusinila"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "93|4|sillo geudaeegeneun naesega hyeonseboda na-eulini"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "93|5|junimkkeseo geudaeege eunhyeleul bepulgeos-imae geudaeneun ilohayeo gippeohalila"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "93|6|geubun-eun goain geudaeleul balgyeonhayeo geudaeleul bohohaji ani haessdeonyo"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "93|7|geubun-eun banghwanghaneun geudaeleul balgyeonhayeo geudaeleul indohaji ani haessdeo nyo"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "93|8|geubun-eun gananhaessdeon geudaeleul buyuhage hayeo juji ani haessdeonyo"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "93|9|geuleomeulo goadeul-eul geochilge daehajimalmyeo"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "93|10|guhaneun ja-ege geojeolhaji malgo"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "93|11|geuligo junim-ui eunhyeleul iyagi hala"
        }
    ]
