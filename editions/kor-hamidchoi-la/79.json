        },
        {
            "chapter": 79,
            "verse": 23,
            "text": "79|23|(salamdeul-eul) moa bulleossnola"
        },
        {
            "chapter": 79,
            "verse": 24,
            "text": "79|24|geu hu geuga malhagil “jim-eun geudaedeul-ui gajang jigohan junim-ila”"
        },
        {
            "chapter": 79,
            "verse": 25,
            "text": "79|25|geulihayeo hananimkkeseoneun naesewa hyeonse-ui jing-gyeloseo geuleul umkyeojab-eusyeossnola"
        },
        {
            "chapter": 79,
            "verse": 26,
            "text": "79|26|jinsillo igeos-eneun dulyeowohaneun jaleul wihan gyohun-i issnola"
        },
        {
            "chapter": 79,
            "verse": 27,
            "text": "79|27|changjoga deo eolyeoun geos-eun geudaedeul-inga animyeon haneul-inga? geubunkkeseo geugeos(haneul)eul jieusyeossnola"
        },
        {
            "chapter": 79,
            "verse": 28,
            "text": "79|28|geubunkkeseoneun geugeos(haneul)ui kkogdaegileul ollisyeossgo geu hu geugeos-eul gyundeunghage dusyeoss-eumyeo"
        },
        {
            "chapter": 79,
            "verse": 29,
            "text": "79|29|geugeos-ui bam-eul eodubge hasyeossgo geugeos-ui achimbich-eul naesyeossnola"
        },
        {
