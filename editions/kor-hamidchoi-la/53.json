        {
            "chapter": 53,
            "verse": 1,
            "text": "53|1|jineun byeol-eul dugo maengsehasa"
        },
        {
            "chapter": 53,
            "verse": 2,
            "text": "53|2|neohuiui donglyoneun banghwanghajidoanhgo yuhogdoejido ani haess-eumyeo"
        },
        {
            "chapter": 53,
            "verse": 3,
            "text": "53|3|geuui yogmang-eul malhago issneun geosdo anila"
        },
        {
            "chapter": 53,
            "verse": 4,
            "text": "53|4|geugeos-eun geuege naelyeojin gyesila"
        },
        {
            "chapter": 53,
            "verse": 5,
            "text": "53|5|geuneun neunglyeog-i iss-eusin bun-eulobuteo baewossnola"
        },
        {
            "chapter": 53,
            "verse": 6,
            "text": "53|6|geuneun jihyeleul gajigo natanass-euni"
        },
        {
            "chapter": 53,
            "verse": 7,
            "text": "53|7|geuneun jipyeongseon gajang nop-eun gos-e iss-eossnola"
        },
        {
            "chapter": 53,
            "verse": 8,
            "text": "53|8|geuleon hu geuneun gakkai dagawass euni"
        },
        {
            "chapter": 53,
            "verse": 9,
            "text": "53|9|geu geolineun hwal yangjjog kkeut saiuigil-i heug-eun geuboda deo gakkawossdeola"
        },
        {
            "chapter": 53,
            "verse": 10,
            "text": "53|10|geuleohge hayeo hananim-eun geuui jong-ege jeonhagoja han gyesileul naelyeoss eumae"
        },
        {
            "chapter": 53,
            "verse": 11,
            "text": "53|11|geuui ma-eum-eun geuga bon geos-edaehayeo geojisham-i eobsdeola"
        },
        {
            "chapter": 53,
            "verse": 12,
            "text": "53|12|geulaedo neohuineun geuga bon geoshe gwanhayeo nonjaenghago issneunyo"
        },
        {
            "chapter": 53,
            "verse": 13,
            "text": "53|13|sillo geuneun dasi hanbeon geuleul boass-euni"
        },
        {
            "chapter": 53,
            "verse": 14,
            "text": "53|14|majimag sideula namuyeop-e iss eossdeola"
        },
        {
            "chapter": 53,
            "verse": 15,
            "text": "53|15|geugos gakkaieneun yeongjuhal cheon gug-i iss-euni"
        },
        {
            "chapter": 53,
            "verse": 16,
            "text": "53|16|bola sideula namuga galiwo jimae"
        },
        {
            "chapter": 53,
            "verse": 17,
            "text": "53|17|geuui siseon-eun heut-eojiji ani hago hangyeleul neomjido anhdeola"
        },
        {
            "chapter": 53,
            "verse": 18,
            "text": "53|18|sillo geuneun gajang widaehan hananim-ui yejeungdeul-eul boassnola"
        },
        {
            "chapter": 53,
            "verse": 19,
            "text": "53|19|sillo neohuineun lateuwa usjaleul boass-eumyeo"
        },
        {
            "chapter": 53,
            "verse": 20,
            "text": "53|20|sebeonjjaeui usang manateuleul bo assneunyo"
        },
        {
            "chapter": 53,
            "verse": 21,
            "text": "53|21|neohuiegeneun namjaga issgo ha nanim-egeneun yeojaga issdan mal-inyo"
        },
        {
            "chapter": 53,
            "verse": 22,
            "text": "53|22|sillo igeos-eun gajang gongpyeongchi moshan bunbaela"
        },
        {
            "chapter": 53,
            "verse": 23,
            "text": "53|23|sillo igeosdeul-eun neohuiwa neohui seonjodeul-i goanhaessdeon ileumdeul-e bulgwahamyeo hananim-eun geugeosdeul-ege amu leon neunglyeogdo buyeohaji ani haessnani geudeul-eun imi junim-eulobuteo bog-eum ui sosig-eul deul-eoss-eum-edo bulguhago eogcheuggwa jasindeul-ui jeosoghan yogmang-eul ttaleul ppun-ila"
        },
        {
            "chapter": 53,
            "verse": 24,
            "text": "53|24|ingan-eun geuga wonhaneun geosman-eul gajilyeo haneunyo"
        },
        {
            "chapter": 53,
            "verse": 25,
            "text": "53|25|geuleona manmul-ui jongmalgwa sijag eun hananim an-e issnola"
        },
        {
            "chapter": 53,
            "verse": 26,
            "text": "53|26|haneul-eneun manh-eun cheonsadeul-i iss eudoe geudeul-ui jungjaeneun hananim-i seon taeghan ja-ege geubun-ui heolag-i issneun ihuga anigoneun yuyonghaji moshanola"
        },
        {
            "chapter": 53,
            "verse": 27,
            "text": "53|27|naeseleul midji anihan jadeul-eun cheonsadeul-eul yeoseong-ila buleuni"
        },
        {
            "chapter": 53,
            "verse": 28,
            "text": "53|28|geudeul-eun alji mosham-ila geudeul eun eogcheugman-eul ttaleul ppun-ila sillo eogcheug-eun jinlie daehayeo amugeosdo yu hyohaji anhnola"
        },
        {
            "chapter": 53,
            "verse": 29,
            "text": "53|29|hananim-ui mesejileul oemyeonhaneunja geudaedo geudeul-eul oemyeonhala geudeul eun hyeonseman-eul chuguhal ppun-ila"
        },
        {
            "chapter": 53,
            "verse": 30,
            "text": "53|30|geugeos-eun geudeul-i algo issneun geos-ila sillo junim-eun geubun-ui gil-eseo beos-eonan jadeul-eul asigo gyesimyeo tto han bog-eum-eul bad-eun geudeuldo algo gyesinila"
        },
        {
            "chapter": 53,
            "verse": 31,
            "text": "53|31|haneulgwa daejie issneun modeun geos-i hananim an-e iss-eo saagham-eul peotteulineun ja geuui haeng-wie ttala beol-eulnaelil geos-iyo seon-eul silcheonhaneun ja gajang joh-eun geos-eulo bosanghal geos-ila"
        },
        {
            "chapter": 53,
            "verse": 32,
            "text": "53|32|keun joeaggwa suchiseuleoun haeng-wi leul pihalyeo haneun jaga jag-eun silsu leul hayeoss-eul ttae sillo junim-eun chungbun hi gwan-yong-eul bepusinila hananim-eun heulg-eseo changjohan neohuileul asimyeo tto han neohui eomeoni taenaee iss-eul ttaeuineohuido asinani neohui seuseuloga wanjeonhan geos-ila jeongdanghwa haji malla geubun-eun saag-eulobuteo gyeong-oehaneun jaleul algo gyesinila"
        },
        {
            "chapter": 53,
            "verse": 33,
            "text": "53|33|oemyeonhaneun jaleul geudaeneun boass neunyo"
        },
        {
            "chapter": 53,
            "verse": 34,
            "text": "53|34|jogeum jumyeonseo akkawo haneun jala"
        },
        {
            "chapter": 53,
            "verse": 35,
            "text": "53|35|boiji anhneun geos-e daehan jisig-ilado gajgo iss-eo geuga bol su iss dan mal-inyo"
        },
        {
            "chapter": 53,
            "verse": 36,
            "text": "53|36|animyeon geuga mose-ui seongseosog e issneun geosdeul-eul alji moshago"
        },
        {
            "chapter": 53,
            "verse": 37,
            "text": "53|37|myeonglyeong-eul ihaenghan abeulaham e gwanhan iyagido alji moshayeossdan mal-inyo"
        },
        {
            "chapter": 53,
            "verse": 38,
            "text": "53|38|jimjinjaga daleun salam-ui jim-eul jil su eobs-eumyeo"
        },
        {
            "chapter": 53,
            "verse": 39,
