{
    "rukus": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "105|1|[Ej Pajomʙar] Ojo nadidi Parvardigorat ʙo ashoʙi fil ci kard"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "105|2|Ojo [fireʙu] naqsai ononro dar taʙohi qaror nadod"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "105|3|Va parandagone guruh-guruh ʙar [sari] onon firistod"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "105|4|[Ki] Bo sanggil [ki dar minqoru cangol dostand] ononro sangʙoron mekardand"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "105|5|Pas, onhoro hamcun kohi cavidasuda soxt"
        }
    ]
}