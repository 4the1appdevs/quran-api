{
    "rukus": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "94|1|[Ej pajomʙar] Ojo Mo sinaatro [ʙaroi nuzuli vahj] ʙarojat nakusudem"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "94|2|Va sangini [-i ʙori gunoh va dusvorii risolat]-ro az [dusi] tu ʙarnadostem"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "94|3|Hamon [ʙore], ki ʙar pustat sangini mekard"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "94|4|Va [nomu] ovozai turo ʙaland soxtem"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "94|5|Pas, musallaman, ʙo [har] dusvori osoni ast"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "94|6|[Ore] Musallaman, ʙo [har] dusvori osoni ast"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "94|7|Pas, hangome ki [az koru umuri dunjo] foriƣ sudi, [ʙa iʙodati Parvardigorat] ʙikus"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "94|8|Va ʙa suji Parvardigorat roƣiʙ [va mustoq] sav"
        }
    ]
}