{
    "rukus": [
        {
            "chapter": 101,
            "verse": 1,
            "text": "101|1|[On] Furukuʙanda"
        },
        {
            "chapter": 101,
            "verse": 2,
            "text": "101|2|[On] Furukuʙanda cist"
        },
        {
            "chapter": 101,
            "verse": 3,
            "text": "101|3|Va tu ci doni, ki [on] furukuʙanda cist"
        },
        {
            "chapter": 101,
            "verse": 4,
            "text": "101|4|Ruze, ki mardum monandi parvonahoi parokanda [hajronu sargardon] xohand ʙud"
        },
        {
            "chapter": 101,
            "verse": 5,
            "text": "101|5|Va kuhho monandi pasmi rangini hallocisuda (zadasuda) xohand sud"
        },
        {
            "chapter": 101,
            "verse": 6,
            "text": "101|6|Va ammo har kase, ki [dar on ruz] pallai mizonas sangin ʙosad"
        },
        {
            "chapter": 101,
            "verse": 7,
            "text": "101|7|[Dar ʙihist] Dar zindagii rohatʙaxs xohad ʙud"
        },
        {
            "chapter": 101,
            "verse": 8,
            "text": "101|8|Va ammo har kas, ki pallai mizonas saʙuk ʙosad"
        },
        {
            "chapter": 101,
            "verse": 9,
            "text": "101|9|Pas, [maskanu] panohgohas «hovija» ast"
        },
        {
            "chapter": 101,
            "verse": 10,
            "text": "101|10|Va tu ci doni, ki on [hovija] cist"
        },
        {
            "chapter": 101,
            "verse": 11,
            "text": "101|11|Otase ast suzon [va su'lavar]"
        }
    ]
}