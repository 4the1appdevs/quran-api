{
    "chapter": [
        {
            "chapter": 114,
            "verse": 1,
            "text": "114|1|[Ej Pajomʙar] Bigu: «Ba Parvardigori mardum panoh meʙaram"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "114|2|Farmonravoi mardum"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "114|3|Ma'ʙudi mardum"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "114|4|Az sarri [sajtoni] vasvasagar, ki [ʙa hangomi zikri Alloh taolo] pinhon megardad"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "114|5|Hamone, ki dar dilhoi mardum vasvasa mekunad"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "114|6|Az cinnijon [ʙosad] va [jo az] odamijon»"
        }
    ]
}