{
    "pages": [
        {
            "chapter": 56,
            "verse": 17,
            "text": "56|17|Navcavonone hamvora sodoʙ girdi onon [ʙa xidmat] megardand"
        },
        {
            "chapter": 56,
            "verse": 18,
            "text": "56|18|Bo comho va kuzaho va qadahhoe az saroʙi cori"
        },
        {
            "chapter": 56,
            "verse": 19,
            "text": "56|19|[Saroʙe] Ki az nusidanas sardard namegardand va dastxusi masti namesavand"
        },
        {
            "chapter": 56,
            "verse": 20,
            "text": "56|20|Va az har meva [dar ixtijor dorand]"
        },
        {
            "chapter": 56,
            "verse": 21,
            "text": "56|21|va gusti paranda, ki ʙixohand"
        },
        {
            "chapter": 56,
            "verse": 22,
            "text": "56|22|Va hurone ƣizolcasm"
        },
        {
            "chapter": 56,
            "verse": 23,
            "text": "56|23|Hamcun marvoridi pinhon dar sadaf"
        },
        {
            "chapter": 56,
            "verse": 24,
            "text": "56|24|[Inho hama] Podosi a'molason ast"
        },
        {
            "chapter": 56,
            "verse": 25,
            "text": "56|25|Dar on co [suxani] jova va gunoholud naxohand sunid"
        },
        {
            "chapter": 56,
            "verse": 26,
            "text": "56|26|Dar on co suxane cuz salom va durud [-i faristagon va ʙihistijon] nest"
        },
        {
            "chapter": 56,
            "verse": 27,
            "text": "56|27|Va [ammo] saodatmandon; ci saodatmandone"
        },
        {
            "chapter": 56,
            "verse": 28,
            "text": "56|28|Dar kanori daraxtoni ʙexori sidr [hastand]"
        },
        {
            "chapter": 56,
            "verse": 29,
            "text": "56|29|Va daraxtoni mavz ʙo xusahoi ʙarhamnisasta"
        },
        {
            "chapter": 56,
            "verse": 30,
            "text": "56|30|Bo sojae gusturda"
        },
        {
            "chapter": 56,
            "verse": 31,
            "text": "56|31|Va oʙi hamvora ravon"
        },
        {
            "chapter": 56,
            "verse": 32,
            "text": "56|32|Va mevahoe farovon"
        },
        {
            "chapter": 56,
            "verse": 33,
            "text": "56|33|ki na tamom savad va na mamnu' gardad"
        },
        {
            "chapter": 56,
            "verse": 34,
            "text": "56|34|Va ʙistarhoi ʙarafrosta [va arzismand]"
        },
        {
            "chapter": 56,
            "verse": 35,
            "text": "56|35|Va hamsarone, ki ʙo ofarinise viƶa padid ovardem [ki zeʙoi va cavonii covidon dorand]"
        },
        {
            "chapter": 56,
            "verse": 36,
            "text": "56|36|Hamvora dusizaand"
        },
        {
            "chapter": 56,
            "verse": 37,
            "text": "56|37|va savhardustone hamsinnusol hastand"
        },
        {
            "chapter": 56,
            "verse": 38,
            "text": "56|38|[Hamai in ne'matho] Baroi saodatmandon ast"
        },
        {
            "chapter": 56,
            "verse": 39,
            "text": "56|39|Dar ʙoƣhoi purne'mat [-i ʙihist coj dorand]"
        },
        {
            "chapter": 56,
            "verse": 40,
            "text": "56|40|ki ʙarxe az onon dar zumrai pesinijon va ʙarxe digar dar zumrai mutaaxxiron hastand"
        },
        {
            "chapter": 56,
            "verse": 41,
            "text": "56|41|Va [ammo] tiraʙaxton; ci tiraʙaxtone"
        },
        {
            "chapter": 56,
            "verse": 42,
            "text": "56|42|Dar mijoni ʙodi zahrogin va oʙi cuson qaror dorand"
        },
        {
            "chapter": 56,
            "verse": 43,
            "text": "56|43|Va sojahoe az dudi sijoh"
        },
        {
            "chapter": 56,
            "verse": 44,
            "text": "56|44|ki na xunuk ast va na xus"
        },
        {
            "chapter": 56,
            "verse": 45,
            "text": "56|45|Onon dar dunjo sarvatmandu sarkas ʙudand"
        },
        {
            "chapter": 56,
            "verse": 46,
            "text": "56|46|Va ʙar gunohi ʙuzurg [-i sirk] isror mevarzidand"
        },
        {
            "chapter": 56,
            "verse": 47,
            "text": "56|47|Va meguftand: «Ojo hangome ki murdem va xoku ustuxon sudem, ʙarangexta xohem sud"
        },
        {
            "chapter": 56,
            "verse": 48,
            "text": "56|48|Ojo nijokoni mo niz [ʙarangexta mesavand]?»"
        },
        {
            "chapter": 56,
            "verse": 49,
            "text": "56|49|[Ej Pajomʙar, ʙa kofiron] Bigu: «Be tardid, guzastagon va ojandagon"
        },
        {
            "chapter": 56,
            "verse": 50,
            "text": "56|50|hama dar va'dagohi qijomat cam' xohand sud»"
        }
    ]
}