    "pages": [
        {
            "chapter": 112,
            "verse": 1,
            "text": "112|1|[Ej Pajomʙar] Bigu: «U Alloh taolo jaktost"
        },
        {
            "chapter": 112,
            "verse": 2,
            "text": "112|2|Alloh taolo ʙenijoz ast [va hama nijozmandi U hastand]"
        },
        {
            "chapter": 112,
            "verse": 3,
            "text": "112|3|Na [farzande] zoda va na zoda sudaast"
        },
        {
            "chapter": 112,
            "verse": 4,
            "text": "112|4|Va hec kas hammonandi va hamtoi U naʙuda va nest»"
        },
        {
            "chapter": 113,
            "verse": 1,
            "text": "113|1|[Ej Pajomʙar] Bigu: «Ba Parvardigori sapedadam panoh meʙaram"
        },
        {
            "chapter": 113,
            "verse": 2,
            "text": "113|2|Az sarri tamomi on ci ofaridaast"
        },
        {
            "chapter": 113,
            "verse": 3,
            "text": "113|3|Va az sarri torikii saʙ on goh ki hama coro faro girad"
        },
        {
            "chapter": 113,
            "verse": 4,
            "text": "113|4|Va az sar [-i zanoni codugar], ki ʙo afsun dar girehho medamand"
        },
        {
            "chapter": 113,
            "verse": 5,
            "text": "113|5|Va az sarri hasud on goh ki hasad varzad»"
        },
        {
            "chapter": 114,
            "verse": 1,
            "text": "114|1|[Ej Pajomʙar] Bigu: «Ba Parvardigori mardum panoh meʙaram"
        },
        {
            "chapter": 114,
            "verse": 2,
            "text": "114|2|Farmonravoi mardum"
        },
        {
            "chapter": 114,
            "verse": 3,
            "text": "114|3|Ma'ʙudi mardum"
        },
        {
            "chapter": 114,
            "verse": 4,
            "text": "114|4|Az sarri [sajtoni] vasvasagar, ki [ʙa hangomi zikri Alloh taolo] pinhon megardad"
        },
        {
            "chapter": 114,
            "verse": 5,
            "text": "114|5|Hamone, ki dar dilhoi mardum vasvasa mekunad"
        },
        {
            "chapter": 114,
            "verse": 6,
            "text": "114|6|Az cinnijon [ʙosad] va [jo az] odamijon»"
        }
    ]
}