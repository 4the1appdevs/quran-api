{"chapter":[{"chapter":102,"verse":1,"text":"102|1|parasparam perumanatikkuka enna karyam ninnale asrad'dhayilakkiyirikkunnu"},{"chapter":102,"verse":2,"text":"102|2|ninnal savakutirannal sandarsikkunnat varekkum"},{"chapter":102,"verse":3,"text":"102|3|nis'sansayam, ninnal valiye arinn keallum"},{"chapter":102,"verse":4,"text":"102|4|pinneyum nis'sansayam ninnal valiye arinn keallum"},{"chapter":102,"verse":5,"text":"102|5|nis'sansayam, ninnal drdhamaya ariv ariyumayirunnenkil"},{"chapter":102,"verse":6,"text":"102|6|jvalikkunna narakatte ninnal kanuka tanne ceyyum"},{"chapter":102,"verse":7,"text":"102|7|pinne tirccayayum ninnal atine drdhamayum kannal kanuka tanne ceyyum"},{"chapter":102,"verse":8,"text":"102|8|pinnit a divasattil sukhanubhavannale parri tirccayayum ninnal ceadyam ceyyappetukatanne ceyyum"}]}