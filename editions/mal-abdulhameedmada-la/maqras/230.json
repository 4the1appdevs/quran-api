{
    "maqras": [
        {
            "chapter": 73,
            "verse": 20,
            "text": "73|20|niyum ninre kuteyullavaril oru vibhagavum ratriyute mikkavarum munnil rantu bhagavum (cilappeal) pakutiyum (cilappeal) munnileannum ninnu namaskarikkunnunt enn tirccayayum ninre raksitavinnariyam. allahuvan ratriyeyum pakalineyum kanakkakkunnat‌. ninnalkk at kliptappetuttanavukayillenn avannariyam. atinal avan ninnalkk ilav ceytirikkunnu. akayal ninnal khur'anil ninn sekaryappettat otikkeant namaskarikkuka. ninnalute kuttattil reagikalum bhumiyil sancaricc allahuvinre anugraham tetikkeantirikkunna vere cilarum allahuvinre margattil yud'dham ceyyunna marr cilarum untakum enn allahuvinnariyam. atinal atil (khur'anil) ninn sekaryappettat ninnal parayanam ceytu keallukayum namaskaram muraprakaram nirvahikkukayum sakatt nalkukayum allahuvinn uttamamaya katam nalkukayum ceyyuka. svadehannalkk venti ninnal entearu nanma munkutti ceyt vekkukayanenkilum allahuvinkal at gunakaravum erravum mahattaya pratiphalamullatumayi ninnal kantettunnatan‌. ninnal allahuveat papameacanam tetukayum ceyyuka. tirccayayum allahu ere pearukkunnavanum karunanidhiyumakunnu"
        },
        {
            "chapter": 74,
            "verse": 1,
            "text": "74|1|he, putaccu mutiyavane"
        },
        {
            "chapter": 74,
            "verse": 2,
            "text": "74|2|elunnerr (janannale) takkit ceyyuka"
        },
        {
            "chapter": 74,
            "verse": 3,
            "text": "74|3|ninre raksitavine mahatvappetuttukayum"
        },
        {
            "chapter": 74,
            "verse": 4,
            "text": "74|4|ninre vastrannal sud'dhiyakkukayum"
        },
        {
            "chapter": 74,
            "verse": 5,
            "text": "74|5|papam vetiyukayum ceyyuka"
        },
        {
            "chapter": 74,
            "verse": 6,
            "text": "74|6|kututal nettam keaticcu keant ni audaryam ceyyarut‌"
        },
        {
            "chapter": 74,
            "verse": 7,
            "text": "74|7|ninre raksitavinu venti ni ksama kaikkealluka"
        },
        {
            "chapter": 74,
            "verse": 8,
            "text": "74|8|ennal kahalattil mulakkappettal"
        },
        {
            "chapter": 74,
            "verse": 9,
            "text": "74|9|ann at oru prayasakaramaya divasamayirikkum"
        },
        {
            "chapter": 74,
            "verse": 10,
            "text": "74|10|satyanisedhikalkk eluppamullatallatta oru divasam"
        },
        {
            "chapter": 74,
            "verse": 11,
            "text": "74|11|enneyum nan ekanayikkeant srsticca oruttaneyum vittekkuka"
        },
        {
            "chapter": 74,
            "verse": 12,
            "text": "74|12|avann nan samrd'dhamaya sampatt untakki keatukkukayum ceytu"
        },
        {
            "chapter": 74,
            "verse": 13,
            "text": "74|13|sannad'dharayi nilkkunna santatikaleyum"
        },
        {
            "chapter": 74,
            "verse": 14,
            "text": "74|14|avannu nan nalla sekaryannal ceytu keatukkukayum ceytu"
        },
        {
            "chapter": 74,
            "verse": 15,
            "text": "74|15|pinneyum nan kututal keatukkanamenn avan meahikkunnu"
        },
        {
            "chapter": 74,
            "verse": 16,
            "text": "74|16|alla, tirccayayum avan nam'mute drstantannaleat matsaryam kanikkunnavanayirikkunnu"
        },
        {
            "chapter": 74,
            "verse": 17,
            "text": "74|17|prayasamulla oru kayarram kayaran nam valiye avane nirbandhikkunnatan‌"
        },
        {
            "chapter": 74,
            "verse": 18,
            "text": "74|18|tirccayayum avaneannu cinticcu, avaneannu kanakkakkukayum ceytu"
        },
        {
            "chapter": 74,
            "verse": 19,
            "text": "74|19|atinal avan nasikkatte. ennaneyanavan kanakkakkiyat‌"
        },
        {
            "chapter": 74,
            "verse": 20,
            "text": "74|20|vintum avan nasikkatte, ennaneyanavan kanakkakkiyat‌"
        },
        {
            "chapter": 74,
            "verse": 21,
            "text": "74|21|pinnit avaneannu neakki"
        },
        {
            "chapter": 74,
            "verse": 22,
            "text": "74|22|pinne avan mukham culikkukayum mukham karuppikkukayum ceytu"
        },
        {
            "chapter": 74,
            "verse": 23,
            "text": "74|23|pinne avan pinneatt marukayum ahankaram natikkukayum ceytu"
        },
        {
            "chapter": 74,
            "verse": 24,
            "text": "74|24|ennittavan parannu: it (aril ninnea) ud'dharikkappetunna maranamallate marreannumalla"
        },
        {
            "chapter": 74,
            "verse": 25,
            "text": "74|25|it manusyanre vakkallate marreannumalla"
        },
        {
            "chapter": 74,
            "verse": 26,
            "text": "74|26|valiye nan avane sakharil (narakattil) itt erikkunnatan‌"
        },
        {
            "chapter": 74,
            "verse": 27,
            "text": "74|27|sakhar ennal entanenn ninakkariyumea"
        },
        {
            "chapter": 74,
            "verse": 28,
            "text": "74|28|at onnum bakkiyakkukayea vittukalayukayea illa"
        },
        {
            "chapter": 74,
            "verse": 29,
            "text": "74|29|at teali karicc rupam marrikkalayunnatan‌"
        },
        {
            "chapter": 74,
            "verse": 30,
            "text": "74|30|atinre melneattattin patteampat perunt‌"
        },
        {
            "chapter": 74,
            "verse": 31,
            "text": "74|31|narakattinre melneattakkarayi nam malakkukale matraman niscayiccirikkunnat‌. avarute ennatte nam satyanisedhikalkk oru pariksanam matramakkiyirikkunnu. vedam nalkappettittullavarkk drdhabeadhyam varuvanum satyavisvasikalkk visvasam vard'dhikkanum vedam nalkappettavarum satyavisvasikalum sansayattilakappetatirikkanum allahu entearu upamayan itu keant uddesiccittullatenn hrdayannalil reagamullavarum satyanisedhikalum parayuvanum ventiyatre at‌. aprakaram allahu tan uddesikkunnavare pilappikkukayum, tan uddesikkunnavare nervaliyilakkukayum ceyyunnu. ninre raksitavinre sain'yannale avanallate marrarum ariyukayilla. it manusyarkk oru ulbeadhanamallate marreannumalla"
        },
        {
            "chapter": 74,
            "verse": 32,
            "text": "74|32|nis'sansayam, candrane tanneyana satyam"
        },
        {
            "chapter": 74,
            "verse": 33,
            "text": "74|33|ratri pinnitt peakumpeal atine tanneyana satyam"
        },
        {
            "chapter": 74,
            "verse": 34,
            "text": "74|34|prabhatam pularnnal atu tanneyana satyam"
        },
        {
            "chapter": 74,
            "verse": 35,
            "text": "74|35|tirccayayum at (narakam) geravamulla karyannalil onnakunnu"
        },
        {
            "chapter": 74,
            "verse": 36,
            "text": "74|36|manusyarkk oru takkitenna nilayil"
        },
        {
            "chapter": 74,
            "verse": 37,
            "text": "74|37|atayat ninnalil ninn munneatt peakuvanea, pinneatt vekkuvanea uddesikkunnavarkk‌"
        },
        {
            "chapter": 74,
            "verse": 38,
            "text": "74|38|orea vyaktiyum tan sampadiccu veccatin panayappettavanakunnu"
        },
        {
            "chapter": 74,
            "verse": 39,
            "text": "74|39|valatupaksakkarealike"
        },
        {
            "chapter": 74,
            "verse": 40,
            "text": "74|40|cila svargatteappukalilayirikkum avar. avar anvesikkum"
        },
        {
            "chapter": 74,
            "verse": 41,
            "text": "74|41|kurravalikalepparri"
        },
        {
            "chapter": 74,
            "verse": 42,
            "text": "74|42|ninnale narakattil pravesippiccat enteannanenn‌"
        },
        {
            "chapter": 74,
            "verse": 43,
            "text": "74|43|avar (kurravalikal) marupati parayum: nannal namaskarikkunnavarute kuttattilayilla"
        },
        {
            "chapter": 74,
            "verse": 44,
            "text": "74|44|nannal agatikk aharam nalkumayirunnilla"
        },
        {
            "chapter": 74,
            "verse": 45,
            "text": "74|45|teannivasattil mulukunnavarute kute nannalum mulukumayirunnu"
        },
        {
            "chapter": 74,
            "verse": 46,
            "text": "74|46|pratiphalattinre naline nannal nisedhiccu kalayumayirunnu"
        },
        {
            "chapter": 74,
            "verse": 47,
            "text": "74|47|annaneyirikke a urappaya maranam nannalkk vannetti"
        },
        {
            "chapter": 74,
            "verse": 48,
            "text": "74|48|ini avarkk suparsakkarute suparsayeannum prayeajanappetukayilla"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "74|49|ennirikke avarkkentu parri? avar ulbeadhanattil ninn tirinnukalayunnavarayirikkunnu"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "74|50|avar virali piticca kalutakaleppealirikkunnu"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "74|51|sinhattil ninn otiraksappetunna (kalutakal)"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "74|52|alla, avaril orearuttarum agrahikkunnu; tanikk allahuvinkal ninn nivarttiya etukal nalkappetanamenn‌"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "74|53|alla; pakse, avar paraleakatte bhayappetunnilla"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "74|54|alla; tirccayayum it oru ulbeadhanamakunnu"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "74|55|akayal ar uddesikkunnuvea avarat ormiccu keallatte"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "74|56|allahu uddesikkunnuvenkilallate avar ormikkunnatalla. avanakunnu bhaktikkavakasappettavan; papameacanattin avakasappettavan"
        }
