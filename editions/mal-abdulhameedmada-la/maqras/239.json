{
    "maqras": [
        {
            "chapter": 94,
            "verse": 1,
            "text": "94|1|ninakk ninre hrdayam nam visalatayullatakki tannille"
        },
        {
            "chapter": 94,
            "verse": 2,
            "text": "94|2|ninnil ninn ninre a bharam nam irakkivekkukayum ceytu"
        },
        {
            "chapter": 94,
            "verse": 3,
            "text": "94|3|ninre mutukine nericcu kalannataya (bharam)"
        },
        {
            "chapter": 94,
            "verse": 4,
            "text": "94|4|ninakk ninre kirtti nam uyarttittarikayum ceytirikkunnu"
        },
        {
            "chapter": 94,
            "verse": 5,
            "text": "94|5|ennal tirccayayum nerukkattinre kute oru eluppamuntayirikkum"
        },
        {
            "chapter": 94,
            "verse": 6,
            "text": "94|6|tirccayayum nerukkattinre kute oru eluppamuntayirikkum"
        },
        {
            "chapter": 94,
            "verse": 7,
            "text": "94|7|akayal ninakk oliv kittiyal ni ad'dhvanikkuka"
        },
        {
            "chapter": 94,
            "verse": 8,
            "text": "94|8|ninre raksitavilekk tanne ninre agraham samarppikkukayum ceyyuka"
        },
        {
            "chapter": 95,
            "verse": 1,
            "text": "95|1|attiyum, olivum"
        },
        {
            "chapter": 95,
            "verse": 2,
            "text": "95|2|sinaparvvatavum"
        },
        {
            "chapter": 95,
            "verse": 3,
            "text": "95|3|nirbhayatvamulla i rajyavum tanneyana satyam"
        },
        {
            "chapter": 95,
            "verse": 4,
            "text": "95|4|tirccayayum manusyane nam erravum nalla ghatanayeatu kuti srsticcirikkunnu"
        },
        {
            "chapter": 95,
            "verse": 5,
            "text": "95|5|pinnit avane nam adhamaril adhamanakkittirttu"
        },
        {
            "chapter": 95,
            "verse": 6,
            "text": "95|6|visvasikkukayum salkarm'mannal pravarttikkukayum ceytavarealike. ennal avarkkakatte murinn peakatta pratiphalamuntayirikkum"
        },
        {
            "chapter": 95,
            "verse": 7,
            "text": "95|7|ennirikke itin sesam paraleakatte pratiphala natapatiyute karyattil (nabiye,) ninne nisedhiccu tallan ent n'yayamanullat‌"
        },
        {
            "chapter": 95,
            "verse": 8,
            "text": "95|8|allahu vidhikarttakkalil veccu erravum valiya vidhikarttavallayea"
        },
        {
            "chapter": 96,
            "verse": 1,
            "text": "96|1|srsticcavanaya ninre raksitavinre namattil vayikkuka"
        },
        {
            "chapter": 96,
            "verse": 2,
            "text": "96|2|manusyane avan bhrunattil ninn srsticcirikkunnu"
        },
        {
            "chapter": 96,
            "verse": 3,
            "text": "96|3|ni vayikkuka ninre raksitav erravum valiya audaryavanakunnu"
        },
        {
            "chapter": 96,
            "verse": 4,
            "text": "96|4|pena keant pathippiccavan"
        },
        {
            "chapter": 96,
            "verse": 5,
            "text": "96|5|manusyan ariyattat avan pathippiccirikkunnu"
        },
        {
            "chapter": 96,
            "verse": 6,
            "text": "96|6|nis'sansayam manusyan dhikkariyayi tirunnu"
        },
        {
            "chapter": 96,
            "verse": 7,
            "text": "96|7|tanne svayam paryaptanayi kantatinal"
        },
        {
            "chapter": 96,
            "verse": 8,
            "text": "96|8|tirccayayum ninre raksitavilekkan matakkam"
        },
        {
            "chapter": 96,
            "verse": 9,
            "text": "96|9|vilakkunnavane ni kantuvea"
        },
        {
            "chapter": 96,
            "verse": 10,
            "text": "96|10|oru atiyane, avan namaskariccal"
        },
        {
            "chapter": 96,
            "verse": 11,
            "text": "96|11|addeham sanmargattilanenkil , (a vilakkunnavanre avastha entayirikkumenn‌) ni kantuvea"
        },
        {
            "chapter": 96,
            "verse": 12,
            "text": "96|12|athava addeham suksmata kai keallan kalpiccirikkukayanenkil"
        },
        {
            "chapter": 96,
            "verse": 13,
            "text": "96|13|avan (a vilakkunnavan) nisedhiccu tallukayum tirinnukalayukayum ceytirikkayanenkil (avanre avastha entayirikkumenn‌) ni kantuvea"
        },
        {
            "chapter": 96,
            "verse": 14,
            "text": "96|14|avan manas'silakkiyittille, allahu kanunnunetann‌"
        },
        {
            "chapter": 96,
            "verse": 15,
            "text": "96|15|nis'sansayam. avan viramiccittillenkal nam a kutuma piticcu valikkuka tanne ceyyum"
        },
        {
            "chapter": 96,
            "verse": 16,
            "text": "96|16|kallam parayunna , papam ceyyunna kutuma"
        },
        {
            "chapter": 96,
            "verse": 17,
            "text": "96|17|ennitt avan avanre sabhayilullavare viliccukeallatte"
        },
        {
            "chapter": 96,
            "verse": 18,
            "text": "96|18|nam sabaniyattine (siksa natappakkunna malakkukale) viliccukeallam"
        },
        {
            "chapter": 96,
            "verse": 19,
            "text": "96|19|nis'sansayam; ni avane anusariccu peakarut , ni pranamikkukayum samipyam netukayum ceyyuka"
        },
        {
            "chapter": 97,
            "verse": 1,
            "text": "97|1|tirccayayum nam itine (khur'anine) nirnayattinre ratriyil avatarippiccirikkunnu"
        },
        {
            "chapter": 97,
            "verse": 2,
            "text": "97|2|nirnayattinre ratri ennal entanenn ninakkariyamea"
        },
        {
            "chapter": 97,
            "verse": 3,
            "text": "97|3|nirnayattinre ratri ayiram masattekkal uttamamakunnu"
        },
        {
            "chapter": 97,
            "verse": 4,
            "text": "97|4|malakkukalum atmavum avarute raksitavinre ellakaryatte sambandhiccumulla uttaravumayi a ratriyil iranni varunnu"
        },
        {
            "chapter": 97,
            "verse": 5,
            "text": "97|5|prabhateadayam vare at samadhanamatre"
        },
        {
            "chapter": 98,
            "verse": 1,
            "text": "98|1|vedakkarilum bahudaivavisvasikalilum petta satyanisedhikal vyaktamaya teliv tannalkk kittunnat vare (avisvasattil ninn‌) veritt pearunnavarayittilla"
        },
        {
            "chapter": 98,
            "verse": 2,
            "text": "98|2|atayat parisud'dhi nalkappetta etukal otikelpikkunna, allahuvinkal ninnulla oru dutan (varunnatu vare)"
        },
        {
            "chapter": 98,
            "verse": 3,
            "text": "98|3|avayil (etukalil) vakratayillatta rekhakalanullat‌"
        },
        {
            "chapter": 98,
            "verse": 4,
            "text": "98|4|vedam nalkappettavar avarkk vyaktamaya teliv vannukittiyatin sesamallate bhinnikkukayuntayittilla"
        },
        {
            "chapter": 98,
            "verse": 5,
            "text": "98|5|kil‌vanakkam allahuvin matram akki keant rjumanaskaraya nilayil avane aradhikkuvanum, namaskaram nilanirttuvanum sakatt nalkuvanum allate avareat kalpikkappettittilla. atatre vakratayillatta matam"
        },
        {
            "chapter": 98,
            "verse": 6,
            "text": "98|6|tirccayayum vedakkarilum bahudaivavisvasikalilumpetta satyanisedhikal narakagniyilakunnu. avaratil nityavasikalayirikkum . akkuttar tanneyakunnu srstikalil measappettavar"
        },
        {
            "chapter": 98,
            "verse": 7,
            "text": "98|7|tirccayayum visvasikkukayum salkarm'mannal pravarttikkukayum ceytavararea avar tanneyakunnu srstikalil uttamar"
        },
        {
            "chapter": 98,
            "verse": 8,
            "text": "98|8|avarkk avarute raksitavinkalulla pratiphalam talbhagattu kuti aruvikal olukunna, sthiravasattinulla svargatteappukalakunnu. avaratil nityavasikalayirikkum; ennennekkumayitt‌. allahu avare parri trptippettirikkunnu. avar avane parriyum trptippettirikkunnu. etearuvan tanre raksitavine bhayappettuvea avannullatakunnu at‌"
        },
        {
            "chapter": 99,
            "verse": 1,
            "text": "99|1|bhumi prakampanam keallikkappettal - atinre bhayankaramaya a prakampanam"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "99|2|bhumi atinre bharannal puram tallukayum"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "99|3|atin entuparri enn manusyan parayukayum ceytal"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "99|4|anne divasam at (bhumi) atinre varttamanannal parannariyikkunnatan‌"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "99|5|ninre raksitav atin beadhanam nalkiyat nimittam"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "99|6|anne divasam manusyar pala sanghannalayi purappetunnatan‌. avarkk avarute karm'mannal kanikkappetentatinayitt‌"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "99|7|appeal ar oru anuvinre tukkam nanmaceytirunnuvea avanat kanum"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "99|8|ar oru anuvinre tukkam tinma ceytirunnuvea avan atum kanum"
        },
        {
            "chapter": 100,
            "verse": 1,
            "text": "100|1|kitaccu keant otunnavayum"
        },
        {
            "chapter": 100,
            "verse": 2,
            "text": "100|2|annane (kulamp kallil) urasi tippeari parappikkunnavayum"
        },
        {
            "chapter": 100,
            "verse": 3,
            "text": "100|3|ennitt prabhatattil akramanam natattunnavayum"
        },
        {
            "chapter": 100,
            "verse": 4,
            "text": "100|4|anneratt peatipatalam ilakkivittavayum"
        },
        {
            "chapter": 100,
            "verse": 5,
            "text": "100|5|atilute (satru) sanghattinre natuvil pravesiccavayum (kutirakal) tanne satyam"
        },
        {
            "chapter": 100,
            "verse": 6,
            "text": "100|6|tirccayayum manusyan tanre raksitavineat nandikettavan tanne"
        },
        {
            "chapter": 100,
            "verse": 7,
            "text": "100|7|tirccayayum avan atinn saksyam vahikkunnavanumakunnu"
        },
        {
            "chapter": 100,
            "verse": 8,
            "text": "100|8|tirccayayum avan dhanatteatulla sneham kathinamayavanakunnu"
        }
    ]
