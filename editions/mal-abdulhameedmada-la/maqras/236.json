{
    "maqras": [
        {
            "chapter": 84,
            "verse": 1,
            "text": "84|1|akasam pilarumpeal"
        },
        {
            "chapter": 84,
            "verse": 2,
            "text": "84|2|at atinre raksitavin kilpetukayum ceyyumpeal-at (annane kilpetan) katappettirikkunnutanum"
        },
        {
            "chapter": 84,
            "verse": 3,
            "text": "84|3|bhumi nittappetumpeal"
        },
        {
            "chapter": 84,
            "verse": 4,
            "text": "84|4|atilullat at (purattekk‌) itukayum, at kaliyayittirukayum ceyyumpeal"
        },
        {
            "chapter": 84,
            "verse": 5,
            "text": "84|5|atinre raksitavin at kilpetukayum ceyyumpeal- at (annane kilpetan) katappettirikkunnu tanum"
        },
        {
            "chapter": 84,
            "verse": 6,
            "text": "84|6|he, manusya, ni ninre raksitavinkalekk katutta ad'dhvanam natatti cellunnavanum annane avanumayi kantumuttunnavanumakunnu"
        },
        {
            "chapter": 84,
            "verse": 7,
            "text": "84|7|ennal (paraleakatt‌) etearuvann tanre rekha valatukaiyyil nalkappettuvea"
        },
        {
            "chapter": 84,
            "verse": 8,
            "text": "84|8|avan laghuvaya vicaranaykk (matram) vidheyanakunnatan‌"
        },
        {
            "chapter": 84,
            "verse": 9,
            "text": "84|9|avan avanre svantakkarute atuttekk santustanayikkeant tiriccupeakukayum ceyyum"
        },
        {
            "chapter": 84,
            "verse": 10,
            "text": "84|10|ennal etearuvan tanre rekha avanre mutukinre pinnilute keatukkappettuvea"
        },
        {
            "chapter": 84,
            "verse": 11,
            "text": "84|11|avan nasame enn nilavilikkukayum"
        },
        {
            "chapter": 84,
            "verse": 12,
            "text": "84|12|alikkattunna narakagniyil katann eriyukayum ceyyum"
        },
        {
            "chapter": 84,
            "verse": 13,
            "text": "84|13|tirccayayum avan avanre svantakkarkkitayil santeasatteate kaliyunnavanayirunnu"
        },
        {
            "chapter": 84,
            "verse": 14,
            "text": "84|14|tirccayayum avan dhariccu; avan matanni varunnate alla enn‌"
        },
        {
            "chapter": 84,
            "verse": 15,
            "text": "84|15|ate, tirccayayum avanre raksitav avanepparri kantariyunnavanayirikkunnu"
        },
        {
            "chapter": 84,
            "verse": 16,
            "text": "84|16|ennal astamayaseabhayekkeant nan satyam ceytu parayunnu"
        },
        {
            "chapter": 84,
            "verse": 17,
            "text": "84|17|ratriyum atu onnicc cerkkunnavayum keantum"
        },
        {
            "chapter": 84,
            "verse": 18,
            "text": "84|18|candran purnnata prapikkumpeal atine keantum"
        },
        {
            "chapter": 84,
            "verse": 19,
            "text": "84|19|tirccayayum ninnal ghattanghattamayi kayarikkeantirikkunnatan‌"
        },
        {
            "chapter": 84,
            "verse": 20,
            "text": "84|20|ennal avarkkentuparri? avar visvasikkunnilla"
        },
        {
            "chapter": 84,
            "verse": 21,
            "text": "84|21|avarkk khur'an otikeatukkappettal avar sujud ceyyunnumilla"
        },
        {
            "chapter": 84,
            "verse": 22,
            "text": "84|22|pakse avisvasikal nisedhiccu tallukayan‌"
        },
        {
            "chapter": 84,
            "verse": 23,
            "text": "84|23|avar manas'sukalil suksicc vekkunnatinepparri allahu nallavannam ariyunnavanakunnu"
        },
        {
            "chapter": 84,
            "verse": 24,
            "text": "84|24|akayal (nabiye,) ni avarkk vedanayeriya oru siksayepparri santeasavartta ariyikkuka"
        },
        {
            "chapter": 84,
            "verse": 25,
            "text": "84|25|visvasikkukayum salkarm'mannal pravarttikkukayum ceytavarkkealike. avarkk murinnu peakatta pratiphalamunt‌"
        },
        {
            "chapter": 85,
            "verse": 1,
            "text": "85|1|naksatramandalannalulla akasam tanneyana satyam"
        },
        {
            "chapter": 85,
            "verse": 2,
            "text": "85|2|vagdanam ceyyappetta a divasam tanneyana satyam"
        },
        {
            "chapter": 85,
            "verse": 3,
            "text": "85|3|saksiyum saksyam vahikkappetunna karyavum tanneyana satyam"
        },
        {
            "chapter": 85,
            "verse": 4,
            "text": "85|4|a kitanninre alkkar nasiccu peakatte"
        },
        {
            "chapter": 85,
            "verse": 5,
            "text": "85|5|atayat viraku niracca tiyute alkkar"
        },
        {
            "chapter": 85,
            "verse": 6,
            "text": "85|6|avar atinkal irikkunnavarayirunna sandarbham"
        },
        {
            "chapter": 85,
            "verse": 7,
            "text": "85|7|satyavisvasikalekkeant tannal ceyyunnatin avar drk‌saksikalayirunnu"
        },
        {
            "chapter": 85,
            "verse": 8,
            "text": "85|8|pratapasaliyum stutyarhanumaya allahuvil avar visvasikkunnu ennat matramayirunnu avarute (satyavisvasikalute) mel avar (marddakar) cumattiya kurram"
        },
        {
            "chapter": 85,
            "verse": 9,
            "text": "85|9|akasannaluteyum bhumiyuteyum mel adhipatyam ullavanumaya (allahuvil). allahu ellakaryattinum saksiyakunnu"
        },
        {
            "chapter": 85,
            "verse": 10,
            "text": "85|10|satyavisvasikaleyum satyavisvasinikaleyum marddikkukayum, pinnit pascattapikkatirikkukayum ceytavararea avarkku narakasiksayunt‌. tircca. avarkk cuttukarikkunna siksayunt‌"
        },
        {
            "chapter": 85,
            "verse": 11,
            "text": "85|11|visvasikkukayum salkarm'mannal pravarttikkukayum ceytavararea avarkk talbhagattukuti aruvikal olukunna svargatteappukalunt‌; tircca. atatre valiya vijayam"
        },
        {
            "chapter": 85,
            "verse": 12,
            "text": "85|12|tirccayayum ninre raksitavinre pituttam kathinamayat tanneyakunnu"
        },
        {
            "chapter": 85,
            "verse": 13,
            "text": "85|13|tirccayayum avan tanneyan adyamayi untakkunnatum avartticc untakkunnatum"
        },
        {
            "chapter": 85,
            "verse": 14,
            "text": "85|14|avan ere pearukkunnavanum ere snehamullavanum"
        },
        {
            "chapter": 85,
            "verse": 15,
            "text": "85|15|sinhasanattinre utamayum, mahatvamullavanum"
        },
        {
            "chapter": 85,
            "verse": 16,
            "text": "85|16|tan uddesikkunnatentea at tikaccum pravarttikamakkunnavanuman‌"
        },
        {
            "chapter": 85,
            "verse": 17,
            "text": "85|17|a sain'yannalute varttamanam ninakk vannukittiyirikkunnea"
        },
        {
            "chapter": 85,
            "verse": 18,
            "text": "85|18|athava phir'aunreyum thamudinreyum (varttamanam)"
        },
        {
            "chapter": 85,
            "verse": 19,
            "text": "85|19|alla, satyanisedhikal nisedhiccu tallunnatilakunnu erpettittullat‌"
        },
        {
            "chapter": 85,
            "verse": 20,
            "text": "85|20|allahu avarute pinvasattukuti (avare) valayam ceytukeantirikkunnavanakunnu"
        },
        {
            "chapter": 85,
            "verse": 21,
            "text": "85|21|alla, at mahatvameriya oru khur'anakunnu"
        },
        {
            "chapter": 85,
            "verse": 22,
            "text": "85|22|sanraksitamaya oru phalakattilan atullat‌"
        },
        {
            "chapter": 86,
            "verse": 1,
            "text": "86|1|akasam tanneyana, ratriyil varunnatu tanneyana satyam"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "86|2|ratriyil varunnat ennal entanenn ninakkariyumea"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "86|3|tulacc kayarunna naksatramatre at‌"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "86|4|tanre karyattil oru melneattakkaranuntayikkeantallate yatearalumilla"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "86|5|ennal manusyan cinticcu neakkatte tan entil ninnan srstikkappettirikkunnat enn‌"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "86|6|tericcu vilunna oru dravakattil ninnatre avan srstikkappettirikkunnat‌"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "86|7|mutukellinum, variyellukalkkumitayil ninn at purattu varunnu"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "86|8|avane (manusyane) tiriccukeantu varan tirccayayum avan (allahu) kalivullavanakunnu"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "86|9|rahasyannal pariseadhikkappetunna divasam"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "86|10|appeal avan (manusyan‌) yatearu saktiyea, sahayiyea untayirikkukayilla"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "86|11|avartticc malapeyyikkunna akasattekkeantum"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "86|12|sasyalatadikal mulappikkunna bhumiyekkeantum satyam"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "86|13|tirccayayum itu nirnayakamaya oru vakkakunnu"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "86|14|itu tamasayalla"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "86|15|tirccayayum avar (valiya) tantram prayeagiccu keantirikkum"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "86|16|nanum (valiya) tantram prayeagiccu keantirikkum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "86|17|akayal (nabiye,) ni satyanisedhikalkk kalatamasam nalkuka. alpasamayattekk avarkk tamasam nalkiyekkuka"
        }
    ]
}