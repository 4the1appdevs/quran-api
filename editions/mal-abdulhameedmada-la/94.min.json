{"chapter":[{"chapter":94,"verse":1,"text":"94|1|ninakk ninre hrdayam nam visalatayullatakki tannille"},{"chapter":94,"verse":2,"text":"94|2|ninnil ninn ninre a bharam nam irakkivekkukayum ceytu"},{"chapter":94,"verse":3,"text":"94|3|ninre mutukine nericcu kalannataya (bharam)"},{"chapter":94,"verse":4,"text":"94|4|ninakk ninre kirtti nam uyarttittarikayum ceytirikkunnu"},{"chapter":94,"verse":5,"text":"94|5|ennal tirccayayum nerukkattinre kute oru eluppamuntayirikkum"},{"chapter":94,"verse":6,"text":"94|6|tirccayayum nerukkattinre kute oru eluppamuntayirikkum"},{"chapter":94,"verse":7,"text":"94|7|akayal ninakk oliv kittiyal ni ad'dhvanikkuka"},{"chapter":94,"verse":8,"text":"94|8|ninre raksitavilekk tanne ninre agraham samarppikkukayum ceyyuka"}]}