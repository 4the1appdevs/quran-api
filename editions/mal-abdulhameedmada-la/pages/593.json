{
    "pages": [
        {
            "chapter": 89,
            "verse": 1,
            "text": "89|1|prabhatam tanneyana satyam"
        },
        {
            "chapter": 89,
            "verse": 2,
            "text": "89|2|pattu ratrikal tanneyana satyam"
        },
        {
            "chapter": 89,
            "verse": 3,
            "text": "89|3|irattayum orrayum tanneyana satyam"
        },
        {
            "chapter": 89,
            "verse": 4,
            "text": "89|4|ratri sancaricc keantirikke at tanneyana satyam"
        },
        {
            "chapter": 89,
            "verse": 5,
            "text": "89|5|atil (mel parannavayil) karyabeadhamullavann satyattin vakayuneta"
        },
        {
            "chapter": 89,
            "verse": 6,
            "text": "89|6|ad samudayatte keant ninre raksitav entu ceytuvenn ni kantille"
        },
        {
            "chapter": 89,
            "verse": 7,
            "text": "89|7|atayat tunukalute utamakalaya iram geatratte keant‌"
        },
        {
            "chapter": 89,
            "verse": 8,
            "text": "89|8|tattulyamayitteann rajyannalil srstikkappettittillatta geatram"
        },
        {
            "chapter": 89,
            "verse": 9,
            "text": "89|9|tal‌varayil paravetti kettitamuntakkiyavaraya thamud geatrattekkeantum"
        },
        {
            "chapter": 89,
            "verse": 10,
            "text": "89|10|anikalute alaya phir'aunekkeantum"
        },
        {
            "chapter": 89,
            "verse": 11,
            "text": "89|11|natukalil atikramam pravarttikkukayum"
        },
        {
            "chapter": 89,
            "verse": 12,
            "text": "89|12|avite kulappam vard'dhippikkukayum ceytavaranavar"
        },
        {
            "chapter": 89,
            "verse": 13,
            "text": "89|13|atinal ninre raksitav avarute mel siksayute cam'matti varsiccu"
        },
        {
            "chapter": 89,
            "verse": 14,
            "text": "89|14|tirccayayum ninre raksitav patiyirikkunna sthanattu tanneyunt‌"
        },
        {
            "chapter": 89,
            "verse": 15,
            "text": "89|15|ennal manusyane avanre raksitav pariksikkukayum annane avane adarikkukayum avan sekhyam nalkukayum ceytal avan parayum; enre raksitav enne adariccirikkunnu enn‌"
        },
        {
            "chapter": 89,
            "verse": 16,
            "text": "89|16|ennal avane (manusyane) avan pariksikkukayum ennittavanre upajivanam itunniyatakkukayum ceytal avan parayum; enre raksitav enne apamaniccirikkunnu enn‌"
        },
        {
            "chapter": 89,
            "verse": 17,
            "text": "89|17|alla, pakse ninnal anathaye adarikkunnilla"
        },
        {
            "chapter": 89,
            "verse": 18,
            "text": "89|18|pavappettavanre aharattin ninnal preatsahanam nalkunnumilla"
        },
        {
            "chapter": 89,
            "verse": 19,
            "text": "89|19|anantaravakasa svatt ninnal varikkutti tinnukayum ceyyunnu"
        },
        {
            "chapter": 89,
            "verse": 20,
            "text": "89|20|dhanatte ninnal amitamaya teatil snehikkukayum ceyyunnu"
        },
        {
            "chapter": 89,
            "verse": 21,
            "text": "89|21|alla, bhumi peatipeatiyayi peatikkappetukayum"
        },
        {
            "chapter": 89,
            "verse": 22,
            "text": "89|22|ninre raksitavum, aniyaniyayi malakkukalum varukayum"
        },
        {
            "chapter": 89,
            "verse": 23,
            "text": "89|23|ann narakam keantu varappetukayum ceytal! anne divasam manusyann orma varunnatan‌. eviteninnanavann orma varunnat‌"
        }
    ]
}