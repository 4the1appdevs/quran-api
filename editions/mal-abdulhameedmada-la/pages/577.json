{
    "pages": [
        {
            "chapter": 74,
            "verse": 48,
            "text": "74|48|ini avarkk suparsakkarute suparsayeannum prayeajanappetukayilla"
        },
        {
            "chapter": 74,
            "verse": 49,
            "text": "74|49|ennirikke avarkkentu parri? avar ulbeadhanattil ninn tirinnukalayunnavarayirikkunnu"
        },
        {
            "chapter": 74,
            "verse": 50,
            "text": "74|50|avar virali piticca kalutakaleppealirikkunnu"
        },
        {
            "chapter": 74,
            "verse": 51,
            "text": "74|51|sinhattil ninn otiraksappetunna (kalutakal)"
        },
        {
            "chapter": 74,
            "verse": 52,
            "text": "74|52|alla, avaril orearuttarum agrahikkunnu; tanikk allahuvinkal ninn nivarttiya etukal nalkappetanamenn‌"
        },
        {
            "chapter": 74,
            "verse": 53,
            "text": "74|53|alla; pakse, avar paraleakatte bhayappetunnilla"
        },
        {
            "chapter": 74,
            "verse": 54,
            "text": "74|54|alla; tirccayayum it oru ulbeadhanamakunnu"
        },
        {
            "chapter": 74,
            "verse": 55,
            "text": "74|55|akayal ar uddesikkunnuvea avarat ormiccu keallatte"
        },
        {
            "chapter": 74,
            "verse": 56,
            "text": "74|56|allahu uddesikkunnuvenkilallate avar ormikkunnatalla. avanakunnu bhaktikkavakasappettavan; papameacanattin avakasappettavan"
        },
        {
            "chapter": 75,
            "verse": 1,
            "text": "75|1|uyirttelunnelpinre nalukeant nanita satyam ceyyunnu"
        },
        {
            "chapter": 75,
            "verse": 2,
            "text": "75|2|kurrappetuttunna manas'sinekkeantum nan satyam ceytu parayunnu"
        },
        {
            "chapter": 75,
            "verse": 3,
            "text": "75|3|manusyan vicarikkunnuntea; nam avanre ellukale orumiccukuttukayillenn‌"
        },
        {
            "chapter": 75,
            "verse": 4,
            "text": "75|4|ate, nam avanre viralttumpukale pealum sarippetuttan kalivullavanayirikke"
        },
        {
            "chapter": 75,
            "verse": 5,
            "text": "75|5|pakse (ennittum) manusyan avanre bhavi jivitattil teannivasam ceyyan uddesikkunnu"
        },
        {
            "chapter": 75,
            "verse": 6,
            "text": "75|6|eppealan i uyirttelunnelpinre nal ennavan ceadikkunnu"
        },
        {
            "chapter": 75,
            "verse": 7,
            "text": "75|7|ennal kann ancippeakukayum"
        },
        {
            "chapter": 75,
            "verse": 8,
            "text": "75|8|candrann grahanam badhikkukayum"
        },
        {
            "chapter": 75,
            "verse": 9,
            "text": "75|9|suryanum candranum orumiccukuttappetukayum ceytal"
        },
        {
            "chapter": 75,
            "verse": 10,
            "text": "75|10|anne divasam manusyan parayum; eviteyan otiraksappetanullatenn‌"
        },
        {
            "chapter": 75,
            "verse": 11,
            "text": "75|11|illa. yatearu raksayumilla"
        },
        {
            "chapter": 75,
            "verse": 12,
            "text": "75|12|ninre raksitavinkalekkan anne divasam cennukutal"
        },
        {
            "chapter": 75,
            "verse": 13,
            "text": "75|13|anne divasam manusyan munkutti ceytatinepparriyum nittiveccatinepparriyum avann vivaramariyikkappetum"
        },
        {
            "chapter": 75,
            "verse": 14,
            "text": "75|14|tanneyumalla. manusyan tanikketiril tanne oru telivayirikkum"
        },
        {
            "chapter": 75,
            "verse": 15,
            "text": "75|15|avan olikalivukal samarppiccalum sari"
        },
        {
            "chapter": 75,
            "verse": 16,
            "text": "75|16|ni at (khur'an) dhrtippett hrdisthamakkan venti atum keant ninre nav calippikkenta"
        },
        {
            "chapter": 75,
            "verse": 17,
            "text": "75|17|tirccayayum atinre (khur'anre) samaharanavum at otittaralum nam'mute badhyatayakunnu"
        },
        {
            "chapter": 75,
            "verse": 18,
            "text": "75|18|annane nam at otittannal a ott ni pintutaruka"
        },
        {
            "chapter": 75,
            "verse": 19,
            "text": "75|19|pinnit at vivariccutaralum nam'mute badhyatayakunnu"
        }
    ]
}