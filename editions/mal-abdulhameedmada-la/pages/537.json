{
    "pages": [
        {
            "chapter": 56,
            "verse": 77,
            "text": "56|77|tirccayayum it adaraniyamaya oru khur'an tanneyakunnu"
        },
        {
            "chapter": 56,
            "verse": 78,
            "text": "56|78|bhadramayi suksikkappetta oru rekhayilakunnu at‌"
        },
        {
            "chapter": 56,
            "verse": 79,
            "text": "56|79|parisud'dhi nalkappettavarallate at sparsikkukayilla"
        },
        {
            "chapter": 56,
            "verse": 80,
            "text": "56|80|leakaraksitavinkal ninn avatarippikkappettatatre at‌"
        },
        {
            "chapter": 56,
            "verse": 81,
            "text": "56|81|appeal i varttamanattinre karyattilanea ninnal purampucc kanikkunnat‌"
        },
        {
            "chapter": 56,
            "verse": 82,
            "text": "56|82|satyatte nisedhikkuka ennat ninnal ninnalute vihitamakkukayanea"
        },
        {
            "chapter": 56,
            "verse": 83,
            "text": "56|83|ennal at (jivan) teantakkuliyil ettumpeal entukeantan (ninnalkkat piticcu nirttanakattat‌)"
        },
        {
            "chapter": 56,
            "verse": 84,
            "text": "56|84|ninnal anneratt neakkikkeantirikkumallea"
        },
        {
            "chapter": 56,
            "verse": 85,
            "text": "56|85|naman a vyaktiye sambandhiccitattealam ninnalekkalum atuttavan. pakse ninnal kantariyunnilla"
        },
        {
            "chapter": 56,
            "verse": 86,
            "text": "56|86|appeal ninnal (daivika niyamattin‌) vidheyarallattavaranenkil"
        },
        {
            "chapter": 56,
            "verse": 87,
            "text": "56|87|ninnalkkentukeant at (jivan) matakki etukkanavunnilla; ninnal satyavadikalanenkil"
        },
        {
            "chapter": 56,
            "verse": 88,
            "text": "56|88|appeal avan (mariccavan) samipyam sid'dhiccavaril pettavananenkil"
        },
        {
            "chapter": 56,
            "verse": 89,
            "text": "56|89|(avann‌) asvasavum visistamaya upajivanavum sukhanubhutiyute svargatteappum untayirikkum"
        },
        {
            "chapter": 56,
            "verse": 90,
            "text": "56|90|ennal avan valatupaksakkaril pettavananenkilea"
        },
        {
            "chapter": 56,
            "verse": 91,
            "text": "56|91|valatupaksakkarilpetta ninakk samadhanam ennayirikkum (avannu labhikkunna abhivadyam)"
        },
        {
            "chapter": 56,
            "verse": 92,
            "text": "56|92|ini avan durmargikalaya satyanisedhikalil pettavananenkilea"
        },
        {
            "chapter": 56,
            "verse": 93,
            "text": "56|93|cuttutilakkunna vellam keantulla salkkaravum"
        },
        {
            "chapter": 56,
            "verse": 94,
            "text": "56|94|narakattil vecculla cutterikkaluman‌. (avannullat‌)"
        },
        {
            "chapter": 56,
            "verse": 95,
            "text": "56|95|tirccayayum itu tanneyan urappulla yathart'thyam"
        },
        {
            "chapter": 56,
            "verse": 96,
            "text": "56|96|akayal ni ninre mahanaya raksitavinre namam prakirttikkuka"
        },
        {
            "chapter": 57,
            "verse": 1,
            "text": "57|1|akasannalilum bhumiyilumullatellam allahuvin prakirttanam ceytirikkunnu. avan pratapiyum yuktimanumatre"
        },
        {
            "chapter": 57,
            "verse": 2,
            "text": "57|2|avannakunnu akasannaluteyum bhumiyuteyum adhipatyam. avan jivippikkukayum marippikkukayum ceyyunnu. avan sarvvakaryattinum kalivullavanuman‌"
        },
        {
            "chapter": 57,
            "verse": 3,
            "text": "57|3|avan adiyum antimanum pratyaksamayavanum pareaksamayavanuman‌. avan sarvvakaryannalekkuriccum arivullavanuman‌"
        }
    ]
