{
    "rukus": [
        {
            "chapter": 105,
            "verse": 1,
            "text": "105|1|anakkarekkeant ninre raksitav pravartticcat ennane enn ni kantille"
        },
        {
            "chapter": 105,
            "verse": 2,
            "text": "105|2|avarute tantram avan pilavilakkiyille"
        },
        {
            "chapter": 105,
            "verse": 3,
            "text": "105|3|kuttankuttamayikkeantulla paksikale avarute nerkk avan ayakkukayum ceytu"
        },
        {
            "chapter": 105,
            "verse": 4,
            "text": "105|4|cuttupaluppicca kalimankallukalkeant avare eriyunnataya"
        },
        {
            "chapter": 105,
            "verse": 5,
            "text": "105|5|annane avan avare tinneatukkappetta vaikkeal turumpupealeyakki"
        }
    ]
}