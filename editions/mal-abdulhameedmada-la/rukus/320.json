{
    "rukus": [
        {
            "chapter": 26,
            "verse": 70,
            "text": "26|70|atayat ninnal enteannineyan aradhiccu keantirikkunnat enn tanre pitavineatum, tanre janannaleatum addeham ceadicca sandarbham"
        },
        {
            "chapter": 26,
            "verse": 71,
            "text": "26|71|avar parannu: nannal cila vigrahannale aradhikkukayum avayute mumpil ‍bhajanamirikkukayum ceyyunnu"
        },
        {
            "chapter": 26,
            "verse": 72,
            "text": "26|72|addeham parannu: ninnal prart'thikkumpeal avarat kelkkumea"
        },
        {
            "chapter": 26,
            "verse": 73,
            "text": "26|73|athava, avar ninnalkk upakaramea upadravamea ceyyumea"
        },
        {
            "chapter": 26,
            "verse": 74,
            "text": "26|74|avar parannu: alla, nannalute pitakkal aprakaram ceyyunnatayi nannal kantirikkunnu (enn matram)"
        },
        {
            "chapter": 26,
            "verse": 75,
            "text": "26|75|addeham parannu: appeal ninnal aradhiccu keantirikkunnat entineyanenn ninnal cinticcu neakkiyittuntea"
        },
        {
            "chapter": 26,
            "verse": 76,
            "text": "26|76|ninnalum ninnalute purvvapitakkalum"
        },
        {
            "chapter": 26,
            "verse": 77,
            "text": "26|77|ennal avar (daivannal) enre satrukkalakunnu leakaraksitav olike"
        },
        {
            "chapter": 26,
            "verse": 78,
            "text": "26|78|atayat enne srsticc enikk margadarsanam nalkikkeantirikkunnavan"
        },
        {
            "chapter": 26,
            "verse": 79,
            "text": "26|79|enikk aharam tarikayum kutinir tarikayum ceyyunnavan"
        },
        {
            "chapter": 26,
            "verse": 80,
            "text": "26|80|enikk reagam badhiccal avanan enne sukhappetuttunnat‌"
        },
        {
            "chapter": 26,
            "verse": 81,
            "text": "26|81|enne marippikkukayum pinnit jivippikkukayum ceyyunnavan"
        },
        {
            "chapter": 26,
            "verse": 82,
            "text": "26|82|pratiphalattinre nalil etearuvan enre terr pearuttutarumenn nan asikkunnuvea avan"
        },
        {
            "chapter": 26,
            "verse": 83,
            "text": "26|83|enre raksitave, enikk ni tatvajnanam nalkukayum enne ni sajjanannaleateappam cerkkukayum ceyyename"
        },
        {
            "chapter": 26,
            "verse": 84,
            "text": "26|84|pilkkalakkarkkitayil enikk ni salkirtti untakkename"
        },
        {
            "chapter": 26,
            "verse": 85,
            "text": "26|85|enne ni sukhasampurnnamaya svargattinre avakasikalil ‍pettavanakkename"
        },
        {
            "chapter": 26,
            "verse": 86,
            "text": "26|86|enre pitavin ni pearuttukeatukkename tirccayayum addeham valipilaccavarute kuttattilayirikkunnu"
        },
        {
            "chapter": 26,
            "verse": 87,
            "text": "26|87|avar (manusyar) uyirttelunnelpikkappetunna divasam enne ni apamanattilakkarute"
        },
        {
            "chapter": 26,
            "verse": 88,
            "text": "26|88|atayat svattea santanannalea prayeajanappetatta divasam"
        },
        {
            "chapter": 26,
            "verse": 89,
            "text": "26|89|kurramarra hrdayavumayi allahuvinkal ‍cennavarkkealike"
        },
        {
            "chapter": 26,
            "verse": 90,
            "text": "26|90|(ann‌) suksmata palikkunnavarkk svargam atuppikkappetunnatan‌"
        },
        {
            "chapter": 26,
            "verse": 91,
            "text": "26|91|durmargikalkk narakam turannu kanikkappetunnatuman‌"
        },
        {
            "chapter": 26,
            "verse": 92,
            "text": "26|92|avareat ceadikkappetukayum ceyyum: ninnal aradhiccirunnatellam eviteppeayi"
        },
        {
            "chapter": 26,
            "verse": 93,
            "text": "26|93|allahuvinu purame avar ninnale sahayikkukayea, svayam sahayam netukayea ceyyunnuntea"
        },
        {
            "chapter": 26,
            "verse": 94,
            "text": "26|94|turnn avarum (aradhyanmar) a durmargikalum atil ‍(narakattil ‍) mukham kutti vilttappetunnatan‌"
        },
        {
            "chapter": 26,
            "verse": 95,
            "text": "26|95|iblisinre muluvan sain'yannalum"
        },
        {
            "chapter": 26,
            "verse": 96,
            "text": "26|96|avite vecc an'yean'yam valakk kutikkeantirikke avar parayum"
        },
        {
            "chapter": 26,
            "verse": 97,
            "text": "26|97|allahuvana satyam! nannal vyaktamaya valiketil ‍tanneyayirunnu"
        },
        {
            "chapter": 26,
            "verse": 98,
            "text": "26|98|ninnalkk nannal leakaraksitavineat tulyata kalpikkunna samayatt‌"
        },
        {
            "chapter": 26,
            "verse": 99,
            "text": "26|99|nannale valipilappiccat a kurravalikalallate marrarumalla"
        },
        {
            "chapter": 26,
            "verse": 100,
            "text": "26|100|ippeal nannalkk suparsakkarayi arumilla"
        },
        {
            "chapter": 26,
            "verse": 101,
            "text": "26|101|urra suhrttumilla"
        },
        {
            "chapter": 26,
            "verse": 102,
            "text": "26|102|atinal ‍nannalkkeannu matannippeakan kalinnirunnenkil enkil ‍nannal satyavisvasikalute kuttattilakumayirunnu"
        },
        {
            "chapter": 26,
            "verse": 103,
            "text": "26|103|tirccayayum atil (manusyarkk‌) oru drstantamunt ennal avaril adhikaperum visvasikkunnavarayilla"
        },
        {
            "chapter": 26,
            "verse": 104,
            "text": "26|104|tirccayayum ninre raksitav tanneyakunnu pratapiyum karunanidhiyum"
        }
    ]
}