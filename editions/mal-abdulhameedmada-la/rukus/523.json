{
    "rukus": [
        {
            "chapter": 81,
            "verse": 1,
            "text": "81|1|suryan currippeatiyappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 2,
            "text": "81|2|naksatrannal utirnnu vilumpeal"
        },
        {
            "chapter": 81,
            "verse": 3,
            "text": "81|3|parvvatannal sancarippikkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 4,
            "text": "81|4|purnnagarbhinikalaya ottakannal avaganikkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 5,
            "text": "81|5|van'yamrgannal orumiccukuttappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 6,
            "text": "81|6|samudrannal alikkattikkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 7,
            "text": "81|7|atmavukal kuttiyinakkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 8,
            "text": "81|8|(jivaneate) kuliccu mutappetta penkuttiyeatu ceadikkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 9,
            "text": "81|9|tan entearu kurrattinan keallappettat enn‌"
        },
        {
            "chapter": 81,
            "verse": 10,
            "text": "81|10|(karm'mannal rekhappetuttiya) etukal turannuvekkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 11,
            "text": "81|11|uparileakam mara nikkikanikkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 12,
            "text": "81|12|jvalikkunna narakagni alikkattikkappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 13,
            "text": "81|13|svargam atuttu keantuvarappetumpeal"
        },
        {
            "chapter": 81,
            "verse": 14,
            "text": "81|14|orea vyaktiyum tan tayyarakkikkeantu vannittullat entenn ariyunnatan‌"
        },
        {
            "chapter": 81,
            "verse": 15,
            "text": "81|15|pinvannippeakunnavaye (naksatrannale) kkeant nan satyam ceytu parayunnu"
        },
        {
            "chapter": 81,
            "verse": 16,
            "text": "81|16|sancariccukeantirikkunnavayum apratyaksamayikkeantirikkunnavayum"
        },
        {
            "chapter": 81,
            "verse": 17,
            "text": "81|17|ratri ninnumpeal atu keantum"
        },
        {
            "chapter": 81,
            "verse": 18,
            "text": "81|18|prabhatam vitarnn varumpeal atu keantum (nan satyam ceytu parayunnu)"
        },
        {
            "chapter": 81,
            "verse": 19,
            "text": "81|19|tirccayayum it (khur'an) man'yanaya oru dutanre vakkakunnu"
        },
        {
            "chapter": 81,
            "verse": 20,
            "text": "81|20|saktiyullavanum, sinhasanasthanaya allahuvinkal sthanamullavanumaya (dutanre)"
        },
        {
            "chapter": 81,
            "verse": 21,
            "text": "81|21|avite anusarikkappetunnavanum visvastanumaya (dutanre)"
        },
        {
            "chapter": 81,
            "verse": 22,
            "text": "81|22|ninnalute kuttukaran (pravacakan) oru bhrantanalla tanne"
        },
        {
            "chapter": 81,
            "verse": 23,
            "text": "81|23|tirccayayum addehatte (jib‌ril enna dutane) pratyaksamaya mandalattil vecc addeham kantittunt‌"
        },
        {
            "chapter": 81,
            "verse": 24,
            "text": "81|24|addeham adrsyavarttayute karyattil pisukk kanikkunnavanumalla"
        },
        {
            "chapter": 81,
            "verse": 25,
            "text": "81|25|it (khur'an) sapikkappetta oru pisacinre vakkumalla"
        },
        {
            "chapter": 81,
            "verse": 26,
            "text": "81|26|appeal enneattan ninnal peakunnat‌"
        },
        {
            "chapter": 81,
            "verse": 27,
            "text": "81|27|it leakarkk ventiyulla oru ulbeadhanamallate marreannumalla"
        },
        {
            "chapter": 81,
            "verse": 28,
            "text": "81|28|atayat ninnalute kuttattil ninn nere nilakeallan uddesiccavarkk venti"
        },
        {
            "chapter": 81,
            "verse": 29,
            "text": "81|29|leakaraksitavaya allahu uddesikkunnuvenkilallate ninnal uddesikkukayilla"
        }
    ]
}