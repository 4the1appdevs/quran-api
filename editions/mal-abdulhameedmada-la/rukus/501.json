{
    "rukus": [
        {
            "chapter": 69,
            "verse": 38,
            "text": "69|38|ennal ninnal kanunnavayekkeant nan satyam ceyt parayunnu"
        },
        {
            "chapter": 69,
            "verse": 39,
            "text": "69|39|ninnal kanattavayekkeantum"
        },
        {
            "chapter": 69,
            "verse": 40,
            "text": "69|40|tirccayayum it man'yanaya oru dutanre vakku tanneyakunnu"
        },
        {
            "chapter": 69,
            "verse": 41,
            "text": "69|41|itearu kaviyute vakkalla. valare kuracce ninnal visvasikkunnullu"
        },
        {
            "chapter": 69,
            "verse": 42,
            "text": "69|42|oru jyeatsyanre vakkumalla. valarekkuracce ninnal aleaciccu manas'silakkunnullu"
        },
        {
            "chapter": 69,
            "verse": 43,
            "text": "69|43|it leakaraksitavinkal ninn avatarippikkappettatakunnu"
        },
        {
            "chapter": 69,
            "verse": 44,
            "text": "69|44|nam'mute peril addeham (pravacakan) valla vakkum ketticcamaccu parannirunnenkil"
        },
        {
            "chapter": 69,
            "verse": 45,
            "text": "69|45|addehatte nam valatukai keant pitikutukayum"
        },
        {
            "chapter": 69,
            "verse": 46,
            "text": "69|46|ennitt addehattinre jivanadi nam muriccukalayukayum ceyyumayirunnu"
        },
        {
            "chapter": 69,
            "verse": 47,
            "text": "69|47|appeal ninnalil arkkum addehattil ninn (siksaye) tatayanavilla"
        },
        {
            "chapter": 69,
            "verse": 48,
            "text": "69|48|tirccayayum it (khur'an) bhayabhaktiyullavarkk oru ulbeadhanamakunnu"
        },
        {
            "chapter": 69,
            "verse": 49,
            "text": "69|49|tirccayayum ninnalute kuttattil (itine) nisedhiccu tallunnavaruntenn namukkariyam"
        },
        {
            "chapter": 69,
            "verse": 50,
            "text": "69|50|tirccayayum it satyanisedhikalkk khedattin karanavumakunnu"
        },
        {
            "chapter": 69,
            "verse": 51,
            "text": "69|51|tirccayayum it drdhamaya yathart'thyamakunnu"
        },
        {
            "chapter": 69,
            "verse": 52,
            "text": "69|52|atinal ni ninre mahanaya raksitavinre namatte prakirttikkuka"
        }
    ]
}