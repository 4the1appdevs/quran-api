{
    "rukus": [
        {
            "chapter": 86,
            "verse": 1,
            "text": "86|1|akasam tanneyana, ratriyil varunnatu tanneyana satyam"
        },
        {
            "chapter": 86,
            "verse": 2,
            "text": "86|2|ratriyil varunnat ennal entanenn ninakkariyumea"
        },
        {
            "chapter": 86,
            "verse": 3,
            "text": "86|3|tulacc kayarunna naksatramatre at‌"
        },
        {
            "chapter": 86,
            "verse": 4,
            "text": "86|4|tanre karyattil oru melneattakkaranuntayikkeantallate yatearalumilla"
        },
        {
            "chapter": 86,
            "verse": 5,
            "text": "86|5|ennal manusyan cinticcu neakkatte tan entil ninnan srstikkappettirikkunnat enn‌"
        },
        {
            "chapter": 86,
            "verse": 6,
            "text": "86|6|tericcu vilunna oru dravakattil ninnatre avan srstikkappettirikkunnat‌"
        },
        {
            "chapter": 86,
            "verse": 7,
            "text": "86|7|mutukellinum, variyellukalkkumitayil ninn at purattu varunnu"
        },
        {
            "chapter": 86,
            "verse": 8,
            "text": "86|8|avane (manusyane) tiriccukeantu varan tirccayayum avan (allahu) kalivullavanakunnu"
        },
        {
            "chapter": 86,
            "verse": 9,
            "text": "86|9|rahasyannal pariseadhikkappetunna divasam"
        },
        {
            "chapter": 86,
            "verse": 10,
            "text": "86|10|appeal avan (manusyan‌) yatearu saktiyea, sahayiyea untayirikkukayilla"
        },
        {
            "chapter": 86,
            "verse": 11,
            "text": "86|11|avartticc malapeyyikkunna akasattekkeantum"
        },
        {
            "chapter": 86,
            "verse": 12,
            "text": "86|12|sasyalatadikal mulappikkunna bhumiyekkeantum satyam"
        },
        {
            "chapter": 86,
            "verse": 13,
            "text": "86|13|tirccayayum itu nirnayakamaya oru vakkakunnu"
        },
        {
            "chapter": 86,
            "verse": 14,
            "text": "86|14|itu tamasayalla"
        },
        {
            "chapter": 86,
            "verse": 15,
            "text": "86|15|tirccayayum avar (valiya) tantram prayeagiccu keantirikkum"
        },
        {
            "chapter": 86,
            "verse": 16,
            "text": "86|16|nanum (valiya) tantram prayeagiccu keantirikkum"
        },
        {
            "chapter": 86,
            "verse": 17,
            "text": "86|17|akayal (nabiye,) ni satyanisedhikalkk kalatamasam nalkuka. alpasamayattekk avarkk tamasam nalkiyekkuka"
        }
    ]
}