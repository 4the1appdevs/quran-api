{
    "chapter": [
        {
            "chapter": 99,
            "verse": 1,
            "text": "99|1|bhumi prakampanam keallikkappettal - atinre bhayankaramaya a prakampanam"
        },
        {
            "chapter": 99,
            "verse": 2,
            "text": "99|2|bhumi atinre bharannal puram tallukayum"
        },
        {
            "chapter": 99,
            "verse": 3,
            "text": "99|3|atin entuparri enn manusyan parayukayum ceytal"
        },
        {
            "chapter": 99,
            "verse": 4,
            "text": "99|4|anne divasam at (bhumi) atinre varttamanannal parannariyikkunnatan‌"
        },
        {
            "chapter": 99,
            "verse": 5,
            "text": "99|5|ninre raksitav atin beadhanam nalkiyat nimittam"
        },
        {
            "chapter": 99,
            "verse": 6,
            "text": "99|6|anne divasam manusyar pala sanghannalayi purappetunnatan‌. avarkk avarute karm'mannal kanikkappetentatinayitt‌"
        },
        {
            "chapter": 99,
            "verse": 7,
            "text": "99|7|appeal ar oru anuvinre tukkam nanmaceytirunnuvea avanat kanum"
        },
        {
            "chapter": 99,
            "verse": 8,
            "text": "99|8|ar oru anuvinre tukkam tinma ceytirunnuvea avan atum kanum"
        }
    ]
}