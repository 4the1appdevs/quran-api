{
    "chapter": [
        {
            "chapter": 91,
            "verse": 1,
            "text": "91|1|suryanum atinre prabhayum tanneyana satyam"
        },
        {
            "chapter": 91,
            "verse": 2,
            "text": "91|2|candran tanneyana satyam; at atine tutarnn varumpeal"
        },
        {
            "chapter": 91,
            "verse": 3,
            "text": "91|3|pakaline tanneyana satyam; at atine (suryane) pratyaksappetuttumpel"
        },
        {
            "chapter": 91,
            "verse": 4,
            "text": "91|4|ratriye tanneyana satyam; at atine mutumpeal"
        },
        {
            "chapter": 91,
            "verse": 5,
            "text": "91|5|akasatteyum, atine sthapicca ritiyeyum tanneyana satyam"
        },
        {
            "chapter": 91,
            "verse": 6,
            "text": "91|6|bhumiyeyum, atine vistrtamakkiya ritiyeyum tanneyana satyam"
        },
        {
            "chapter": 91,
            "verse": 7,
            "text": "91|7|manusyastitvatteyum atine sanvidhanicca ritiyeyum tanneyana satyam"
        },
        {
            "chapter": 91,
            "verse": 8,
            "text": "91|8|ennitt atinn atinre dustatayum atinre suksmatayum sambandhicc avan beadham nalkukayum ceytirikkunnu"
        },
        {
            "chapter": 91,
            "verse": 9,
            "text": "91|9|tirccayayum atine (astitvatte) parisud'dhamakkiyavan vijayam kaivariccu"
        },
        {
            "chapter": 91,
            "verse": 10,
            "text": "91|10|atine kalankappetuttiyavan tirccayayum nirbhagyamatayukayum ceytu"
        },
        {
            "chapter": 91,
            "verse": 11,
            "text": "91|11|thamud geatram atinre dhikkaram mulam (satyatte) nisedhiccu tallukayuntayi"
        },
        {
            "chapter": 91,
            "verse": 12,
            "text": "91|12|avarute kuttattile erravum dustatayullavan orunni purappetta sandarbham"
        },
        {
            "chapter": 91,
            "verse": 13,
            "text": "91|13|appeal allahuvinre dutan avareat parannu. allahuvinre ottakatteyum atinre vellam kutiyum ninnal suksikkuka"
        },
        {
            "chapter": 91,
            "verse": 14,
            "text": "91|14|appeal avar addehatte nisedhiccu tallukayum atine (ottakatte) arukeala natattukayum ceytu. appeal avarute papam nimittam avarute raksitav avarkk samula nasam varuttukayum (avarkkellam) at samamakkukayum ceytu"
        },
        {
            "chapter": 91,
            "verse": 15,
            "text": "91|15|atinre anantaraphalam avan bhayappettirunnumilla"
        }
    ]
}