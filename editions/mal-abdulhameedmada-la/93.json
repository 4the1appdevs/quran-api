{
    "chapter": [
        {
            "chapter": 93,
            "verse": 1,
            "text": "93|1|purvvahnam tanneyana satyam"
        },
        {
            "chapter": 93,
            "verse": 2,
            "text": "93|2|ratri tanneyana satyam; at santamavumpeal"
        },
        {
            "chapter": 93,
            "verse": 3,
            "text": "93|3|(nabiye,) ninre raksitav ninne kaivittittilla. veruttittumilla"
        },
        {
            "chapter": 93,
            "verse": 4,
            "text": "93|4|tirccayayum paraleakaman ninakk ihaleakattekkal uttamamayittullat‌"
        },
        {
            "chapter": 93,
            "verse": 5,
            "text": "93|5|valiye ninakk ninre raksitav (anugrahannal) nalkunnatum appeal ni trptippetunnatumana.‌"
        },
        {
            "chapter": 93,
            "verse": 6,
            "text": "93|6|ninne avan oru anathayayi kanetattukayum, ennitt (ninakk‌) asrayam nalkukayum ceytille"
        },
        {
            "chapter": 93,
            "verse": 7,
            "text": "93|7|ninne avan vali ariyattavanayi kanetattukayum ennitt (ninakk‌) margadarsanam nalkukayum ceytirikkunnu"
        },
        {
            "chapter": 93,
            "verse": 8,
            "text": "93|8|ninne avan daridranayi kanetattukayum ennitt avan aisvaryam nalkukayum ceytirikkunnu"
        },
        {
            "chapter": 93,
            "verse": 9,
            "text": "93|9|ennirikke anathaye ni aticcamarttarut‌"
        },
        {
            "chapter": 93,
            "verse": 10,
            "text": "93|10|ceadicc varunnavane ni viratti vitukayum ceyyarut‌"
        },
        {
            "chapter": 93,
            "verse": 11,
            "text": "93|11|ninre raksitavinre anugrahatte sambandhicc ni sansarikkuka"
        }
    ]
}